{
    "base": {
        "type": "Base",
        "delegate_arguments": {
            "name": "renderer_material_index"
        },
        "subcomponent": "material_index"
    },
    "material_index": {
        "type": "Keymap",
        "keys": {
            "materials": {"type": "list", "subcomponent": "materials"}
        }
    },
    "materials": {
        "type": "Set",
        "delegate_arguments": {
            "field": "material",
            "measure_length": true
        },
        "types": ["dict"],
        "subcomponent": "material"
    },
    "material": {
        "type": "Keymap",
        "sort": "by_component_order",
        "keys": {
            "name": {"type": "str"},
            "path": {"type": "str"}
        }
    }
}