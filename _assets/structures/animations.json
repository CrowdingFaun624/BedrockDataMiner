{
    "base": {
        "type": "Base",
        "name": "animations",
        "subcomponent": "files"
    },
    "decimal": {
        "type": "TypeAlias",
        "types": ["float", "int"]
    },
    "files": {
        "type": "Dict",
        "field": "file",
        "types": ["dict"],
        "sort": "by_key",
        "measure_length": true,
        "detect_key_moves": true,
        "key_weight": 0.5,
        "subcomponent": "resource_packs"
    },
    "resource_packs": {
        "type": "Dict",
        "field": "resource pack",
        "types": ["dict"],
        "measure_length": true,
        "detect_key_moves": true,
        "sort": "by_key",
        "normalizer": {
            "function_name": "collapse_resource_packs_dict_with_defined_in"
        },
        "subcomponent": "animations_file"
    },
    "animations_file": {
        "type": "Keymap",
        "normalizer": {
            "function_name": "animations_fix_old"
        },
        "sort": "by_component_order",
        "keys": {
            "defined_in": {"type": "list", "subcomponent": "defined_in"},
            "format_version": {"type": "str"},
            "animations": {"type": "dict", "subcomponent": "animations", "weight": 20}
        }
    },
    "defined_in": {
        "type": "Set",
        "field": "resource pack",
        "types": ["str"],
        "measure_length": true,
        "subcomponent": null
    },
    "animations": {
        "type": "Dict",
        "field": "animation",
        "types": ["dict"],
        "sort": "by_key",
        "measure_length": true,
        "min_value_similarity_threshold": 0.75,
        "subcomponent": "animation"
    },
    "timestamps_and_vectors_or_vector_or_lerp": {
        "type": "Group",
        "subcomponents": {
            "dict": "timestamps_and_vectors_or_lerp",
            "list": "vector_special"
        }
    },
    "timestamps_and_vectors_or_lerp": {
        "type": "Dict",
        "field": "timestamp",
        "types": ["dict", "list"],
        "detect_key_moves": false,
        "measure_length": true,
        "subcomponent": "vector_or_lerp"
    },
    "vector_or_lerp": {
        "type": "Group",
        "subcomponents": {
            "dict": "lerp",
            "list": "vector_special"
        }
    },
    "lerp": {
        "type": "Keymap",
        "sort": "by_component_order",
        "keys": {
            "lerp_mode": {"type": "str"},
            "pre": {"type": "list", "subcomponent": "vector_special"},
            "post": {"type": "list", "subcomponent": "vector_special"}
        }
    },
    "vector_special": {
        "type": "List",
        "field": "vector item",
        "types": ["decimal", "dict", "str"],
        "print_all": true,
        "print_flat": true,
        "measure_length": false,
        "subcomponent": {
            "type": "Group",
            "subcomponents": {
                "decimal": null,
                "dict": {
                    "type": "Keymap",
                    "sort": "by_component_order",
                    "keys": {
                        "x": {"type": "str"},
                        "y": {"type": "str"},
                        "z": {"type": "str"}
                    }
                },
                "str": {
                    "type": "String"
                }
            }
        }
    },
    "animation": {
        "type": "Keymap",
        "sort": "by_component_order",
        "keys": {
            "anim_time_update": {"type": "str"},
            "animation_length": {"type": "decimal"},
            "loop": {"type": ["bool", "str"]},
            "override_previous_animation": {"type": "bool"},
            "start_delay": {"type": "str"},
            "bones": {"type": "dict", "subcomponent": "bones"},
            "particle_effects": {"type": "dict", "subcomponent": "particle_effects"},
            "sound_effects": {"type": "dict", "subcomponent": "sound_effects"}
        }
    },
    "bones": {
        "type": "Dict",
        "field": "bone",
        "types": ["dict"],
        "measure_length": true,
        "sort": "by_key",
        "min_value_similarity_threshold": 0.3,
        "min_key_similarity_threshold": 0.5,
        "key_weight": 0.3,
        "value_weight": 0.5,
        "key_component": {
            "type": "String"
        },
        "subcomponent": "bone"
    },
    "bone": {
        "type": "Keymap",
        "sort": "by_key",
        "keys": {
            "relative_to": {"type": "dict", "subcomponent": "bone.relative_to"},
            "rotation": {"type": ["list", "dict"], "subcomponent": "timestamps_and_vectors_or_vector_or_lerp"},
            "position": {"type": ["list", "dict"], "subcomponent": "timestamps_and_vectors_or_vector_or_lerp"},
            "scale": {"type": ["decimal", "dict", "list", "str"], "subcomponent": "bone.scale"}
        }
    },
    "bone.relative_to": {
        "type": "Keymap",
        "keys": {
            "rotation": {"type": "str"}
        }
    },
    "bone.scale": {
        "type": "Group",
        "subcomponents": {
            "decimal": null,
            "dict": "timestamps_and_vectors_or_lerp",
            "list": "vector_special",
            "str": null
        }
    },
    "particle_effects": {
        "type": "Dict",
        "field": "timestamp",
        "types": ["dict"],
        "detect_key_moves": false,
        "measure_length": true,
        "sort": "by_key",
        "subcomponent": {
            "type": "Keymap",
            "keys": {
                "effect": {"type": "str"},
                "pre_effect_script": {"type": "str"}
            }
        }
    },
    "sound_effects": {
        "type": "Dict",
        "field": "timestamp",
        "types": ["dict"],
        "detect_key_moves": false,
        "measure_length": true,
        "sort": "by_key",
        "subcomponent": {
            "type": "Keymap",
            "keys": {
                "effect": {"type": "str"}
            }
        }
    }
}