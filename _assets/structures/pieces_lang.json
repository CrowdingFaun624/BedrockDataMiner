{
    "base": {
        "type": "Base",
        "delegate_arguments": {
            "name": "pieces_lang"
        },
        "subcomponent": "pieces"
    },
    "pieces": {
        "type": "Dict",
        "types": "dict",
        "subcomponent": "lang_files",
        "sort": "by_key",
        "delegate_arguments": {
            "field": "piece"
        }
    },
    "lang_files": {
        "type": "Dict",
        "types": "file",
        "subcomponent": "cache",
        "sort": "by_key",
        "delegate_arguments": {
            "field": "lang file"
        }
    },
    "cache": {
        "type": "Cache",
        "types": "file",
        "subcomponent": {
            "type": "File",
            "file_types": "file",
            "content_types": "dict",
            "subcomponent": "lang"
        }
    },
    "lang": {
        "type": "Dict",
        "delegate_arguments": {
            "measure_length": true,
            "field": "key"
        },
        "types": "dict",
        "sort": "by_key",
        "subcomponent": "key"
    },
    "key": {
        "type": "Keymap",
        "delegate_arguments": {
            "field": "language"
        },
        "sort": "by_component_order",
        "keys": {
            "en_US": {"type": "str", "subcomponent": {"type": "String"}},
            "neutral": {"type": "str", "subcomponent": {"type": "String"}}
        }
    }
}