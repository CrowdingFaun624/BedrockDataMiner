{
    "base": {
        "type": "Base",
        "name": "languages",
        "normalizer": {
            "function_name": "normalizers/languages_normalize.lua"
        },
        "subcomponent": "language"
    },
    "language": {
        "type": "Dict",
        "field": "language",
        "types": ["dict"],
        "detect_key_moves": true,
        "measure_length": true,
        "sort": "by_key",
        "subcomponent": "resource_packs"
    },
    "resource_packs": {
        "type": "Dict",
        "field": "resource pack",
        "types": ["dict"],
        "normalizer": {
            "function_name": "collapse_resource_packs_dict"
        },
        "sort": "by_key",
        "subcomponent": "language_property"
    },
    "language_property": {
        "type": "Keymap",
        "field": "property",
        "sort": "by_component_order",
        "keys": {
            "name": {"type": "str"},
            "defined_in": {"type": "list", "subcomponent": "defined_in"}
        }
    },
    "defined_in": {
        "type": "Set",
        "field": "resource pack",
        "types": ["str"],
        "print_flat": true,
        "sort": true,
        "subcomponent": null
    }
}