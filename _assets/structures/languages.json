{
    "base": {
        "type": "Base",
        "delegate_arguments": {
            "name": "languages"
        },
        "normalizer": {
            "function_name": "normalizers/languages_normalize"
        },
        "subcomponent": "language"
    },
    "language": {
        "type": "Dict",
        "delegate_arguments": {
            "field": "language",
            "measure_length": true
        },
        "types": ["dict"],
        "detect_key_moves": true,
        "sort": "by_key",
        "subcomponent": "resource_packs"
    },
    "resource_packs": {
        "type": "Dict",
        "delegate_arguments": {
            "field": "resource pack"
        },
        "types": ["dict"],
        "normalizer": {
            "function_name": "normalizers/collapse_resource_packs/dict"
        },
        "sort": "by_key",
        "subcomponent": "language_property"
    },
    "language_property": {
        "type": "Keymap",
        "delegate_arguments": {
            "field": "property"
        },
        "sort": "by_component_order",
        "keys": {
            "name": {"type": "str", "delegate_arguments": {"always_print": true}},
            "defined_in": {"type": "list", "subcomponent": "defined_in"}
        }
    },
    "defined_in": {
        "type": "Set",
        "delegate_arguments": {
            "field": "resource pack",
            "print_flat": true
        },
        "types": ["str"],
        "sort": true,
        "subcomponent": null
    }
}