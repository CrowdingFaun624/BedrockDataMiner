{
    "base": {
        "type": "Base",
        "delegate_arguments": {
            "name": "sounds_json"
        },
        "subcomponent": "categories"
    },
    "normalizers/collapse_resource_packs/flat": {
        "type": "Normalizer",
        "function_name": "normalizers/collapse_resource_packs/flat"
    },
    "categories": {
        "type": "Keymap",
        "delegate_arguments": {
            "field": "category"
        },
        "sort": "by_component_order",
        "keys": {
            "individual_event_sounds": {"type": "dict", "subcomponent": "flat_sound_collection"},
            "block_sounds": {"type": "dict", "subcomponent": "block_sound_collections"},
            "interactive_block_sounds": {"type": "dict", "subcomponent": "interactive_block_sound_collections"},
            "entity_sounds": {"type": "dict", "subcomponent": "entity_sound_collections"},
            "interactive_entity_sounds": {"type": "dict", "subcomponent": "interactive_entity_sound_collections"}
        }
    },
    "block_sound_collections": {
        "type": "Dict",
        "delegate_arguments": {
            "field": "block",
            "measure_length": true
        },
        "types": ["dict"],
        "sort": "by_key",
        "min_value_similarity_threshold": 0.75,
        "min_key_similarity_threshold": 0.5,
        "key_component": {
            "type": "String"
        },
        "subcomponent": "sound_collection"
    },
    "interactive_block_sound_collections": {
        "type": "Dict",
        "delegate_arguments": {
            "field": "interactive block",
            "measure_length": true
        },
        "types": ["dict"],
        "sort": "by_key",
        "min_value_similarity_threshold": 0.75,
        "min_key_similarity_threshold": 0.5,
        "key_component": {
            "type": "String"
        },
        "subcomponent": "sound_collection"
    },
    "entity_sound_collections": {
        "type": "Dict",
        "delegate_arguments": {
            "field": "entity",
            "measure_length": true
        },
        "types": ["dict"],
        "sort": "by_key",
        "min_value_similarity_threshold": 0.75,
        "min_key_similarity_threshold": 0.5,
        "key_component": {
            "type": "String"
        },
        "subcomponent": "sound_collection"
    },
    "interactive_entity_sound_collections": {
        "type": "Dict",
        "delegate_arguments": {
            "field": "interactive entity",
            "measure_length": true
        },
        "types": ["dict"],
        "sort": "by_key",
        "min_value_similarity_threshold": 0.75,
        "min_key_similarity_threshold": 0.5,
        "key_component": {
            "type": "String"
        },
        "subcomponent": "interactive_entity_sound_collection"
    },
    "flat_sound_collection": {
        "type": "Keymap",
        "delegate_arguments": {
            "field": "property"
        },
        "keys": {
            "events": {"type": "dict", "subcomponent": "events"}
        }
    },
    "sound_collection": {
        "type": "Keymap",
        "delegate_arguments": {
            "field": "property"
        },
        "sort": "by_component_order",
        "keys": {
            "volume": {"type": "dict", "subcomponent": "volume_resource_packs"},
            "pitch": {"type": "dict", "subcomponent": "pitch_resource_packs"},
            "events": {"type": "dict", "subcomponent": "events"}
        }
    },
    "interactive_entity_sound_collection": {
        "type": "Keymap",
        "delegate_arguments": {
            "field": "property"
        },
        "sort": "by_component_order",
        "keys": {
            "volume": {"type": "dict", "subcomponent": "volume_resource_packs"},
            "pitch": {"type": "dict", "subcomponent": "pitch_resource_packs"},
            "events": {"type": "dict", "subcomponent": "interactive_entity_events"}
        }
    },
    "pitch_resource_packs": {
        "type": "Dict",
        "delegate_arguments": {
            "field": "resource pack"
        },
        "types": ["float", "int", "list"],
        "subcomponent": "pitch_group",
        "sort": "by_key",
        "normalizer": "normalizers/collapse_resource_packs/flat"
    },
    "volume_resource_packs": {
        "type": "Dict",
        "delegate_arguments": {
            "field": "resource pack"
        },
        "types": ["float", "int", "list"],
        "subcomponent": "volume_group",
        "sort": "by_key",
        "normalizer": "normalizers/collapse_resource_packs/flat"
    },
    "pitch_group": {
        "type": "Group",
        "subcomponents": {
            "float": null,
            "int": null,
            "list": "pitch_list"
        }
    },
    "volume_group": {
        "type": "Group",
        "subcomponents": {
            "float": null,
            "int": null,
            "list": "volume_list"
        }
    },
    "pitch_list": {
        "type": "List",
        "delegate_arguments": {
            "field": "pitch",
            "print_flat": true,
            "print_all": true
        },
        "types": ["float", "int"],
        "subcomponent": null
    },
    "volume_list": {
        "type": "List",
        "delegate_arguments": {
            "field": "volume",
            "print_flat": true,
            "print_all": true
        },
        "types": ["float", "int"],
        "subcomponent": null
    },
    "events": {
        "type": "Dict",
        "delegate_arguments": {
            "field": "event",
            "measure_length": true
        },
        "types": ["dict"],
        "sort": "by_key",
        "subcomponent": "events_resource_packs"
    },
    "interactive_entity_events": {
        "type": "Dict",
        "delegate_arguments": {
            "field": "event",
            "measure_length": true
        },
        "types": ["dict"],
        "sort": "by_key",
        "subcomponent": "interactive_entity_events_resource_packs"
    },
    "events_resource_packs": {
        "type": "Dict",
        "delegate_arguments": {
            "field": "resource pack",
            "measure_length": true
        },
        "types": ["str", "dict"],
        "subcomponent": "sound_group",
        "sort": "by_key",
        "normalizer": [
            "normalizers/collapse_resource_packs/flat",
            {
                "function_name": "normalizers/sounds_json_remove_bad_events",
                "arguments": {
                    "is_interactive_entity": false
                }
            }
        ]
    },
    "interactive_entity_events_resource_packs": {
        "type": "Dict",
        "delegate_arguments": {
            "field": "resource pack",
            "measure_length": true
        },
        "types": ["dict"],
        "subcomponent": "sound_case",
        "sort": "by_key",
        "normalizer": [
            "normalizers/collapse_resource_packs/flat",
            {
                "function_name": "normalizers/sounds_json_remove_bad_events",
                "arguments": {
                    "is_interactive_entity": true
                }
            }
        ]
    },
    "sound_case": {
        "type": "Dict",
        "delegate_arguments": {
            "field": "case",
            "measure_length": true
        },
        "types": ["str", "dict"],
        "subcomponent": "sound_group",
        "sort": "by_key"
    },
    "sound_group": {
        "type": "Group",
        "subcomponents": {
            "str": null,
            "dict": "sound"
        }
    },
    "sound": {
        "type": "Keymap",
        "delegate_arguments": {
            "field": "sound"
        },
        "normalizer": [
            {
                "function_name": "delete_optional_keys",
                "arguments": {
                    "keys": ["ambient", "death", "hurt", "attenuation_distance"]
                }
            },
            {
                "function_name": "move_key",
                "arguments": {
                    "from_key": "sounds",
                    "to_key": "sound"
                }
            }
        ],
        "sort": "by_component_order",
        "keys": {
            "volume": {"type": ["float", "int", "list"], "subcomponent": "volume_group"},
            "pitch": {"type": ["float", "int", "list"], "subcomponent": "pitch_group"},
            "sound": {"type": "str", "subcomponent": {"type": "String"}, "delegate_arguments": {"always_print": true}}
        }
    }
}