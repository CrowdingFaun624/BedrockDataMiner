{
    "base": {
        "type": "Base",
        "delegate_arguments": {
            "name": "personas"
        },
        "subcomponent": "personas"
    },
    "color_list": {
        "type": "List",
        "delegate_arguments": {
            "field": "color",
            "print_all": true,
            "print_flat": true,
            "measure_length": false
        },
        "types": ["str"],
        "subcomponent": null
    },
    "personas": {
        "type": "Dict",
        "delegate_arguments": {
            "field": "persona",
            "measure_length": true
        },
        "types": "file",
        "sort": "by_key",
        "subcomponent": "cache"
    },
    "cache": {
        "type": "Cache",
        "types": "file",
        "subcomponent": {
            "type": "File",
            "file_types": "file",
            "content_types": ["dict", "list"],
            "subcomponent": "persona"
        }
    },
    "persona": {
        "type": "Group",
        "subcomponents": {
            "dict": {
                "type": "Keymap",
                "keys": {
                    "appearance": {"type": "list", "subcomponent": "appearance"},
                    "name": {"type": "str", "subcomponent": {"type": "String"}, "delegate_arguments": {"always_print": true}},
                    "thumbnailPath": {"type": "str"},
                    "uuid": {"type": "str"}
                }
            },
            "list": "appearance"
        }
    },
    "appearance": {
        "type": "Set",
        "delegate_arguments": {
            "field": "appearance",
            "measure_length": true
        },
        "types": ["dict"],
        "subcomponent": {
            "type": "Keymap",
            "keys": {
                "arm": {"type": "str"},
                "col": {"type": "list", "subcomponent": "color_list"},
                "id": {"type": "str"},
                "skcol": {"type": "str"},
                "skin": {"type": "bool"}
            }
        }
    }
}