{
    "base": {
        "type": "Base",
        "name": "terrain_textures",
        "normalizer": {
            "function_name": "normalizers/terrain_textures_normalize.py"
        },
        "subcomponent": "terrain_textures"
    },
    "outdated": {
        "type": "Tag"
    },
    "collapse_resource_packs_flat": {
        "type": "Normalizer",
        "function_name": "collapse_resource_packs_flat"
    },
    "terrain_textures": {
        "type": "Keymap",
        "sort": "by_component_order",
        "keys": {
            "num_mip_levels": {"type": "dict", "subcomponent": "simple_int_resource_pack_dict"},
            "padding": {"type": "dict", "subcomponent": "simple_int_resource_pack_dict"},
            "texture_name": {"type": "dict", "subcomponent": "simple_str_resource_pack_dict"},
            "texture_data": {"type": "dict", "subcomponent": "texture_data"}
        }
    },
    "simple_int_resource_pack_dict": {
        "type": "Dict",
        "field": "resource pack",
        "types": ["int"],
        "measure_length": true,
        "normalizer": "collapse_resource_packs_flat",
        "sort": "by_key",
        "subcomponent": null
    },
    "simple_str_resource_pack_dict": {
        "type": "Dict",
        "field": "resource pack",
        "types": ["str"],
        "measure_length": true,
        "normalizer": "collapse_resource_packs_flat",
        "sort": "by_key",
        "subcomponent": null
    },
    "texture_data": {
        "type": "Dict",
        "field": "terrain",
        "types": ["dict"],
        "measure_length": true,
        "sort": "by_key",
        "key_component": {
            "type": "String"
        },
        "min_key_similarity_threshold": 0.5,
        "subcomponent": "resource_packs"
    },
    "resource_packs": {
        "type": "Dict",
        "field": "resource pack",
        "types": ["dict"],
        "normalizer": {
            "function_name": "collapse_resource_packs_dict"
        },
        "min_value_similarity_threshold": 0.4,
        "measure_length": true,
        "sort": "by_key",
        "subcomponent": "terrain"
    },
    "terrain": {
        "type": "Keymap",
        "keys": {
            "blockshape": {"type": "str", "tags": ["outdated"]},
            "defined_in": {"type": "list", "subcomponent": "defined_in", "weight": 0},
            "mip_fade_amount": {"type": "float", "tags": ["outdated"]},
            "mip_fade_color": {"type": "list", "subcomponent": "color", "tags": ["outdated"]},
            "quad": {"type": "int"},
            "textures": {"type": ["dict", "list", "str"], "subcomponent": "textures_group", "weight": 2}
        }
    },
    "defined_in": {
        "type": "Set",
        "field": "resource pack",
        "types": ["str"],
        "measure_length": true,
        "sort": true,
        "subcomponent": null
    },
    "color": {
        "type": "List",
        "field": "channel",
        "types": ["int"],
        "measure_length": false,
        "print_all": true,
        "print_flat": true,
        "subcomponent": null
    },
    "textures_group": {
        "type": "Group",
        "subcomponents": {
            "dict": "texture_dict",
            "list": "texture_list",
            "str": {
                "type": "String"
            }
        }
    },
    "texture_list": {
        "type": "Set",
        "field": "texture",
        "types": ["dict", "str"],
        "measure_length": true,
        "subcomponent": "texture_group"
    },
    "texture_group": {
        "type": "Group",
        "subcomponents": {
            "dict": "texture_dict",
            "str": {
                "type": "String"
            }
        }
    },
    "texture_dict": {
        "type": "Keymap",
        "keys": {
            "mip_fade_amount": {"type": "float", "tags": ["outdated"]},
            "mip_fade_color": {"type": "list", "subcomponent": "color", "tags": ["outdated"]},
            "overlay_color": {"type": "str"},
            "path": {"type": "str", "subcomponent": {
                "type": "String"
            }},
            "tint_color": {"type": "str"}
        }
    }
}