{
    "base": {
        "type": "Base",
        "delegate_arguments": {
            "name": "terrain_meta"
        },
        "normalizer": {
            "function_name": "wrap_in_dict",
            "arguments": {
                "key": "name",
                "delete": true
            }
        },
        "subcomponent": "images"
    },
    "images": {
        "type": "Dict",
        "delegate_arguments": {
            "field": "image",
            "measure_length": true
        },
        "types": ["dict"],
        "sort": "by_key",
        "subcomponent": "image"
    },
    "image": {
        "type": "Keymap",
        "keys": {
            "additonal_textures": {"type": "list", "subcomponent": "uvs"},
            "uv": {"type": "dict", "subcomponent": "uv"},
            "uvs": {"type": "list", "subcomponent": "uvs"}
        }
    },
    "uvs": {
        "type": "List",
        "delegate_arguments": {
            "field": "uv",
            "measure_length": true
        },
        "types": ["dict"],
        "subcomponent": "uv"
    },
    "uv": {
        "type": "Keymap",
        "sort": "by_component_order",
        "pre_normalized_types": "list",
        "normalizer": {
            "function_name": "wrap_tuple",
            "arguments": {
                "keys": ["x1", "y1", "x2", "y2", "1", "2"]
            }
        },
        "keys": {
            "x1": {"type": ["float", "int"]},
            "y1": {"type": ["float", "int"]},
            "x2": {"type": ["float", "int"]},
            "y2": {"type": ["float", "int"]},
            "1":  {"type": ["float", "int"]},
            "2":  {"type": ["float", "int"]}
        }
    }
}