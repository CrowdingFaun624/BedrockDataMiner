            "#legendary": {"type": "list", "subcomponent": "vector_float"},
            "#look_at_cursor": {"type": "bool"},
            "#modal_label_text": {"type": "str"},
            "#modal_title_text": {"type": "str"},
            "#no_buttons_visible": {"type": "bool"},
            "#open": {"type": "bool"},
            "#owned_incompatible_prompt_color": {"type": "bool"},
            "#pack_id": {"type": "int"},
            "#panel_title": {"type": "str"},
