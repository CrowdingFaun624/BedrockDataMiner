{
    "base": {
        "type": "Base",
        "delegate_arguments": {
            "name": "ui"
        },
        "subcomponent": "namespaces"
    },
    "decimal": {
        "type": "TypeAlias",
        "types": ["int", "float"]
    },
    "outdated": {
        "type": "Tag"
    },
    "any": {
        "type": "Group",
        "subcomponents": {
            "dict": "any.dict",
            "list": "any.list",
            "bool": null,
            "float": null,
            "int": null,
            "null": null,
            "str": null
        }
    },
    "any.dict": {
        "type": "Dict",
        "types": ["dict", "list", "bool", "float", "int", "null", "str"],
        "detect_key_moves": false,
        "subcomponent": "any"
    },
    "any.list": {
        "type": "List",
        "types": ["dict", "list", "bool", "float", "int", "null", "str"],
        "subcomponent": "any"
    },
    "vector_float": {
        "type": "List",
        "types": "float",
        "delegate_arguments": {
            "print_flat": true,
            "print_all": true
        },
        "subcomponent": null
    },
    "str_or_decimal_or_vector_decimal_or_str": {
        "type": "Group",
        "subcomponents": {
            "decimal": null,
            "list": "vector_decimal_or_str",
            "str": null
        }
    },
    "vector_decimal_or_str": {
        "type": "List",
        "types": ["decimal", "str"],
        "delegate_arguments": {
            "print_flat": true,
            "print_all": true
        },
        "subcomponent": null
    },
    "str_or_int_or_vector_int": {
        "type": "Group",
        "subcomponents": {
            "int": null,
            "list": "vector_int",
            "str": null
        }
    },
    "vector_int": {
        "type": "List",
        "types": "int",
        "delegate_arguments": {
            "print_flat": true,
            "print_all": true
        },
        "subcomponent": null
    },
    "str_or_vector_int": {
        "type": "Group",
        "subcomponents": {
            "str": null,
            "list": "vector_int"
        }
    },
    "str_or_vector_int_or_str": {
        "type": "Group",
        "subcomponents": {
            "str": null,
            "list": "vector_int_or_str"
        }
    },
    "str_or_vector_float": {
        "type": "Group",
        "subcomponents": {
            "str": null,
            "list": "vector_float"
        }
    },
    "vector_int_or_str": {
        "type": "List",
        "types": ["int", "str"],
        "delegate_arguments": {
            "print_flat": true,
            "print_all": true
        },
        "subcomponent": null
    },
    "namespaces": {
        "type": "Dict",
        "types": "dict",
        "delegate_arguments": {
            "field": "file",
            "measure_length": true
        },
        "normalizer": {
            "function_name": "normalizers/ui_normalize"
        },
        "subcomponent": "cache"
    },
    "cache": {
        "type": "Cache",
        "types": "dict",
        "subcomponent": "elements"
    },
    "elements": {
        "type": "Dict",
        "types": "dict",
        "delegate_arguments": {
            "field": "file",
            "measure_length": true
        },
        "subcomponent": "element"
    },
    "element": {
        "type": "Keymap",
        "delegate": null, // ui_normalize wraps each element with a dict of the element's type.
        // the type is only needed to choose which branch, so it is ignored.
        "keys": {
            "alpha": {"type": "dict", "subcomponent": "animation"},
            "aseprite_flip_book": {"type": "dict", "subcomponent": "animation"},
            "button": {"type": "dict", "subcomponent": "element.button"},
            "carousel_label": {"type": "dict", "subcomponent": "element.carousel_label", "tags": ["outdated"]},
            "clip": {"type": "dict", "subcomponent": "animation"},
            "collection_panel": {"type": "dict", "subcomponent": "element.collection_panel"},
            "color": {"type": "dict", "subcomponent": "animation"},
            "custom": {"type": "dict", "subcomponent": "element.custom"},
            "dropdown": {"type": "dict", "subcomponent": "element.dropdown"},
            "edit_box": {"type": "dict", "subcomponent": "element.edit_box"},
            "factory": {"type": "dict", "subcomponent": "element.factory"},
            "flip_book": {"type": "dict", "subcomponent": "animation"},
            "grid": {"type": "dict", "subcomponent": "element.grid"},
            "grid_item": {"type": "dict", "subcomponent": "element.grid_item", "tags": ["outdated"]},
            "image": {"type": "dict", "subcomponent": "element.image"},
            "image_cycler": {"type": "dict", "subcomponent": "element.image_cycler"},
            "input_panel": {"type": "dict", "subcomponent": "element.input_panel"},
            "label": {"type": "dict", "subcomponent": "element.label"},
            "label_cycler": {"type": "dict", "subcomponent": "element.label_cycler"}, // Note: not supported by wiki
            "offset": {"type": "dict", "subcomponent": "animation"},
            "panel": {"type": "dict", "subcomponent": "element.panel"},
            "screen": {"type": "dict", "subcomponent": "element.screen"},
            "scroll_track": {"type": "dict", "subcomponent": "element.scroll_track"},
            "scroll_view": {"type": "dict", "subcomponent": "element.scroll_view"},
            "scrollbar": {"type": "dict", "subcomponent": "element.scrollbar"},
            "scrollbar_box": {"type": "dict", "subcomponent": "element.scrollbar_box"},
            "scrollbar_track": {"type": "dict", "subcomponent": "element.scrollbar_track"},
            "selection_wheel": {"type": "dict", "subcomponent": "element.selection_wheel"},
            "size": {"type": "dict", "subcomponent": "animation"},
            "slider": {"type": "dict", "subcomponent": "element.slider"},
            "slider_box": {"type": "dict", "subcomponent": "element.slider_box"},
            "stack_panel": {"type": "dict", "subcomponent": "element.stack_panel"},
            "tab": {"type": "dict", "subcomponent": "element.tab", "tags": ["outdated"]},
            "toggle": {"type": "dict", "subcomponent": "element.toggle"},
            "unknown": {"type": "dict", "subcomponent": "element.unknown"},
            "uv": {"type": "dict", "subcomponent": "animation"},
            "wait": {"type": "dict", "subcomponent": "animation"}
        }
    },
    "animation": {
        "type": "Keymap",
        "normalizer": "separate_variables",
        "keys": {
            "$variables": {"type": "dict", "subcomponent": "my_variables"},
            "anim_type": {"type": "str", "delegate_arguments": {"always_print": true}},
            "activated": {"type": ["bool", "str"]},
            "destroy_at_end": {"type": "str"},
            "duration": {"type": ["decimal", "str"]},
            "easing": {"type": "str"},
            "end_event": {"type": "str"},
            "fps": {"type": ["decimal", "str"]},
            "frame_count": {"type": ["int", "str"]},
            "frame_step": {"type": ["decimal", "str"]},
            "from": {"type": ["decimal", "list", "str"], "subcomponent": "str_or_decimal_or_vector_decimal_or_str"},
            "initial_uv": {"type": "list", "subcomponent": "vector_int"},
            "looping": {"type": ["bool", "str"]},
            "next": {"type": "str"},
            "orientation": {"type": "str"},
            "play_event": {"type": "str"},
            "reset_event": {"type": "str"},
            "resettable": {"type": ["bool", "str"]},
            "reversible": {"type": ["bool", "str"]},
            "scale_from_starting_alpha": {"type": ["bool", "str"]},
            "start_event": {"type": "str"},
            "to": {"type": ["decimal", "list", "str"], "subcomponent": "str_or_decimal_or_vector_decimal_or_str"},
            "type": {"type": "str"},
            "wait_until_rendered_to_play": {"type": "bool"}
        }
    },
    "animation_list": {
        "type": "Group",
        "subcomponents": {
            "str": null,
            "list": {
                "type": "Set",
                "types": "str",
                "delegate_arguments": {
                    "measure_length": true,
                    "field": "animation"
                },
                "subcomponent": {
                    "type": "String"
                }
            }
        }
    },
    "bindings": {
        "type": "Group",
        "subcomponents": {
            "str": null,
            "list": {
                "type": "Set",
                "types": "dict",
                "delegate_arguments": {
                    "field": "binding",
                    "measure_length": true
                },
                "subcomponent": "bindings.item"
            }
        }
    },
    "bindings.item": {
        "type": "Keymap",
        "keys": {
            "binding_collection_name": {"type": "str"},
            "binding_collection_prefix": {"type": "str"},
            "binding_condition": {"type": "str"},
            "binding_name": {"type": "str"},
            "binding_name_override": {"type": "str"},
            "binding_type": {"type": "str"},
            "ignored": {"type": "str"},
            "resolve_sibling_scope": {"type": "bool"},
            "source_control_name": {"type": "str"},
            "source_property_name": {"type": "str"},
            "target_property_name": {"type": "str"}
        }
    },
    "button_mappings": {
        "type": "Group",
        "subcomponents": {
            "str": null,
            "list": {
                "type": "Set",
                "types": "dict",
                "delegate_arguments": {
                    "field": "button mapping",
                    "measure_length": true
                },
                "subcomponent": "button_mappings.item"
            }
        }
    },
    "button_mappings.item": {
        "type": "Keymap",
        "keys": {
            "button_up_right_of_first_refusal": {"type": "bool"},
            "consume_event": {"type": "bool"},
            "from_button_id": {"type": "str", "subcomponent": {"type": "String"}},
            "handle_deselect": {"type": "bool"},
            "handle_select": {"type": "bool"},
            "ignore_input_scope": {"type": "str"},
            "ignored": {"type": "str"},
            "input_mode_condition": {"type": "str"},
            "mapping_type": {"type": "str"},
            "scope": {"type": "str"},
            "to_button_id": {"type": "str", "subcomponent": {"type": "String"}}
        }
    },
    "control_ids": {
        "type": "Group",
        "subcomponents": {
            "str": null,
            "dict": {
                "type": "Keymap",
                "keys": {
                    "add_resource_dialog": {"type": "str"},
                    "conflict_resolution_popup": {"type": "str"},
                    "joincode_popup": {"type": "str"},
                    "share_link_popup": {"type": "str"},
                    "teams_share_link_popup": {"type": "str"},
                    "teams_share_link_resource_popup": {"type": "str"},
                    "teams_share_link_simple_popup": {"type": "str"}
                }
            }
        }
    },
    "control_list": {
        "type": "Set",
        "types": ["str", "dict"],
        "delegate_arguments": {
            "field": "control",
            "measure_length": true
        },
        "subcomponent": {
            "type": "Group",
            "subcomponents": {
                "str": {"type": "String"},
                "dict": "control_list.item"
            }
        }
    },
    "control_list.item": {
        "type": "Keymap",
        "keys": {
            "control_name": {"type": "str", "subcomponent": {"type": "String"}}
        }
    },
    "controls": {
        "type": "Group",
        "subcomponents": {
            "str": null,
            "list": {
                "type": "Set",
                "types": "dict",
                "delegate_arguments": {
                    "field": "control",
                    "measure_length": true
                },
                "subcomponent": "control.item"
            }
        }
    },
    "control.item": {
        "type": "Dict",
        "types": "dict",
        "delegate_arguments": {
            "field": "element"
        },
        "subcomponent": "element"
    },
    "factory": {
        "type": "Group",
        "subcomponents": {
            "str": null,
            "dict": {
                "type": "Keymap",
                "normalizer": "separate_variables",
                "keys": {
                    "$variables": {"type": "dict", "subcomponent": "my_variables"},
                    "name": {"type": "str"},
                    "control_ids": {"type": ["dict", "str"], "subcomponent": "control_ids"},
                    "factory_variables": {"type": "str"}
                }
            }
        }
    },
    "focus_container_custom": {
        "type": "Group",
        "subcomponents": {
            "str": null,
            "list": {
                "type": "Set",
                "types": "dict",
                "delegate_arguments": {
                    "field": "container",
                    "measure_length": true
                },
                "subcomponent": "focus_container_custom.item"
            }
        }
    },
    "focus_container_custom.item": {
        "type": "Keymap",
        "keys": {
            "other_focus_container_name": {"type": "str"},
            "focus_id_inside": {"type": "str"}
        }
    },
    "focus_mapping": {
        "type": "Set",
        "types": "dict",
        "delegate_arguments": {
            "field": "mapping",
            "measure_length": true
        },
        "subcomponent": "focus_mapping.item"
    },
    "focus_mapping.item": {
        "type": "Keymap",
        "keys": {
            "focus_identifier": {"type": "str", "delegate_arguments": {"always_print": true}, "subcomponent": {"type": "String"}},
            "focus_change_right": {"type": "str"}
        }
