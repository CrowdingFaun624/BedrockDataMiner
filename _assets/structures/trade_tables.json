{
    "base": {
        "type": "Base",
        "name": "trade_tables",
        "subcomponent": "trade_tables",
        "imports": [
            {
                "from": "structures/entities",
                "components": [
                    {"component": "filter"}
                ]
            },
            {
                "from": "structures/loot_tables",
                "components": [
                    {"component": "range_int_or_int"},
                    {"component": "normalize_functions"},
                    {"component": "functions"}
                ]
            }
        ]
    },
    "outdated": {
        "type": "Tag"
    },
    "decimal": {
        "type": "TypeAlias",
        "types": ["float", "int"]
    },
    "trade_tables": {
        "type": "Dict",
        "field": "trade table",
        "types": ["dict"],
        "measure_length": true,
        "sort": "by_key",
        "subcomponent": "behavior_packs"
    },
    "behavior_packs": {
        "type": "Dict",
        "types": ["dict"],
        "field": "behavior pack",
        "normalizer": {
            "function_name": "collapse_resource_packs_dict_with_defined_in"
        },
        "sort": "by_key",
        "subcomponent": "trade_table"
    },
    "trade_table": {
        "type": "Keymap",
        "sort": "by_component_order",
        "keys": {
            "format_version": {"type": "str"},
            "defined_in": {"type": "list", "subcomponent": "defined_in"},
            "tiers": {"type": "list", "subcomponent": "tiers"}
        }
    },
    "defined_in": {
        "type": "Set",
        "field": "behavior pack",
        "types": ["str"],
        "measure_length": true,
        "sort": true,
        "subcomponent": null
    },
    "tiers": {
        "type": "List",
        "field": "tier",
        "types": ["dict"],
        "subcomponent": "tier"
    },
    "tier": {
        "type": "Keymap",
        "keys": {
            "groups": {"type": "list", "subcomponent": "groups", "weight": 10},
            "num_to_select": {"type": "int"},
            "total_exp_required": {"type": "int"},
            "trades": {"type": "list", "subcomponent": "trades", "weight": 10}
        }
    },
    "groups": {
        "type": "Set",
        "field": "group",
        "types": ["dict"],
        "measure_length": true,
        "min_similarity_threshold": 0.3,
        "subcomponent": "tier"
    },
    "trades": {
        "type": "Set",
        "field": "trade",
        "types": ["dict"],
        "measure_length": true,
        "min_similarity_threshold": 0.3,
        "subcomponent": "trade"
    },
    "biome_list": {
        "type": "Set",
        "field": "biome",
        "types": ["str"],
        "measure_length": true,
        "sort": true,
        "subcomponent": null
    },
    "trade": {
        "type": "Keymap",
        "sort": "by_component_order",
        "keys": {
            "weight": {"type": "int"},
            "max_uses": {"type": "int"},
            "reward_exp": {"type": "bool"},
            "total_exp_required": {"type": "int"},
            "trader_exp": {"type": "int"},
            "gives": {"type": "list", "subcomponent": "trade:gives"},
            "wants": {"type": "list", "subcomponent": "trade:wants"}
        }
    },
    "trade:gives": {
        "type": "Set",
        "field": "given item",
        "types": ["dict"],
        "measure_length": true,
        "min_similarity_threshold": 0.75,
        "subcomponent": "trade:gives.item"
    },
    "trade:wants": {
        "type": "Set",
        "field": "wanted item",
        "types": ["dict"],
        "measure_length": true,
        "min_similarity_threshold": 0.75,
        "subcomponent": "trade:wants.item"
    },
    "trade:gives.item": {
        "type": "Keymap",
        "normalizer": "normalize_functions",
        "sort": "by_component_order",
        "keys": {
            "item": {"type": "str", "subcomponent": {
                "type": "String"
            }, "weight": 12},
            "quantity": {"type": ["dict", "int"], "subcomponent": "range_int_or_int"},
            "biomes": {"type": "list", "subcomponent": "biome_list"},
            "choice": {"type": "list", "subcomponent": "trade:gives.item.choice"},
            "filters": {"type": "dict", "subcomponent": "filter"},
            "functions": {"type": "dict", "subcomponent": "functions"}
        }
    },
    "trade:gives.item.choice": {
        "type": "Set",
        "field": "choice",
        "types": ["dict"],
        "measure_length": true,
        "subcomponent": "trade:gives.item"
    },
    "trade:wants.item": {
        "type": "Keymap",
        "normalizer": "normalize_functions",
        "sort": "by_component_order",
        "keys": {
            "item": {"type": "str", "subcomponent": {
                "type": "String"
            }, "weight": 12},
            "quantity": {"type": ["dict", "int"], "subcomponent": "range_int_or_int"},
            "choice": {"type": "list", "subcomponent": "trade:wants.item.choice"},
            "biomes": {"type": "list", "subcomponent": "biome_list", "tags": ["outdated"]},
            "filters": {"type": "dict", "subcomponent": "filter"},
            "functions": {"type": "dict", "subcomponent": "functions", "tags": ["outdated"]},
            "price_multiplier": {"type": "decimal"}
        }
    },
    "trade:wants.item.choice": {
        "type": "Set",
        "field": "choice",
        "types": ["dict"],
        "measure_length": true,
        "subcomponent": "trade:wants.item"
    }
}