{
    "base": {
        "type": "Base",
        "name": "animation_controllers",
        "subcomponent": "files"
    },
    "outdated": {
        "type": "Tag"
    },
    "decimal": {
        "type": "TypeAlias",
        "types": ["float", "int"]
    },
    "empty": {
        "type": "Keymap",
        "keys": {}
    },
    "files": {
        "type": "Dict",
        "field": "file",
        "sort": "by_key",
        "types": ["dict"],
        "measure_length": true,
        "detect_key_moves": true,
        "subcomponent": "resource_packs"
    },
    "resource_packs": {
        "type": "Dict",
        "field": "resource pack",
        "types": ["dict"],
        "measure_length": true,
        "detect_key_moves": true,
        "sort": "by_key",
        "normalizer": {
            "function_name": "collapse_resource_packs_dict_with_defined_in"
        },
        "subcomponent": "animation_controllers_file"
    },
    "animation_controllers_file": {
        "type": "Keymap",
        "normalizer": {
            "function_name": "animation_controllers_fix_old"
        },
        "sort": "by_component_order",
        "keys": {
            "defined_in": {"type": "list", "subcomponent": "defined_in"},
            "format_version": {"type": "str"},
            "animation_controllers": {"type": "dict", "subcomponent": "animation_controllers"}
        }
    },
    "defined_in": {
        "type": "Set",
        "field": "resource pack",
        "types": ["str"],
        "measure_length": true,
        "subcomponent": null
    },
    "animation_controllers": {
        "type": "Dict",
        "field": "animation controller",
        "types": ["dict"],
        "measure_length": true,
        "subcomponent": "animation_controller"
    },
    "animation_controller": {
        "type": "Keymap",
        "sort": "by_component_order",
        "keys": {
            "initial_state": {"type": "str"},
            "states": {"type": "dict", "subcomponent": "states"}
        }
    },
    "states": {
        "type": "Dict",
        "field": "state",
        "sort": "by_key",
        "types": ["dict"],
        "measure_length": true,
        "subcomponent": "state"
    },
    "state": {
        "type": "Keymap",
        "keys": {
            "animations": {"type": ["dict", "list"], "subcomponent": {
                "type": "Group",
                "subcomponents": {
                    "dict": "animations:dict",
                    "list": "animations"
                }
            }},
            "blend_transition": {"type": "decimal"},
            "blend_via_shortest_path": {"type": "bool"},
            "parameters": {"type": "list", "subcomponent": "parameters", "tags": ["outdated"]},
            "particle_effects": {"type": "list", "subcomponent": "particle_effects"},
            "sound_effects": {"type": "list", "subcomponent": "sound_effects"},
            "transitions": {"type": "list", "subcomponent": "transitions"}
        }
    },
    "animations": {
        "type": "Set",
        "field": "animation dict",
        "types": ["dict", "str"],
        "measure_length": true,
        "subcomponent": {
            "type": "Group",
            "subcomponents": {
                "dict": "animation",
                "str": null
            }
        }
    },
    "animations:dict": {
        "type": "Dict",
        "field": "animation",
        "types": ["dict", "list"],
        "measure_length": true,
        "subcomponent": {
            "type": "Group",
            "subcomponents": {
                "dict": "empty",
                "list": "animation:list"
            }
        }
    },
    "animation": {
        "type": "Dict",
        "field": "animation",
        "types": ["list", "str"],
        "measure_length": true,
        "subcomponent": {
            "type": "Group",
            "subcomponents": {
                "list": "animation:list",
                "str": null
            }
        }
    },
    "animation:list": {
        "type": "Set",
        "types": ["dict"],
        "measure_length": true,
        "subcomponent": "animation:list.dict"
    },
    "animation:list.dict": {
        "type": "Dict",
        "field": "timestamp",
        "types": ["decimal"],
        "measure_length": true,
        "subcomponent": null
    },
    "parameters": {
        "type": "Set",
        "field": "parameter",
        "types": ["str"],
        "measure_length": true,
        "subcomponent": null
    },
    "particle_effects": {
        "type": "Set",
        "field": "particle effect",
        "types": ["dict"],
        "measure_length": true,
        "subcomponent": "particle_effect"
    },
    "particle_effect": {
        "type": "Keymap",
        "keys": {
            "effect": {"type": "str"},
            "locator": {"type": "str"}
        }
    },
    "sound_effects": {
        "type": "Set",
        "field": "sound effect",
        "types": ["dict"],
        "measure_length": true,
        "subcomponent": "sound_effect"
    },
    "sound_effect": {
        "type": "Keymap",
        "keys": {
            "effect": {"type": "str"}
        }
    },
    "transitions": {
        "type": "Set",
        "field": "transition list",
        "types": ["dict"],
        "measure_length": true,
        "subcomponent": "transition"
    },
    "transition": {
        "type": "Dict",
        "field": "transition",
        "types": ["str"],
        "measure_length": true,
        "subcomponent": null
    }
}