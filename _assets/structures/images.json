        },
        "subcomponent": "files"
    },
    "files": {
        "type": "Dict",
        "types": "dict",
        "subcomponent": "cache",
        "delegate_arguments": {
            "field": "file",
            "measure_length": true
        }
    },
    "cache": {
        "type": "Cache",
        "types": "dict",
        "subcomponent": "images"
    },
    "images": {
