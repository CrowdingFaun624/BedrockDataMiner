        },
        "keys": {
            "bilinear": {"type": "bool"},
            "textures": {"type": "list", "subcomponent": "textures"}
        }
    },
    "textures": {
        "type": "Set",
        "types": "str",
        "subcomponent": {
            "type": "String"
        },
        "delegate_arguments": {
            "field": "texture",
            "measure_length": true
        }
