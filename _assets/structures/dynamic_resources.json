{
    "base": {
        "type": "Base",
        "delegate_arguments": {
            "name": "dynamic_resources"
        },
        "subcomponent": "cache"
    },
    "cache": {
        "type": "Cache",
        "types": "file",
        "subcomponent": {
            "type": "File",
            "file_types": "file",
            "content_types": "dict",
            "subcomponent": "file"
        }
    },
    "file": {
        "type": "Keymap",
        "keys": {
            "resources": {"type": "dict", "subcomponent": "resources"}
        }
    },
    "resources": {
        "type": "Keymap",
        "delegate_arguments": {
            "field": "resource",
            "measure_length": true
        },
        "keys": {
            "DYNAMIC_BRIGHTNESS": {"type": "str"},
            "DYNAMIC_WEATHER": {"type": "str"},
            "DYNAMIC_SEASON_FOLIAGE": {"type": "str"}
        }
    }
}