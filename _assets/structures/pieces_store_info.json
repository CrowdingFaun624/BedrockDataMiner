{
    "base": {
        "type": "Base",
        "delegate_arguments": {
            "name": "pieces_store_info"
        },
        "subcomponent": "pieces"
    },
    "pieces": {
        "type": "Dict",
        "types": "dict",
        "subcomponent": "store_info_files",
        "sort": "by_key",
        "delegate_arguments": {
            "field": "piece"
        }
    },
    "store_info_files": {
        "type": "Dict",
        "types": "file",
        "subcomponent": "cache",
        "sort": "by_key",
        "delegate_arguments": {
            "field": "store info file"
        }
    },
    "cache": {
        "type": "Cache",
        "types": "file",
        "subcomponent": {
            "type": "File",
            "file_types": "file",
            "content_types": "dict",
            "subcomponent": "store_info"
        }
    },
    "store_info": {
        "type": "Keymap",
        "keys": {
            "piece_store_name": {"type": "str", "subcomponent": {"type": "String"}, "weight": 2, "delegate_arguments": {"always_print": true}},
            "piece_thumbnail": {"type": "str", "subcomponent": {"type": "String"}}
        }
    }
}