{
    "base": {
        "type": "Base",
        "name": "sound_files",
        "subcomponent": "sound_file"
    },
    "sound_file": {
        "type": "Dict",
        "field": "sound file",
        "types": ["dict"],
        "detect_key_moves": true,
        "sort": "by_key",
        "measure_length": true,
        "subcomponent": "internal_sound_file"
    },
    "internal_sound_file": {
        "type": "Dict",
        "field": "internal sound file",
        "types": ["dict"],
        "detect_key_moves": true,
        "measure_length": true,
        "sort": "by_key",
        "subcomponent": "sound_property"
    },
    "sound_property": {
        "type": "Keymap",
        "field": "property",
        "normalizer": {
            "function_name": "sound_files_remove_obj"
        },
        "sort": "by_component_order",
        "keys": {
            "sha1_hash": {"type": "str"},
            "filesize": {"type": "int"},
            "pictures": {"type": "list", "subcomponent": "pictures"},
            "tags": {"type": "dict", "subcomponent": "tags"},
            "_subchunks": {"type": "list", "subcomponent": "subchunks"},
            "streaminfo": {"type": "dict", "subcomponent": "streaminfo"}
        }
    },
    "pictures": {
        "type": "Set",
        "field": "picture",
        "types": ["null"],
        "subcomponent": null,
        "measure_length": true
    },
    "tags": {
        "type": "Keymap",
        "field": "tag type",
        "sort": "by_key",
        "keys": {
            "comment": {"type": "list", "subcomponent": "tag"},
            "encoder": {"type": "list", "subcomponent": "tag"},
            "title": {"type": "list", "subcomponent": "tag"},
            "tracknumber": {"type": "list", "subcomponent": "tag"}
        }
    },
    "tag": {
        "type": "Set",
        "field": "tag",
        "types": ["str"],
        "sort": true,
        "subcomponent": null
    },
    "subchunks": {
        "type": "Set",
        "types": ["null"],
        "subcomponent": null,
        "measure_length": true
    },
    "streaminfo": {
        "type": "Keymap",
        "field": "property",
        "sort": "by_key",
        "keys": {
            "_start": {"type": "int"},
            "_size": {"type": "int"},
            "_version": {"type": "list", "subcomponent": "version"},
            "_extension_data": {"type": "null"},
            "audio_format": {"type": "str"},
            "bit_depth": {"type": "int"},
            "bitrate": {"type": ["float", "int"]},
            "channels": {"type": "int"},
            "duration": {"type": "float"},
            "max_bitrate": {"type": "int"},
            "min_bitrate": {"type": "int"},
            "nominal_bitrate": {"type": "int"},
            "sample_rate": {"type": "int"}
        }
    },
    "version": {
        "type": "List",
        "field": "version number",
        "types": ["int"],
        "subcomponent": null,
        "print_flat": true
    }
}