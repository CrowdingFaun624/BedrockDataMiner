{
    "base": {
        "type": "Base",
        "delegate_arguments": {
            "name": "skin_models"
        },
        "imports": [
            {
                "from": "structures/models",
                "components": [
                    {
                        "component": "model"
                    }
                ]
            }
        ],
        "subcomponent": "cache"
    },
    "cache": {
        "type": "Cache",
        "types": "fake_file",
        "subcomponent": {
            "type": "File",
            "pre_normalized_types": "dict",
            "file_types": "fake_file",
            "garbage_collect": false,
            "content_types": "dict",
            "normalizer": {
                "function_name": "normalizers/models_model_normalize"
            },
            "subcomponent": "files"
        }
    },
    "files": {
        "type": "Dict",
        "delegate_arguments": {
            "field": "file",
            "measure_length": true
        },
        "detect_key_moves": true,
        "sort": "by_key",
        "types": "dict",
        "key_component": {
            "type": "String"
        },
        "min_key_similarity_threshold": 0.9,
        "subcomponent": "skin_packs"
    },
    "skin_packs": {
        "type": "Dict",
        "delegate_arguments": {
            "field": "skin pack",
            "measure_length": true
        },
        "types": ["dict"],
        // cannot have collapse_resource_packs normalizer
        "detect_key_moves": true,
        "sort": "by_key",
        "subcomponent": "model"
    }
}