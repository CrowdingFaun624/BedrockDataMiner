{
    "base": {
        "type": "Base",
        "delegate_arguments": {
            "name": "recipes"
        },
        "subcomponent": "recipes"
    },
    "recipes": {
        "type": "Dict",
        "delegate_arguments": {
            "field": "recipe",
            "measure_length": true
        },
        "types": ["dict"],
        "detect_key_moves": true,
        "sort": "by_key",
        "subcomponent": "behavior_packs"
    },
    "behavior_packs": {
        "type": "Dict",
        "delegate_arguments": {
            "field": "behavior pack",
            "measure_length": true
        },
        "types": ["file"],
        "detect_key_moves": true,
        "post_normalizer": {
            "function_name": "normalizers/collapse_resource_packs/flat"
        },
        "sort": "by_key",
        "subcomponent": "cache"
    },
    "defined_in": {
        "type": "Set",
        "delegate_arguments": {
            "field": "behavior pack",
            "measure_length": true
        },
        "types": ["str"],
        "sort": true,
        "subcomponent": null
    },
    "description": {
        "type": "Keymap",
        "keys": {
            "identifier": {"type": "str", "subcomponent": {"type": "String"}, "delegate_arguments": {"always_print": true}}
        }
    },
    "item": {
        "type": "Keymap",
        "keys": {
            "count": {"type": "int"},
            "data": {"type": "int"},
            "item": {"type": "str", "subcomponent": {"type": "String"}, "delegate_arguments": {"always_print": true}},
            "tag": {"type": "str"}
        }
    },
    "item_or_str": {
        "type": "Group",
        "subcomponents": {
            "dict": "item",
            "str": null
        }
    },
    "item_list": {
        "type": "Set",
        "delegate_arguments": {
            "field": "item",
            "measure_length": true
        },
        "types": ["dict"],
        "subcomponent": "item"
    },
    "item_list_or_item": {
        "type": "Group",
        "subcomponents": {
            "dict": "item",
            "list": "item_list"
        }
    },
    "tag_list": {
        "type": "Set",
        "delegate_arguments": {
            "field": "tag",
            "measure_length": true
        },
        "types": ["str"],
        "subcomponent": null,
        "sort": true
    },
    "tag_list_or_tag": {
        "type": "Group",
        "subcomponents": {
            "list": "tag_list",
            "str": null
        }
    },
    "cache": {
        "type": "Cache",
        "types": "file",
        "subcomponent": {
            "type": "File",
            "file_types": "file",
            "content_types": "dict",
            "subcomponent": "recipe"
        }
    },
    "recipe": {
        "type": "Keymap",
        "normalizer":  {
            "function_name": "normalizers/recipes_fix_old"
        },
        "sort": "by_component_order",
        "keys": {
            "format_version": {"type": "str"},
            "defined_in": {"type": "list", "subcomponent": "defined_in"},
            "minecraft:recipe_brewing_container": {"type": "dict", "subcomponent": "recipe.brewing_container"},
            "minecraft:recipe_brewing_mix": {"type": "dict", "subcomponent": "recipe.brewing_mix"},
            "minecraft:recipe_furnace": {"type": "dict", "subcomponent": "recipe.furnace"},
            "minecraft:recipe_material_reduction": {"type": "dict", "subcomponent": "recipe.material_reducer"},
            "minecraft:recipe_shaped": {"type": "dict", "subcomponent": "recipe.shaped"},
            "minecraft:recipe_shapeless": {"type": "dict", "subcomponent": "recipe.shapeless"},
            "minecraft:recipe_smithing_transform": {"type": "dict", "subcomponent": "recipe.smithing_transform"},
            "minecraft:recipe_smithing_trim": {"type": "dict", "subcomponent": "recipe.smithing_trim"}
        }
    },
    "recipe.brewing_container": {
        "type": "Keymap",
        "sort": "by_component_order",
        "keys": {
            "description": {"type": "dict", "subcomponent": "description", "delegate_arguments": {"always_print": true}},
            "input": {"type": "str"},
            "output": {"type": "str"},
            "reagent": {"type": "str"},
            "tags": {"type": ["list", "str"], "subcomponent": "tag_list_or_tag"}
        }
    },
    "recipe.brewing_mix": {
        "type": "Keymap",
        "sort": "by_component_order",
        "keys": {
            "description": {"type": "dict", "subcomponent": "description", "delegate_arguments": {"always_print": true}},
            "input": {"type": "str"},
            "output": {"type": "str"},
            "reagent": {"type": "str"},
            "tags": {"type": ["list", "str"], "subcomponent": "tag_list_or_tag"}
        }
    },
    "recipe.furnace": {
        "type": "Keymap",
        "sort": "by_component_order",
        "keys": {
            "description": {"type": "dict", "subcomponent": "description", "delegate_arguments": {"always_print": true}},
            "input": {"type": ["dict", "str"], "subcomponent": "item_or_str"},
            "output": {"type": "str"},
            "priority": {"type": "int"},
            "tags": {"type": ["list", "str"], "subcomponent": "tag_list_or_tag"},
            "unlock": {"type": ["dict", "list"], "subcomponent": "recipe:unlock_group"}
        }
    },
    "recipe.material_reducer": {
        "type": "Keymap",
        "sort": "by_component_order",
        "keys": {
            "description": {"type": "dict", "subcomponent": "description", "delegate_arguments": {"always_print": true}},
            "input": {"type": ["dict", "str"], "subcomponent": "item_or_str"},
            "output": {"type": "list", "subcomponent": "item_list"},
            "tags": {"type": ["list", "str"], "subcomponent": "tag_list_or_tag"}
        }
    },
    "recipe.shaped": {
        "type": "Keymap",
        "sort": "by_component_order",
        "keys": {
            "description": {"type": "dict", "subcomponent": "description", "delegate_arguments": {"always_print": true}},
            "group": {"type": "str"},
            "key": {"type": "dict", "subcomponent": "recipe.shaped:keys"},
            "pattern": {"type": "list", "subcomponent": "recipe.shaped:pattern"},
            "priority": {"type": "int"},
            "result": {"type": ["dict", "list"], "subcomponent": "item_list_or_item"},
            "tags": {"type": ["list", "str"], "subcomponent": "tag_list_or_tag"},
            "unlock": {"type": ["dict", "list"], "subcomponent": "recipe:unlock_group"}
        }
    },
    "recipe.shaped:keys": {
        "type": "Dict",
        "delegate_arguments": {
            "field": "key",
            "measure_length": true,
            "print_all": true
        },
        "types": ["dict"],
        "detect_key_moves": true,
        "subcomponent": "item"
    },
    "recipe.shaped:pattern": {
        "type": "List",
        "delegate_arguments": {
            "field": "line",
            "measure_length": false,
            "print_all": true,
            "print_flat": false
        },
        "types": ["str"],
        "subcomponent": null
    },
    "recipe.shapeless": {
        "type": "Keymap",
        "sort": "by_component_order",
        "keys": {
            "description": {"type": "dict", "subcomponent": "description", "delegate_arguments": {"always_print": true}},
            "group": {"type": "str"},
            "ingredients": {"type": "list", "subcomponent": "recipe.shapeless:ingredients"},
            "priority": {"type": "int"},
            "result": {"type": ["dict", "list"], "subcomponent": "item_list_or_item"},
            "tags": {"type": ["list", "str"], "subcomponent": "tag_list_or_tag"},
            "unlock": {"type": ["dict", "list"], "subcomponent": "recipe:unlock_group"}
        }
    },
    "recipe.shapeless:ingredients": {
        "type": "Set",
        "types": ["dict"],
        "delegate_arguments": {
            "measure_length": true,
            "print_all": true,
            "field": "ingredient"
        },
        "subcomponent": "item"
    },
    "recipe.smithing_transform": {
        "type": "Keymap",
        "sort": "by_component_order",
        "keys": {
            "addition": {"type": "str"},
            "base": {"type": "str"},
            "description": {"type": "dict", "subcomponent": "description", "delegate_arguments": {"always_print": true}},
            "result": {"type": "str"},
            "tags": {"type": ["list", "str"], "subcomponent": "tag_list_or_tag"},
            "template": {"type": "str"}
        }
    },
    "recipe.smithing_trim": {
        "type": "Keymap",
        "sort": "by_component_order",
        "keys": {
            "addition": {"type": "dict", "subcomponent": "item"},
            "base": {"type": "dict", "subcomponent": "item"},
            "description": {"type": "dict", "subcomponent": "description", "delegate_arguments": {"always_print": true}},
            "tags": {"type": ["list", "str"], "subcomponent": "tag_list_or_tag"},
            "template": {"type": "dict", "subcomponent": "item"}
        }
    },
    "recipe:unlock_group": {
        "type": "Group",
        "subcomponents": {
            "dict": {
                "type": "Keymap",
                "keys": {
                    "context": {"type": "str", "subcomponent": {"type": "String"}, "delegate_arguments": {"always_print": true}}
                }
            },
            "list": {
                "type": "Set",
                "delegate_arguments": {
                    "field": "condition",
                    "measure_length": true
                },
                "types": ["dict"],
                "subcomponent": "item"
            }
        }
    }
}