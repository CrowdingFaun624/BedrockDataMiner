{
    "base": {
        "type": "Base",
        "delegate_arguments": {
            "name": "blocks_client"
        },
        "subcomponent": "cache"
    },
    "cache": {
        "type": "Cache",
        "types": "fake_file",
        "subcomponent": {
            "type": "File",
            "pre_normalized_types": "dict",
            "file_types": "fake_file",
            "garbage_collect": false, // other methods will catch the files. This is a fake file and does not matter.
            "content_types": "dict",
            "normalizer": {
                "function_name": "normalizers/blocks_client_normalize"
            },
            "subcomponent": "block"
        }
    },
    "block": {
        "type": "Dict",
        "delegate_arguments": {
            "field": "block",
            "measure_length": true
        },
        "types": ["dict"],
        "detect_key_moves": true,
        "min_key_similarity_threshold": 0.2,
        "min_value_similarity_threshold": 0.4,
        "key_weight": 0.5,
        "value_weight": 0.5,
        "sort": "by_key",
        "key_component": {
            "type": "String"
        },
        "subcomponent": "resource_pack"
    },
    "resource_pack": {
        "type": "Dict",
        "delegate_arguments": {
            "field": "resource pack",
            "measure_length": true
        },
        "types": ["dict"],
        "detect_key_moves": true,
        "sort": "by_key",
        "min_value_similarity_threshold": 0.25,
        "post_normalizer": {
            "function_name": "normalizers/collapse_resource_packs/dict"
        },
        "subcomponent": "block_properties"
    },
    "block_properties": {
        "type": "Keymap",
        "delegate_arguments": {
            "field": "property"
        },
        "normalizer": {
            // https://bugs.mojang.com/browse/MCPE-76182
            "function_name": "delete_optional_key",
            "arguments": {
                "key": "sounds"
            }
        },
        "sort": "by_component_order",
        "keys": {
            "ambient_occlusion_exponent": {"type": "float"},
            "blockshape": {"type": "str", "weight": 4},
            "brightness_gamma": {"type": "float"},
            "carried_textures": {"type": ["dict", "str"], "subcomponent": "textures_group", "weight": 4},
            "defined_in": {"type": "list", "subcomponent": "defined_in", "weight": 0},
            "isotropic": {"type": ["bool", "dict"], "subcomponent": "isotropic_group"},
            "sound": {"type": "str", "weight": 2},
            "textures": {"type": ["dict", "str"], "subcomponent": "textures_group", "weight": 4}
        }
    },
    "textures_group": {
        "type": "Group",
        "subcomponents": {
            "dict": "textures",
            "str": "texture"
        }
    },
    "isotropic_group": {
        "type": "Group",
        "subcomponents": {
            "bool": null,
            "dict": "isotropic"
        }
    },
    "defined_in": {
        "type": "Set",
        "delegate_arguments": {
            "field": "resource pack",
            "print_flat": true
        },
        "subcomponent": null,
        "types": ["str"]
    },
    "textures": {
        "type": "Keymap",
        "delegate_arguments": {
            "field": "direction",
            "print_all": true
        },
        "sort": "by_component_order",
        "keys": {
            "down": {"type": "str", "subcomponent": "texture"},
            "east": {"type": "str", "subcomponent": "texture"},
            "north": {"type": "str", "subcomponent": "texture"},
            "side": {"type": "str", "subcomponent": "texture"},
            "south": {"type": "str", "subcomponent": "texture"},
            "up": {"type": "str", "subcomponent": "texture"},
            "west": {"type": "str", "subcomponent": "texture"}
        }
    },
    "texture": {
        "type": "String"
    },
    "isotropic": {
        "type": "Keymap",
        "delegate_arguments": {
            "field": "direction",
            "print_all": true
        },
        "sort": "by_component_order",
        "keys": {
            "down": {"type": "bool"},
            "up": {"type": "bool"}
        }
    }
}