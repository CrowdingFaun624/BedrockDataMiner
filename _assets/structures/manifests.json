{
    "base": {
        "type": "Base",
        "delegate_arguments": {
            "name": "manifests"
        },
        "subcomponent": "packs"
    },
    "version": {
        "type": "Group",
        "subcomponents": {
            "float": null,
            "str": null,
            "list": {
                "type": "List",
                "subcomponent": null,
                "types": ["int", "float"],
                "delegate_arguments": {
                    "print_flat": true,
                    "print_all": true
                }
            }
        }
    },
    "packs": {
        "type": "Dict",
        "types": "file",
        "delegate_arguments": {
            "field": "pack"
        },
        "sort": "by_key",
        "subcomponent": "cache"
    },
    "cache": {
        "type": "Cache",
        "types": "file",
        "subcomponent": {
            "type": "File",
            "file_types": "file",
            "content_types": "dict",
            "subcomponent": "manifest"
        }
    },
    "manifest": {
        "type": "Keymap",
        "sort": "by_component_order",
        "keys": {
            "format_version": {"type": ["int", "float"]},
            "header": {"type": "dict", "subcomponent": "header", "delegate_arguments": {"always_print": true}, "weight": 12},
            "modules": {"type": "list", "subcomponent": "modules"},
            "dependencies": {"type": "list", "subcomponent": "dependencies"},
            "capabilities": {"type": "list", "subcomponent": "capabilities"},
            "description": {"type": "str", "tags": ["outdated"]},
            "key_art_path": {"type": "str", "tags": ["outdated"]},
            "name": {"type": "str", "subcomponent": {"type": "String"}, "delegate_arguments": {"always_print": true}, "tags": ["outdated"]},
            "pack_id": {"type": "str", "tags": ["outdated"]},
            "packs_version": {"type": "str", "tags": ["outdated"]},
            "screenshots": {"type": "list", "subcomponent": "screenshots", "tags": ["outdated"]},
            "metadata": {"type": "dict", "subcomponent": "metadata"},
            "store_offer_name": {"type": "str"}
        }
    },
    "header": {
        "type": "Keymap",
        "sort": "by_component_order",
        "keys": {
            "description": {"type": "str", "subcomponent": {"type": "String"}},
            "name": {"type": "str", "subcomponent": {"type": "String"}, "delegate_arguments": {"always_print": true}},
            "type": {"type": "str"},
            "uuid": {"type": "str", "delegate_arguments": {"always_print": true}, "weight": 12},
            "version": {"type": ["list", "float", "str"], "subcomponent": "version"},
            "min_engine_version": {"type": ["list", "float", "str"], "subcomponent": "version"},
            "pack_scope": {"type": "str"},
            "pack_id": {"type": "str", "tags": ["outdated"]},
            "packs_version": {"type": "str", "tags": ["outdated"]},
            "store_offer_name": {"type": "str", "tags": ["outdated"]},
            "key_art_path": {"type": "str", "tags": ["outdated"]},
            "screenshots": {"type": "list", "subcomponent": "screenshots", "tags": ["outdated"]},
            "modules": {"type": "list", "subcomponent": "modules"},
            "dependencies": {"type": "list", "subcomponent": "dependencies"},
            "module_name": {"type": "str", "subcomponent": {"type": "String"}, "tags": ["outdated"]},
            "entry": {"type": "str", "tags": ["outdated"]},
            "language": {"type": "str"},
            "metadata": {"type": "dict", "subcomponent": "metadata"}
        }
    },
    "modules": {
        "type": "Set",
        "types": "dict",
        "delegate_arguments": {
            "field": "module"
        },
        "subcomponent": "header"
    },
    "dependencies": {
        "type": "Set",
        "types": "dict",
        "delegate_arguments": {
            "field": "dependency"
        },
        "subcomponent": "header"
    },
    "capabilities": {
        "type": "Set",
        "types": "str",
        "delegate_arguments": {
            "field": "capability"
        },
        "subcomponent": {
            "type": "String"
        }
    },
    "screenshots": {
        "type": "Set",
        "types": "dict",
        "delegate_arguments": {
            "field": "screenshot",
            "measure_length": true
        },
        "subcomponent": "screenshot"
    },
    "screenshot": {
        "type": "Keymap",
        "keys": {
            "path": {"type": "str", "subcomponent": {"type": "String"}, "delegate_arguments": {"always_print": true}}
        }
    },
    "metadata": {
        "type": "Keymap",
        "keys": {
            "authors": {"type": "list", "subcomponent": "authors"},
            "url": {"type": "str"}
        }
    },
    "authors": {
        "type": "Set",
        "types": "str",
        "delegate_arguments": {
            "field": "author",
            "measure_length": true
        },
        "subcomponent": {
            "type": "String"
        }
    }
}