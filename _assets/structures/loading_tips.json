{
    "base": {
        "type": "Base",
        "delegate_arguments": {
            "name": "loading_tips"
        },
        "subcomponent": "cache"
    },
    "cache": {
        "type": "Cache",
        "types": "fake_file",
        "subcomponent": {
            "type": "File",
            "pre_normalized_types": "dict",
            "file_types": "fake_file",
            "garbage_collect": false,
            "content_types": "dict",
            "normalizer": {
                "function_name": "normalizers/collapse_resource_packs/dict_file",
                "arguments": {
                    "add_defined_in": false
                }
            },
            "subcomponent": "resource_packs"
        }
    },
    "resource_packs": {
        "type": "Dict",
        "delegate_arguments": {
            "field": "resource pack"
        },
        "types": ["dict"],
        "sort": "by_key",
        "subcomponent": "categories"
    },
    "categories": {
        "type": "Dict",
        "delegate_arguments": {
            "field": "category"
        },
        "types": ["list"],
        "detect_key_moves": true,
        "sort": "by_key",
        "subcomponent": "tips"
    },
    "tips": {
        "type": "Set",
        "delegate_arguments": {
            "field": "tip"
        },
        "types": ["str"],
        "sort": true,
        "subcomponent": null
    }
}