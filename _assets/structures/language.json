{
    "base": {
        "type": "Base",
        "delegate_arguments": {
            "name": "language"
        },
        "subcomponent": "resource_packs"
    },
    "resource_packs": {
        "type": "Dict",
        "delegate_arguments": {
            "field": "resource pack",
            "measure_length": true
        },
        "types": ["dict"],
        "normalizer": {
            "function_name": "normalizers/collapse_resource_packs/dict",
            "arguments": {
                "add_defined_in": false
            }
        },
        "sort": "by_key",
        "subcomponent": "keys"
    },
    "keys": {
        "type": "Dict",
        "delegate_arguments": {
            "field": "key",
            "measure_length": true
        },
        "types": ["dict"],
        "detect_key_moves": true,
        "sort": "by_key",
        "subcomponent": "properties"
    },
    "properties": {
        "type": "Keymap",
        "sort": "by_component_order",
        "keys": {
            "value": {"type": "str", "delegate_arguments": {"always_print": true}},
            "comment": {"type": "str"}
        }
    }
}