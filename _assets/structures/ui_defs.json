{
    "base": {
        "type": "Base",
        "delegate_arguments": {
            "name": "ui_defs"
        },
        "subcomponent": "resource_packs"
    },
    "resource_packs": {
        "type": "Dict",
        "types": "file",
        "delegate_arguments": {
            "measure_length": true,
            "field": "resource pack"
        },
        "sort": "by_key",
        "subcomponent": "cache"
    },
    "cache": {
        "type": "Cache",
        "types": "file",
        "subcomponent": {
            "type": "File",
            "file_types": "file",
            "content_types": "dict",
            "subcomponent": "file"
        }
    },
    "file": {
        "type": "Keymap",
        "keys": {
            "ui_defs": {"type": "list", "subcomponent": "ui_defs"}
        }
    },
    "ui_defs": {
        "type": "Set",
        "types": "str",
        "subcomponent": {
            "type": "String"
        },
        "delegate_arguments": {
            "field": "def",
            "measure_length": true
        }
    }
}