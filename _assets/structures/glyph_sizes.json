{
    "base": {
        "type": "Base",
        "delegate_arguments": {
            "name": "glyph_sizes"
        },
        "subcomponent": "cache"
    },
    "cache": {
        "type": "Cache",
        "types": "file",
        "subcomponent": {
            "type": "File",
            "file_types": "file",
            "content_types": "dict",
            "subcomponent": "file"
        }
    },
    "file": {
        "type": "Dict",
        "delegate_arguments": {
            "measure_length": true,
            "field": "character"
        },
        "types": "dict",
        "detect_key_moves": false,
        "subcomponent": "range"
    },
    "range": {
        "type": "Keymap",
        "delegate_arguments": {
            "field": "end"
        },
        "sort": "by_component_order",
        "keys": {
            "left": {"type": "int"},
            "right": {"type": "int"}
        }
    }
}