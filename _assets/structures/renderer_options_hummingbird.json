        "sort": "by_component_order",
        "keys": {
            "name": {"type": "str", "subcomponent": {"type": "String"}, "delegate_arguments": {"always_print": true}, "weight": 4},
            "vertexShader": {"type": "str"},
            "fragment_shader": {"type": "str"}
        }
    },
    "mappings": {
        "type": "Dict",
        "normalizer": {
            "function_name": "normalizers/renderer_options_dragon_normalize"
        },
        "delegate_arguments": {
            "field": "mapping",
            "measure_length": true
        },
        "types": ["str", "null"],
        "key_component": {
            "type": "String"
