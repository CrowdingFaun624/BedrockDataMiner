{
    "base": {
        "type": "Base",
        "delegate_arguments": {
            "name": "sound_definitions"
        },
        "subcomponent": "cache"
    },
    "cache": {
        "type": "Cache",
        "types": "fake_file",
        "subcomponent": {
            "type": "File",
            "pre_normalized_types": "dict",
            "file_types": "fake_file",
            "garbage_collect": false,
            "content_types": "dict",
            "normalizer": {
                "function_name": "normalizers/normalize_sound_definitions"
            },
            "subcomponent": "sound_event"
        }
    },
    "sound_event": {
        "type": "Dict",
        "delegate_arguments": {
            "field": "sound event",
            "measure_length": true
        },
        "types": ["dict"],
        "sort": "by_key",
        "detect_key_moves": true,
        "key_component": {
            "type": "String"
        },
        "min_key_similarity_threshold": 0.1,
        "subcomponent": "resource_pack"
    },
    "resource_pack": {
        "type": "Dict",
        "delegate_arguments": {
            "field": "resource pack",
            "measure_length": true
        },
        "types": ["dict"],
        "detect_key_moves": true,
        "sort": "by_key",
        "normalizer": {
            "function_name": "normalizers/collapse_resource_packs/dict"
        },
        "subcomponent": "sound_event_property"
    },
    "sound_event_property": {
        "type": "Keymap",
        "delegate_arguments": {
            "field": "property"
        },
        "normalizer": {
            // https://bugs.mojang.com/browse/MCPE-153558
            // https://bugs.mojang.com/browse/MCPE-178265
            "function_name": "delete_optional_keys",
            "arguments": {
                "keys": ["pitch", "volume"]
            }
        },
        "sort": "by_component_order",
        "keys": {
            "category": {"type": "str"},
            "defined_in": {"type": "list", "subcomponent": "defined_in"},
            "max_distance": {"type": ["float", "int"]},
            "min_distance": {"type": ["float", "int"]},
            "subtitle": {"type": "str"},
            "sounds": {"type": "list", "subcomponent": "sounds"}
        }
    },
    "sounds": {
        "type": "Set",
        "delegate_arguments": {
            "field": "sound",
            "print_all": true,
            "measure_length": true
        },
        "types": ["dict"],
        "min_similarity_threshold": 0.3,
        "subcomponent": "sound_properties"
    },
    "sound_properties": {
        "type": "Keymap",
        "delegate_arguments": {
            "field": "property"
        },
        "pre_normalized_types": ["str", "dict"],
        "normalizer": [
            {
                "function_name": "normalizers/sound_definitions_make_strings_to_dict"
            },
            {
                // https://bugs.mojang.com/browse/MCPE-153561
                "function_name": "delete_optional_key",
                "arguments": {
                    "key": "pitch:"
                }
            }
        ],
        "sort": "by_component_order",
        "keys": {
            "name": {"type": "str", "subcomponent": {
                "type": "String",
                "max_similarity_ancestor_depth": 7
            }, "weight": 10, "required": true, "delegate_arguments": {"always_print": true}},
            "type": {"type": "str", "delegate_arguments": {"always_print": true}},
            "exclude_from_pocket_platforms": {"type": "bool"},
            "is3D": {"type": "bool"},
            "load_on_low_memory": {"type": "bool"},
            "pitch": {"type": ["float", "int"]},
            "stream": {"type": "bool"},
            "volume": {"type": ["float", "int"]},
            "weight": {"type": "int"}
        }
    },
    "defined_in": {
        "type": "Set",
        "delegate_arguments": {
            "field": "resource pack",
            "print_flat": true
        },
        "types": ["str"],
        "subcomponent": null,
        "sort": true
    }
}