{
    "json": {
        "serializer_class": "JsonSerializer"
    },
    "language_serializer": {
        "serializer_class": "LanguageSerializer"
    },
    "lines_serializer": {
        "serializer_class": "LinesSerializer"
    },
    "nbt_big": {
        "serializer_class": "NbtSerializer",
        "arguments": {
            "endianness": "big",
            "gzipped": true
        }
    },
    "nbt_little": {
        "serializer_class": "NbtSerializer",
        "arguments": {
            "endianness": "little",
            "gzipped": false
        }
    },
    "models_repairable_json": {
        "serializer_class": "RepairableJsonSerializer",
        "arguments": {
            "remove_suffix": "\r\n\r\n}\r\n"
        }
    },
    "resources_repairable_json": {
        "serializer_class": "RepairableJsonSerializer",
        "arguments": {
            "replace_suffix_old": "\r\n}\r\n}",
            "replace_suffix_new": "\r\n}"
        }
    },
    "sound_files_serializer": {
        "serializer_class": "SoundSerializer"
    },
    "media_serializer": {
        "serializer_class": "MediaSerializer"
    },
    "text_serializer": {
        "serializer_class": "TextSerializer"
    },
    "material_bin_serializer": {
        "serializer_class": "MaterialBinSerializer"
    },
    "glyph_sizes_serializer": {
        "serializer_class": "GlyphSizesSerializer"
    },
    "javascript_serializer": {
        "serializer_class": "TextSerializer"
    },
    "css_serializer": {
        "serializer_class": "TextSerializer"
    },
    "html_serializer": {
        "serializer_class": "TextSerializer"
    }
}