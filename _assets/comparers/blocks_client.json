{
    "main": {
        "type": "Main",
        "normalizer": "normalize",
        "dependencies": ["resource_packs"],
        "base_comparer_section": "block"
    },
    "block": {
        "type": "Dict",
        "field": "block",
        "key_types": ["str"],
        "value_types": ["dict"],
        "detect_key_moves": true,
        "measure_length": true,
        "comparer": "resource_pack"
    },
    "resource_pack": {
        "type": "Dict",
        "field": "resource pack",
        "key_types": ["str"],
        "value_types": ["dict"],
        "detect_key_moves": true,
        "comparison_move_function": "resource_pack_comparison_move_function",
        "measure_length": true,
        "comparer": "block_properties"
    },
    "block_properties": {
        "type": "TypedDict",
        "field": "property",
        "types": {
            "blockshape": {"type": "str"},
            "brightness_gamma": {"type": "float"},
            "carried_textures": {"type": ["dict", "str"], "comparer": "textures_group"},
            "defined_in": {"type": "list", "comparer": "defined_in"},
            "isotropic": {"type": ["bool", "dict"], "comparer": "isotropic_group"},
            "sound": {"type": "str"},
            "textures": {"type": ["dict", "str"], "comparer": "textures_group"}
        }
    },
    "textures_group": {
        "type": "Group",
        "types": {
            "dict": "textures",
            "str": null
        }
    },
    "isotropic_group": {
        "type": "Group",
        "types": {
            "bool": null,
            "dict": "isotropic"
        }
    },
    "defined_in": {
        "field": "resource pack",
        "type": "List",
        "comparer": null,
        "types": ["str"],
        "print_flat": true,
        "ordered": false
    },
    "textures": {
        "type": "TypedDict",
        "field": "direction",
        "types": {
            "down": {"type": "str"},
            "east": {"type": "str"},
            "north": {"type": "str"},
            "side": {"type": "str"},
            "south": {"type": "str"},
            "up": {"type": "str"},
            "west": {"type": "str"}
        }
    },
    "isotropic": {
        "type": "TypedDict",
        "field": "direction",
        "types": {
            "down": {"type": "bool"},
            "up": {"type": "bool"}
        }
    }
}