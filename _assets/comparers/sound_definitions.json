{
    "main": {
        "type": "Main",
        "name": "sound_definitions",
        "normalizer": "normalize",
        "dependencies": ["resource_packs"],
        "base_comparer_section": "sound_event"
    },
    "sound_event": {
        "type": "Dict",
        "field": "sound event",
        "key_types": ["str"],
        "value_types": ["dict"],
        "detect_key_moves": true,
        "measure_length": true,
        "comparer": "resource_pack"
    },
    "resource_pack": {
        "type": "Dict",
        "field": "resource pack",
        "key_types": ["str"],
        "value_types": ["dict"],
        "detect_key_moves": true,
        "comparison_move_function": "resource_pack_comparison_move_function",
        "measure_length": true,
        "comparer": "sound_event_property"
    },
    "sound_event_property": {
        "type": "TypedDict",
        "field": "property",
        "types": {
            "category": {"type": "str"},
            "defined_in": {"type": "list", "comparer": "defined_in"},
            "max_distance": {"type": ["float", "int"]},
            "min_distance": {"type": ["float", "int"]},
            "sounds": {"type": "dict", "comparer": "sounds"},
            "subtitle": {"type": "str"}
        }
    },
    "sounds": {
        "type": "Dict",
        "field": "sound",
        "key_types": ["str"],
        "value_types": ["dict"],
        "measure_length": true,
        "print_all": true,
        "detect_key_moves": true,
        "comparison_move_function": "sound_comparison_move_function",
        "comparer": "sound_properties"
    },
    "sound_properties": {
        "type": "TypedDict",
        "field": "property",
        "types": {
            "exclude_from_pocket_platforms": {"type": "bool"},
            "is3D": {"type": "bool"},
            "load_on_low_memory": {"type": "bool"},
            "pitch": {"type": ["float", "int"]},
            "stream": {"type": "bool"},
            "type": {"type": "str"},
            "volume": {"type": ["float", "int"]},
            "weight": {"type": "int"}
        }
    },
    "defined_in": {
        "type": "List",
        "field": "resource pack",
        "types": ["str"],
        "comparer": null,
        "print_flat": true,
        "ordered": false
    }
}