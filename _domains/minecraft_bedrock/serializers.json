    "models_repairable_json": {
        "serializer_class": "RepairableJsonSerializer",
        "arguments": {
            "rules": [
                {
                    "condition": {
                        "type": "always"
                    },
                    "action": {
                        "type": "remove_suffix",
                        "content": "\r\n\r\n}\r\n"
                    }
                }
            ]
        }
    },
    "resources_repairable_json": {
        "serializer_class": "RepairableJsonSerializer",
        "arguments": {
            "rules": [
                {
                    "condition": {
                        "type": "suffix",
                        "content": "\r\n}\r\n}"
                    },
                    "action": {
                        "type": "sequence",
                        "actions": [
                            {
                                "type": "remove_suffix",
                                "content": "\r\n}\r\n}"
                            },
                            {
                                "type": "append",
                                "content": "\r\n}"
                            }
                        ]
                    }
                }
            ]
        }
    },
    "loot_tables_repairable_json": {
        "serializer_class": "RepairableJsonSerializer",
        "arguments": {
            "empty_okay": true,
            "rules": [
                {
                    "condition": {
                        "type": "exception"
                    },
                    "action": {
                        "type": "replace",
                        "content": "{}"
                    }
                }
            ]
        }
    },
    "texture_backwards_compatibility_mapping_repairable_json": {
        "serializer_class": "RepairableJsonSerializer",
        "arguments": {
            "rules": [
                {
                    "condition": {
                        "type": "always"
                    },
                    "action": {
                        "type": "replace_some",
                        "old_content": "}\r\n  \"",
                        "new_content": "},\r\n  \"",
                        "count": 1
                    }
                }
            ]
        }
    },
    "skin_geometries_repairable_json": {
        "serializer_class": "RepairableJsonSerializer",
        "arguments": {
            "rules": [
                {
                    "condition": {
                        "type": "always"
                    },
                    "action": {
                        "type": "replace_some",
                        "old_content": "[0, 95]",
                        "new_content": "[0, 95],",
                        "count": 1
                    }
                }
            ]
        }
    },
    "sound_files_serializer": {
        "serializer_class": "SoundSerializer"
    },
    "media_serializer": {
