{
    "base": {
        "type": "StructureBase",
        "delegate_arguments": {
            "name": "recipes"
        },
        "this_types": "dict",
        "structure": "recipes"
    },
    "recipes": {
        "type": "Dict",
        "delegate_arguments": {
            "field": "recipe",
            "sort": "sort_by_key",
            "measure_length": true
        },
        "value_types": "dict",
        "min_key_similarity_threshold": 0.50,
        "value_structure": "behavior_packs"
    },
    "behavior_packs": {
        "type": "Dict",
        "delegate_arguments": {
            "field": "behavior pack",
            "sort": "sort_by_key",
            "measure_length": true
        },
        "value_types": "dict",
        "post_normalizers": {
            "function_name": "normalizers/collapse_resource_packs collapse_resource_packs_flat"
        },
        "value_structure": "cache"
    },
    "description": {
        "type": "Keymap",
        "keys": {
            "identifier": {"types": "str", "structure": {"type": "String"}, "delegate_arguments": {"always_print": true}}
        }
    },
    "item": {
        "type": "Keymap",
        "keys": {
            "count": {"types": "int"},
            "data": {"types": "int"},
            "item": {"types": "str", "structure": {"type": "String"}, "delegate_arguments": {"always_print": true}},
            "tag": {"types": "str"}
        }
    },
    "item_or_str": {
        "type": "Union",
        "substructures": {
            "dict": "item",
            "str": null
        }
    },
    "item_list": {
        "type": "Dict",
        "key_weight": 0,
        "key_types": "int",
        "this_types": "list",
        "delegate_arguments": {
            "field": "item",
            "show_item_key": false,
            "measure_length": true
        },
        "value_types": "dict",
        "value_structure": "item"
    },
    "item_list_or_item": {
        "type": "Union",
        "substructures": {
            "dict": "item",
            "list": "item_list"
        }
    },
    "tag_list": {
        "type": "Dict",
        "key_weight": 0,
        "key_types": "int",
        "this_types": "list",
        "delegate_arguments": {
            "field": "tag",
            "show_item_key": false,
            "sort": "sort_by_value",
            "measure_length": true
        },
        "value_types": "str",
        "value_structure": null
    },
    "tag_list_or_tag": {
        "type": "Union",
        "substructures": {
            "list": "tag_list",
            "str": null
        }
    },
    "cache": {
        "type": "Cache",
        "this_types": "dict",
        "pre_normalized_types": "file",
        "structure": {
            "type": "File",
            "serializer": "minecraft_common!json",
            "file_types": "file",
            "content_types": "dict",
            "structure": "recipe"
        }
    },
    "recipe": {
        "type": "Keymap",
        "normalizers":  {
            "function_name": "normalizers/normalizers recipes_fix_old"
        },
        "delegate_arguments": {
            "sort": "sort_by_component_order"
        },
        "keys": {
            "format_version": {"types": "str"},
            "minecraft:recipe_brewing_container": {"types": "dict", "structure": "recipe.brewing_container"},
            "minecraft:recipe_brewing_mix": {"types": "dict", "structure": "recipe.brewing_mix"},
            "minecraft:recipe_furnace": {"types": "dict", "structure": "recipe.furnace"},
            "minecraft:recipe_material_reduction": {"types": "dict", "structure": "recipe.material_reducer"},
            "minecraft:recipe_shaped": {"types": "dict", "structure": "recipe.shaped"},
            "minecraft:recipe_shapeless": {"types": "dict", "structure": "recipe.shapeless"},
