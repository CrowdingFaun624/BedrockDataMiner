            "result": {"types": ["dict", "list"], "structure": "item_list_or_item"},
            "tags": {"types": ["list", "str"], "structure": "tag_list_or_tag"},
            "unlock": {"types": ["dict", "list"], "structure": "recipe:unlock_group"}
