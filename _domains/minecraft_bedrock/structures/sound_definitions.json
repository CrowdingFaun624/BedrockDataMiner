{
    "group_aliases": {
        "temp": "structures/template"
    },
    "base": {
        "type": "StructureBase",
        "delegate_arguments": {
            "name": "sound_definitions"
        },
        "this_types": "dict",
        "structure": "cache"
    },
    "cache": {
        "type": "Cache",
        "this_types": "dict",
        "structure": {
            "type": "Normalizer",
            "outer_types": "dict",
            "inner_types": "fake_file",
            "functions": {
                "function_name": "normalizers/normalizers normalize_sound_definitions"
            },
            "structure": {
                "type": "File",
                "serializer": null,
                "outer_types": "fake_file",
                "inner_types": "dict",
                "structure": "sound_event"
            }
        }
    },
    "sound_event": {
        "type": "Dict",
        "delegate_arguments": {
            "field": "sound event",
            "sort": "sort_by_key",
            "measure_length": true
        },
        "value_types": "dict",
        "key_structure": {
            "type": "String",
            "similarity_weight_function": {
                "function_name": "normalizers/normalizers sound_events_similarity_weight"
            }
        },
        "min_key_similarity_threshold": 0.1,
        "value_structure": "resource_pack"
    },
    "resource_pack": {
        "type": "Normalizer",
        "outer_types": "dict",
        "inner_types": "dict",
        "functions": {
            "function_name": "normalizers/collapse_resource_packs collapse_resource_packs_dict"
        },
        "structure": {
            "type": "Dict",
            "delegate_arguments": {
                "field": "resource pack",
                "sort": "sort_by_key",
                "measure_length": true
            },
            "key_types": "tuple",
            "key_structure": "temp/defined_in",
            "value_types": "dict",
            "value_structure": "sound_event_property"
        }
    },
    "sound_event_property": {
        "type": "Normalizer",
        "outer_types": "dict",
        "inner_types": "dict",
        "functions": {
            // https://bugs.mojang.com/browse/MCPE-153558
            // https://bugs.mojang.com/browse/MCPE-178265
            "function_name": "delete_keys",
            "arguments": {
                "keys": ["pitch", "volume"]
            }
        },
        "structure": {
            "type": "Keymap",
            "delegate_arguments": {
                "sort": "sort_by_component_order",
                "field": "property"
            },
            "keys": {
                "category": {"types": "str"},
                "max_distance": {"types": ["float", "int"]},
                "min_distance": {"types": "float"},
                "subtitle": {"types": "str"},
                "sounds": {"types": "list", "structure": "sounds"}
            }
        }
    },
    "sounds": {
        "type": "Dict",
        "key_weight": 0,
        "key_types": "int",
        "this_types": "list",
        "delegate_arguments": {
            "field": "sound",
            "show_item_key": false,
            "print_all": true,
            "measure_length": true
        },
        "value_types": ["dict", "str"],
        "min_value_similarity_threshold": 0.3,
        "value_structure": "sound_properties"
    },
    "sound_properties": {
        "type": "Normalizer",
        "outer_types": ["str", "dict"],
        "inner_types": "dict",
        "functions": [
            {
                "function_name": "normalizers/normalizers sound_definitions_make_strings_to_dict"
            },
            {
                // https://bugs.mojang.com/browse/MCPE-153561
                "function_name": "delete_key",
                "arguments": {
                    "key": "pitch:"
                }
            }
        ],
        "structure": {
            "type": "Keymap",
            "delegate_arguments": {
                "sort": "sort_by_component_order",
                "field": "property"
            },
            "keys": {
                "name": {"types": "str", "structure": {
                    "type": "String"
                }, "similarity_weight": 10, "required": true, "delegate_arguments": {"always_print": true}},
                "type": {"types": "str", "delegate_arguments": {"always_print": true}},
                "exclude_from_pocket_platforms": {"types": "bool"},
                "is3D": {"types": "bool"},
                "load_on_low_memory": {"types": "bool"},
                "pitch": {"types": ["float", "int"]},
                "stream": {"types": "bool"},
                "volume": {"types": ["float", "int"]},
                "weight": {"types": "int"}
            }
        }
    }
}