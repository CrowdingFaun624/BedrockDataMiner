{
    "group_aliases": {
        "s": "serializers",
        "temp": "structures/template"
    },
    "base": {
        "inherit": "temp/base_single_file",
        "$name": "tiers_bin",
        "$serializer": "#@s/tiers_bin_serializer",
        "$structure": "#@tiers_bin"
    },
    "tiers_bin": {
        "type": "Keymap",
        "keys": {
            "gpu": {"types": "dict", "structure": "gpus"}
        }
    },
    "gpus": {
        "type": "Dict",
        "key_structure": {
            "type": "String"
        },
        "min_key_similarity_threshold": 0.3,
        "value_types": ["int", "dict"],
        "value_structure": "gpu",
        "delegate_arguments": {
            "field": "gpu",
            "measure_length": true
        }
    },
    "gpu": {
        "type": "Normalizer",
        "outer_types": ["int", "dict"],
        "inner_types": "dict",
        "functions": {
            "function_name": "normalizers/normalizers tiers_bin_normalize_gpu"
        },
        "structure": {
            "type": "Keymap",
            "keys": {
                "tier": {"types": "int"},
                "hash": {"types": "int"}
            }
        }
    }
}