{
    "base": {
        "type": "Base",
        "delegate_arguments": {
            "name": "sound_files"
        },
        "types": "dict",
        "subcomponent": "sound_file"
    },
    "sound_file": {
        "type": "Dict",
        "delegate_arguments": {
            "field": "sound file",
            "measure_length": true
        },
        "types": ["dict"],
        "detect_key_moves": true,
        "sort": "by_key",
        "min_value_similarity_threshold": 0.9,
        "subcomponent": "internal_sound_file"
    },
    "internal_sound_file": {
        "type": "Dict",
        "delegate_arguments": {
            "field": "internal sound file",
            "measure_length": true
        },
        "types": ["dict"],
        "detect_key_moves": true,
        "sort": "by_key",
        "subcomponent": "sound_property"
    },
    "sound_property": {
        "type": "Keymap",
        "delegate_arguments": {
            "field": "property"
        },
        "normalizer": {
            "function_name": "delete_required_keys",
            "arguments": {
                "keys": ["_obj", "filepath"]
            }
        },
        "sort": "by_component_order",
        "keys": {
            "sha1_hash": {"types": "str", "weight": 16, "delegate_arguments": {"always_print": true}, "required": true, "tags": ["file_hash_tag"]},
            "filesize": {"types": "int"},
            "pictures": {"types": "list", "subcomponent": "pictures"},
            "tags": {"types": "dict", "subcomponent": "tags"},
            "_subchunks": {"types": "list", "subcomponent": "subchunks"},
            "streaminfo": {"types": "dict", "subcomponent": "streaminfo"}
        }
    },
    "pictures": {
        "type": "Set",
        "delegate_arguments": {
            "field": "picture",
            "measure_length": true
        },
        "types": ["null"],
        "subcomponent": null
    },
    "tags": {
        "type": "Keymap",
        "delegate_arguments": {
            "field": "tag type"
        },
        "sort": "by_key",
        "keys": {
            "comment": {"types": "list", "subcomponent": "tag"},
            "encoder": {"types": "list", "subcomponent": "tag"},
            "title": {"types": "list", "subcomponent": "tag"},
            "tracknumber": {"types": "list", "subcomponent": "tag"}
        }
    },
    "tag": {
        "type": "Set",
        "delegate_arguments": {
            "field": "tag"
        },
        "types": ["str"],
        "sort": true,
        "subcomponent": null
    },
    "subchunks": {
        "type": "Set",
        "types": ["null"],
        "subcomponent": null,
        "delegate_arguments": {
            "measure_length": true
        }
    },
    "streaminfo": {
        "type": "Keymap",
        "delegate_arguments": {
            "field": "property"
        },
        "sort": "by_key",
        "keys": {
            "_start": {"types": "int"},
            "_size": {"types": "int"},
            "_version": {"types": "list", "subcomponent": "version"},
            "_extension_data": {"types": "null"},
            "audio_format": {"types": "str"},
            "bit_depth": {"types": "int"},
            "bitrate": {"types": ["float", "int"]},
            "channels": {"types": "int"},
            "duration": {"types": "float"},
            "max_bitrate": {"types": "int"},
            "min_bitrate": {"types": "int"},
            "nominal_bitrate": {"types": "int"},
            "sample_rate": {"types": "int"}
        }
    },
    "version": {
        "type": "List",
        "delegate_arguments": {
            "field": "version number",
            "print_flat": true
        },
        "types": ["int"],
        "subcomponent": null
    }
}