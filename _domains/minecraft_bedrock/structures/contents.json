{
    "base": {
        "type": "Base",
        "delegate_arguments": {
            "name": "contents"
        },
        "types": "dict",
        "subcomponent": "packs"
    },
    "packs": {
        "type": "Dict",
        "types": "file",
        "delegate_arguments": {
            "field": "pack"
        },
        "sort": "by_key",
        "subcomponent": "cache"
    },
    "cache": {
        "type": "Cache",
        "types": "file",
        "subcomponent": {
            "type": "File",
            "file_types": "file",
            "content_types": "dict",
            "subcomponent": "file"
        }
    },
    "file": {
        "type": "Keymap",
        "keys": {
            "content": {"types": "list", "subcomponent": "content"}
        }
    },
    "content": {
        "type": "Set",
        "types": "dict",
        "subcomponent": "content.item"
    },
    "content.item": {
        "type": "Keymap",
        "keys": {
            "path": {"types": "str", "subcomponent": {
                "type": "String"
            }}
        }
    }
}