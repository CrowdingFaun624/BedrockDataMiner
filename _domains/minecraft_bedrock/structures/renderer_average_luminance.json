{
    "base": {
        "type": "Base",
        "delegate_arguments": {
            "name": "renderer_average_luminance"
        },
        "types": "file",
        "subcomponent": "cache"
    },
    "cache": {
        "type": "Cache",
        "types": "file",
        "subcomponent": {
            "type": "File",
            "file_types": "file",
            "content_types": "dict",
            "subcomponent": "average_luminance"
        }
    },
    "average_luminance": {
        "type": "Keymap",
        "sort": "by_key",
        "keys": {
            "min_log_luminance": {"types": "float"},
            "log_luminance_range": {"types": ["float", "int"]},
            "adaptive_time_coefficient": {"types": "float"},
            "light_adaptation": {"types": "float"},
            "dark_adaptation": {"types": "float"},
            "enable_custom_weight": {"types": "bool"},
            "weight_curve_key_values": {"types": "dict", "subcomponent": "weight_curve_key_values"}
        }
    },
    "weight_curve_key_values": {
        "type": "Dict",
        "delegate_arguments": {
            "field": "value",
            "measure_length": true
        },
        "types": ["float"],
        "detect_key_moves": true,
        "sort": "by_key",
        "subcomponent": null
    }
}