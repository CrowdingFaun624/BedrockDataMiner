{
    "group_aliases": {
        "s": "serializers",
        "t": "structure_tags",
        "temp": "structures/template"
    },
    "base": {
        "inherit": "temp/base_pack_files",
        "$name": "font_metadata",
        "$pack_type": "resource pack",
        "$structure": "#@font_metadata"
    },
    "font_metadata": {
        "type": "Keymap",
        "delegate_arguments": {
            "sort": "sort_by_component_order"
        },
        "keys": {
            "version": {"types": "int"},
            "font_aliases": {"types": "list", "structure": "font_aliases"},
            "fonts": {"types": "list", "structure": "fonts"}
        }
    },
    "fonts": {
        "type": "Normalizer",
        "outer_types": "list",
        "inner_types": "dict",
        "functions": {
            "function_name": "wrap_in_dict",
            "arguments": {
                "key": "font_name"
            }
        },
        "structure": {
            "type": "Dict",
            "delegate_arguments": {
                "field": "font",
                "sort": "sort_by_key",
                "measure_length": true
            },
            "value_types": "dict",
            "value_structure": "font"
        }
    },
    "font": {
        "type": "Keymap",
        "keys": {
            "font_file": {"types": "str", "delegate_arguments": {"always_print": true}},
            "font_format": {"types": "str"},
            "font_name": {"types": "str", "delegate_arguments": {"always_print": true}},
            "lowPerformanceCompatible": {"types": "bool"},
            "version": {"types": "int"}
        }
    },
    "font_aliases": {
        "type": "Normalizer",
        "outer_types": "list",
        "inner_types": "dict",
        "functions": {
            "function_name": "wrap_in_dict",
            "arguments": {
                "key": "alias"
            }
        },
        "structure": {
            "type": "Dict",
            "delegate_arguments": {
                "field": "font alias",
                "sort": "sort_by_key",
                "measure_length": true
            },
            "value_types": "dict",
            "value_structure": "font_alias"
        }
    },
    "font_alias": {
        "type": "Keymap",
        "keys": {
            "alias": {"types": "str", "delegate_arguments": {"always_print": true}},
            "font_scale_factor": {"types": "int", "tags": ["t/outdated"]},
            "font_scale_factor_override": {"types": "int"},
            "fonts": {"types": "list", "structure": "font_references"}
        }
    },
    "font_references": {
        "type": "Normalizer",
        "outer_types": "list",
        "inner_types": "dict",
        "functions": {
            "function_name": "wrap_in_dict",
            "arguments": {
                "key": "font_reference"
            }
        },
        "structure": {
            "type": "Dict",
            "delegate_arguments": {
                "field": "font reference",
                "sort": "sort_by_key",
                "measure_length": true
            },
            "value_types": "dict",
            "value_structure": "font_reference"
        }
    },
    "font_reference": {
        "type": "Keymap",
        "keys": {
            "font_reference": {"types": "str"},
            "font_scale": {"types": "float"},
            "font_ranges": {"types": "list", "structure": "font_ranges"}
        }
    },
    "font_ranges": {
        "type": "Dict",
        "allow_key_moves": false,
        "key_weight": 0,
        "key_types": "int",
        "this_types": "list",
        "delegate_arguments": {
            "field": "range",
            "measure_length": true
        },
        "value_types": "dict",
        "value_structure": "font_range"
    },
    "font_range": {
        "type": "Keymap",
        "delegate_arguments": {
            "sort": "sort_by_component_order",
            "print_all": true
        },
        "keys": {
            "first": {"types": "int"},
            "last": {"types": "int"}
        }
    }
}