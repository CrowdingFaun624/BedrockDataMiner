{
    "group_aliases": {
        "s": "serializers"
    },
    "base": {
        "type": "StructureBase",
        "delegate_arguments": {
            "name": "skin_models"
        },
        "this_types": "dict",
        "structure": "cache"
    },
    "cache": {
        "type": "Cache",
        "this_types": "dict",
        "structure": {
            "type": "Condition",
            "substructures": [
                {"types": "dict", "filter": {"type": "EqFilter", "key": "needs_repair", "value": false, "default": true}, "structure": "#!file{serializer=@minecraft_common!s/json}"},
                {"types": "dict", "filter": {"type": "EqFilter", "key": "needs_repair", "value": true}, "structure": "#!file{serializer=@s/skin_geometries_repairable_json}"}
            ]
        }
    },
    "file": {
        "type": "Normalizer",
        "outer_types": "dict",
        "inner_types": "fake_file",
        "functions": {
            "function_name": "normalizers/models_model_normalize",
            "arguments": {
                "serializer": "#serializer"
            }
        },
        "structure": {
            "type": "File",
            "serializer": null,
            "outer_types": "fake_file",
            "inner_types": "dict",
            "structure": "files"
        }
    },
    "files": {
        "type": "Dict",
        "delegate_arguments": {
            "field": "file",
            "sort": "sort_by_key",
            "measure_length": true
        },
        "value_types": "dict",
        "key_structure": {
            "type": "String"
        },
        "min_key_similarity_threshold": 0.9,
        "value_structure": "skin_packs"
    },
    "skin_packs": {
        "type": "Dict",
        "delegate_arguments": {
            "field": "skin pack",
            "sort": "sort_by_key",
            "measure_length": true
        },
        "value_types": "dict",
        // cannot have collapse_resource_packs normalizer
        "value_structure": "structures/models/model"
    }
}