{
    "group_aliases": {
        "s": "serializers",
        "temp": "structures/template"
    },
    "base": {
        "inherit": "temp/base_multiple_files",
        "$name": "personas",
        "$file_field": "persona",
        "$types": ["dict", "list"],
        "$structure": "#@persona"
    },
    "color_list": {
        "type": "Dict",
        "allow_key_moves": false,
        "key_weight": 0,
        "key_types": "int",
        "this_types": "list",
        "delegate_arguments": {
            "field": "color",
            "print_all": true,
            "print_flat": true,
            "measure_length": false
        },
        "value_types": "str",
        "value_structure": null
    },
    "persona": {
        "type": "Union",
        "substructures": {
            "dict": {
                "type": "Keymap",
                "keys": {
                    "appearance": {"types": "list", "structure": "appearance"},
                    "name": {"types": "str", "structure": {"type": "String"}, "delegate_arguments": {"always_print": true}},
                    "thumbnailPath": {"types": "str"},
                    "uuid": {"types": "str"}
                }
            },
            "list": "appearance"
        }
    },
    "appearance": {
        "type": "Dict",
        "key_weight": 0,
        "key_types": "int",
        "this_types": "list",
        "delegate_arguments": {
            "field": "appearance",
            "show_item_key": false,
            "measure_length": true
        },
        "value_types": "dict",
        "value_structure": {
            "type": "Keymap",
            "keys": {
                "arm": {"types": "str"},
                "col": {"types": "list", "structure": "color_list"},
                "id": {"types": "str"},
                "skcol": {"types": "str"},
                "skin": {"types": "bool"}
            }
        }
    }
}