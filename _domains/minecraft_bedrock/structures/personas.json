{
    "base": {
        "type": "StructureBase",
        "delegate_arguments": {
            "name": "personas"
        },
        "this_types": "dict",
        "structure": "personas"
    },
    "color_list": {
        "type": "Dict",
        "allow_key_moves": false,
        "key_weight": 0,
        "key_types": "int",
        "this_types": "list",
        "delegate_arguments": {
            "field": "color",
            "print_all": true,
            "print_flat": true,
            "measure_length": false
        },
        "value_types": ["str"],
        "value_structure": null
    },
    "personas": {
        "type": "Dict",
        "delegate_arguments": {
            "field": "persona",
            "sort": "sort_by_key",
            "measure_length": true
        },
        "value_types": ["dict", "list"],
        "value_structure": "cache"
    },
    "cache": {
        "type": "Cache",
        "this_types": ["dict", "list"],
        "pre_normalized_types": "file",
        "structure": {
            "type": "File",
            "serializer": "minecraft_common!json",
            "file_types": "file",
            "content_types": ["dict", "list"],
            "structure": "persona"
        }
    },
    "persona": {
        "type": "Union",
        "substructures": {
            "dict": {
                "type": "Keymap",
                "keys": {
                    "appearance": {"types": "list", "structure": "appearance"},
                    "name": {"types": "str", "structure": {"type": "String"}, "delegate_arguments": {"always_print": true}},
                    "thumbnailPath": {"types": "str"},
                    "uuid": {"types": "str"}
                }
            },
            "list": "appearance"
        }
    },
    "appearance": {
        "type": "Dict",
        "key_weight": 0,
        "key_types": "int",
        "this_types": "list",
        "delegate_arguments": {
            "field": "appearance",
            "show_item_key": false,
            "measure_length": true
        },
        "value_types": ["dict"],
        "value_structure": {
            "type": "Keymap",
            "keys": {
                "arm": {"types": "str"},
                "col": {"types": "list", "structure": "color_list"},
                "id": {"types": "str"},
                "skcol": {"types": "str"},
                "skin": {"types": "bool"}
            }
        }
    }
}