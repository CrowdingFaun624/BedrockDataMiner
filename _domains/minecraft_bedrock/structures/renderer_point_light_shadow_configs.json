{
    "base": {
        "type": "Base",
        "delegate_arguments": {
            "name": "renderer_point_light_shadow_configs"
        },
        "types": "dict",
        "subcomponent": "files"
    },
    "files": {
        "type": "Dict",
        "delegate_arguments": {
            "field": "file",
            "measure_length": true
        },
        "types": ["file"],
        "detect_key_moves": true,
        "sort": "by_key",
        "subcomponent": "cache"
    },
    "cache": {
        "type": "Cache",
        "types": "file",
        "subcomponent": {
            "type": "File",
            "file_types": "file",
            "content_types": "dict",
            "subcomponent": "file"
        }
    },
    "file": {
        "type": "Keymap",
        "sort": "by_component_order",
        "imports": "point_light_shadow_config",
        "keys": {
            "low": {"types": "dict", "subcomponent": "point_light_shadow_config"},
            "medium": {"types": "dict", "subcomponent": "point_light_shadow_config"},
            "high": {"types": "dict", "subcomponent": "point_light_shadow_config"},
            "ultra": {"types": "dict", "subcomponent": "point_light_shadow_config"}
        }
    },
    "point_light_shadow_config": {
        "type": "Keymap",
        "sort": "by_key",
        "keys": {
            "file": {"types": "str", "subcomponent": {"type": "String"}, "delegate_arguments": {"always_print": true}},
            "cached_shadows": {"types": "bool"},
            "bias": {"types": "float"},
            "slope_bias": {"types": "float"},
            "far_plane": {"types": "float"},
            "near_plane": {"types": "float"},
            "max_point_light_shadows": {"types": "int"},
            "resolution": {"types": "int"}
        }
    }
}