{
    "base": {
        "type": "Base",
        "delegate_arguments": {
            "name": "renderer_tone_mapping"
        },
        "types": "file",
        "subcomponent": "cache"
    },
    "cache": {
        "type": "Cache",
        "types": "file",
        "subcomponent": {
            "type": "File",
            "file_types": "file",
            "content_types": "dict",
            "subcomponent": "tone_mapping"
        }
    },
    "tone_mapping": {
        "type": "Keymap",
        "sort": "by_key",
        "keys": {
            "brightness": {"types": "int"},
            "contrast": {"types": "int"},
            "custom_exposure_compensation": {"types": "dict", "subcomponent": "custom_exposure_compensation"},
            "exposure_compensation": {"types": "float"},
            "exposure_compensation_type": {"types": "str"},
            "f_stop": {"types": "float"},
            "is_auto_exposure_compensation": {"types": "bool", "tags": ["outdated"]},
            "max_luminance": {"types": "float"},
            "min_luminance": {"types": "float"},
            "min_white_point": {"types": "float"},
            "saturation": {"types": "int"},
            "technique": {"types": "str"},
            "whitepoint": {"types": "int", "tags": ["outdated"]}
        }
    },
    "custom_exposure_compensation": {
        "type": "Dict",
        "delegate_arguments": {
            "field": "value",
            "measure_length": true
        },
        "types": ["float"],
        "detect_key_moves": true,
        "sort": "by_key",
        "subcomponent": null
    }
}