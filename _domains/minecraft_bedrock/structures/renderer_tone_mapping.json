{
    "base": {
        "type": "StructureBase",
        "delegate_arguments": {
            "name": "renderer_tone_mapping"
        },
        "this_types": "dict",
        "pre_normalized_types": "file",
        "structure": "cache"
    },
    "cache": {
        "type": "Cache",
        "this_types": "dict",
        "pre_normalized_types": "file",
        "structure": {
            "type": "File",
            "serializer": "minecraft_common!json",
            "file_types": "file",
            "content_types": "dict",
            "structure": "tone_mapping"
        }
    },
    "tone_mapping": {
        "type": "Keymap",
        "delegate_arguments": {
            "sort": "sort_by_key"
        },
        "keys": {
            "brightness": {"types": "int"},
            "contrast": {"types": "int"},
            "custom_exposure_compensation": {"types": "dict", "structure": "custom_exposure_compensation"},
            "exposure_compensation": {"types": "float"},
            "exposure_compensation_type": {"types": "str"},
            "f_stop": {"types": "float"},
            "is_auto_exposure_compensation": {"types": "bool", "tags": ["outdated"]},
            "max_luminance": {"types": "float"},
            "min_luminance": {"types": "float"},
            "min_white_point": {"types": "float"},
            "saturation": {"types": "int"},
            "technique": {"types": "str"},
            "whitepoint": {"types": "int", "tags": ["outdated"]}
        }
    },
    "custom_exposure_compensation": {
        "type": "Dict",
        "delegate_arguments": {
            "field": "value",
            "sort": "sort_by_key",
            "measure_length": true
        },
        "value_types": ["float"],
        "value_structure": null
    }
