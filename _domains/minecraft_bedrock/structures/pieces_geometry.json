{
    "base": {
        "type": "Base",
        "delegate_arguments": {
            "name": "pieces_geometry"
        },
        "imports": [
            {
                "from": "structures/models",
                "components": [
                    {
                        "component": "geometry"
                    }
                ]
            }
        ],
        "types": "dict",
        "subcomponent": "pieces"
    },
    "pieces": {
        "type": "Dict",
        "types": "dict",
        "subcomponent": "geometry_files",
        "sort": "by_key",
        "delegate_arguments": {
            "field": "piece"
        }
    },
    "geometry_files": {
        "type": "Dict",
        "types": "file",
        "subcomponent": "cache",
        "sort": "by_key",
        "delegate_arguments": {
            "field": "geometry file"
        }
    },
    "cache": {
        "type": "Cache",
        "types": "file",
        "subcomponent": {
            "type": "File",
            "file_types": "file",
            "content_types": "dict",
            "subcomponent": "geometries"
        }
    },
    "geometries": {
        "type": "Dict",
        "sort": "by_key",
        "normalizer": {
            "function_name": "delete_optional_key",
            "arguments": {
                "key": "format_version"
            }
        },
        "types": "dict",
        "delegate_arguments": {
            "measure_length": true,
            "field": "geometry"
        },
        "subcomponent": "geometry"
    }
}