{
    "base": {
        "type": "Base",
        "delegate_arguments": {
            "name": "renderer_bloom_configs"
        },
        "types": "dict",
        "subcomponent": "files"
    },
    "files": {
        "type": "Dict",
        "delegate_arguments": {
            "field": "file",
            "measure_length": true
        },
        "types": ["file"],
        "detect_key_moves": true,
        "sort": "by_key",
        "subcomponent": "cache"
    },
    "cache": {
        "type": "Cache",
        "types": "file",
        "subcomponent": {
            "type": "File",
            "file_types": "file",
            "content_types": "dict",
            "subcomponent": "bloom_config"
        }
    },
    "bloom_config": {
        "type": "Keymap",
        "sort": "by_key",
        "keys": {
            "file": {"types": "str", "subcomponent": {"type": "String"}, "delegate_arguments": {"always_print": true}},
            "depth_scaling_factor": {"types": "int"},
            "enable_depth_scaling": {"types": "bool"},
            "intensity": {"types": "float"},
            "maximum_depth_range": {"types": "float"},
            "minimum_depth_multiplier": {"types": "int"},
            "minimum_depth_range": {"types": "float"},
            "threshold": {"types": "float", "tags": ["outdated"]},
            "toggle_default": {"types": "bool"}
        }
    }
}