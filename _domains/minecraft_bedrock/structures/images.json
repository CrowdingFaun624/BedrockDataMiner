{
    "group_aliases": {
        "s": "serializers",
        "temp": "structures/template"
    },
    "base": {
        "inherit": "temp/base_multiple_files",
        "$name": "images",
        "$structure": "#@images"
    },
    "images": {
        "type": "Keymap",
        "delegate_arguments": {
            "sort": "sort_by_component_order"
        },
        "keys": {
            "bilinear": {"types": "bool"},
            "textures": {"types": ["list", "dict"], "structure": "textures"}
        }
    },
    "textures": {
        "type": "Union",
        "substructures": {
            "list": {
                "type": "Dict",
                "key_weight": 0,
                "key_types": "int",
                "this_types": "list",
                "value_types": ["str", "dict"],
                "value_structure": "texture",
                "delegate_arguments": {
                    "field": "texture",
                    "show_item_key": false,
                    "measure_length": true
                }
            },
            "dict": {
                "type": "Dict",
                "value_types": ["str", "dict"],
                "value_structure": "texture",
                "key_structure": {
                    "type": "String"
                },
                "delegate_arguments": {
                    "sort": "sort_by_key",
                    "field": "texture",
                    "measure_length": true
                }
            }
        }
    },
    "texture": {
        "type": "Union",
        "substructures": {
            "str": {"type": "String"},
            "dict": {
                "type": "Keymap",
                "keys": {
                    "texture": {"types": "str", "structure": {"type": "String"}, "similarity_weight": 8, "delegate_arguments": {"always_print": true}},
                    "bilinear": {"types": "bool"},
                    "wrap": {"types": "bool"}
                }
            }
        }
    }
}