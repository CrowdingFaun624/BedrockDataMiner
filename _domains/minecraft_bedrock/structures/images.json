{
    "base": {
        "type": "StructureBase",
        "delegate_arguments": {
            "name": "images"
        },
        "this_types": "dict",
        "structure": "files"
    },
    "files": {
        "type": "Dict",
        "value_types": "dict",
        "value_structure": "cache",
        "delegate_arguments": {
            "sort": "sort_by_key",
            "field": "file",
            "measure_length": true
        }
    },
    "cache": {
        "type": "Cache",
        "this_types": "dict",
        "pre_normalized_types": "file",
        "structure": {
            "type": "File",
            "serializer": "minecraft_common!json",
            "file_types": "file",
            "content_types": "dict",
            "structure": "images"
        }
    },
    "images": {
        "type": "Keymap",
        "delegate_arguments": {
            "sort": "sort_by_component_order"
        },
        "keys": {
            "bilinear": {"types": "bool"},
            "textures": {"types": "list", "structure": "textures"}
        }
    },
    "textures": {
        "type": "Dict",
        "key_weight": 0,
        "key_types": "int",
        "this_types": "list",
        "value_types": ["str", "dict"],
        "value_structure": "texture",
        "delegate_arguments": {
            "field": "texture",
            "show_item_key": false,
            "measure_length": true
        }
    },
    "texture": {
        "type": "Union",
        "substructures": {
            "str": {"type": "String"},
            "dict": {
                "type": "Keymap",
                "keys": {
                    "texture": {"types": "str", "structure": {"type": "String"}, "similarity_weight": 8, "delegate_arguments": {"always_print": true}},
                    "bilinear": {"types": "bool"},
                    "wrap": {"types": "bool"}
                }
            }
        }
    }
}