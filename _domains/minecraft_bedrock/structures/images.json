{
    "base": {
        "type": "Base",
        "delegate_arguments": {
            "name": "images"
        },
        "types": "dict",
        "subcomponent": "files"
    },
    "files": {
        "type": "Dict",
        "types": "file",
        "subcomponent": "cache",
        "sort": "by_key",
        "delegate_arguments": {
            "field": "file",
            "measure_length": true
        }
    },
    "cache": {
        "type": "Cache",
        "types": "file",
        "subcomponent": {
            "type": "File",
            "file_types": "file",
            "content_types": "dict",
            "subcomponent": "images"
        }
    },
    "images": {
        "type": "Keymap",
        "sort": "by_component_order",
        "keys": {
            "bilinear": {"types": "bool"},
            "textures": {"types": ["list", "dict"], "subcomponent": "textures"}
        }
    },
    "textures": {
        "type": "Union",
        "subcomponents": {
            "list": {
                "type": "Set",
                "types": ["str", "dict"],
                "subcomponent": "texture",
                "delegate_arguments": {
                    "field": "texture",
                    "measure_length": true
                }
            },
            "dict": {
                "type": "Dict",
                "types": ["str", "dict"],
                "subcomponent": "texture",
                "sort": "by_key",
                "key_component": {
                    "type": "String"
                },
                "delegate_arguments": {
                    "field": "texture",
                    "measure_length": true
                }
            }
        }
    },
    "texture": {
        "type": "Union",
        "subcomponents": {
            "str": {"type": "String"},
            "dict": {
                "type": "Keymap",
                "keys": {
                    "texture": {"types": "str", "subcomponent": {"type": "String"}, "weight": 8, "delegate_arguments": {"always_print": true}},
                    "bilinear": {"types": "bool"},
                    "wrap": {"types": "bool"}
                }
            }
        }
    }
}