{
    "group_aliases": {
        "s": "serializers",
        "temp": "structures/template"
    },
    "base": {
        "inherit": "temp/base_multiple_pack_files",
        "$name": "jigsaw_structure_sets",
        "$files_key_structure": "temp/string",
        "$pack_type": "behavior pack",
        "$structure": "#@file"
    },
    "file": {
        "type": "Keymap",
        "delegate_arguments": {
            "sort": "sort_by_component_order"
        },
        "keys": {
            "format_version": {"types": "str", "similarity_weight": 0},
            "minecraft:structure_set": {"types": "dict", "structure": "structure_set"}
        }
    },
    "structure_set": {
        "type": "Keymap",
        "delegate_arguments": {
            "sort": "sort_by_component_order"
        },
        "keys": {
            "description": {"types": "dict", "structure": "description", "delegate_arguments": {"always_print": true}},
            "placement": {"types": "dict", "structure": "placement"},
            "structures": {"types": "list", "structure": "structures"}
        }
    },
    "description": {
        "type": "Keymap",
        "delegate_arguments": {
            "sort": "sort_by_component_order"
        },
        "keys": {
            "identifier": {"types": "str", "structure": {"type": "String"}, "delegate_arguments": {"always_print": true}}
        }
    },
    "structures": {
        "type": "Dict",
        "key_weight": 0,
        "key_types": "int",
        "this_types": "list",
        "value_types": "dict",
        "delegate_arguments": {
            "field": "structure",
            "show_item_key": false,
            "measure_length": true
        },
        "value_structure": "structure"
    },
    "structure": {
        "type": "Keymap",
        "delegate_arguments": {
            "sort": "sort_by_component_order"
        },
        "keys": {
            "structure": {"types": "str", "structure": {"type": "String"}, "delegate_arguments": {"always_print": true}},
            "weight": {"types": "int"}
        }
    },
    "placement": {
        "type": "Normalizer",
        "outer_types": "dict",
        "inner_types": "dict",
        "functions": {
            "function_name": "wrap_in_value",
            "arguments": {
                "key": "type"
            }
        },
        "structure": {
            "type": "Keymap",
            "delegate_arguments": {
                "passthrough": true
            },
            "keys": {
                "minecraft:random_spread": {"types": "dict", "structure": "placement.random_spread"}
            }
        }
    },
    "placement.random_spread": {
        "type": "Keymap",
        "delegate_arguments": {
            "sort": "sort_by_component_order"
        },
        "keys": {
            "type": {"types": "str"},
            "salt": {"types": "int"},
            "separation": {"types": "int"},
            "spacing": {"types": "int"},
            "spread_type": {"types": "str"}
        }
    }
}