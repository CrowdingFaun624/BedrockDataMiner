{
    "base": {
        "type": "Base",
        "delegate_arguments": {
            "name": "bootstrap"
        },
        "types": "file",
        "subcomponent": "cache"
    },
    "cache": {
        "type": "Cache",
        "types": "file",
        "subcomponent": {
            "type": "File",
            "file_types": "file",
            "content_types": "dict",
            "subcomponent": "bootstrap"
        }
    },
    "bootstrap": {
        "type": "Keymap",
        "keys": {
            "profile": {"types": ["dict", "list"], "subcomponent": {
                "type": "Union",
                "subcomponents": {
                    "dict": "profile",
                    "list": {
                        "type": "List",
                        "types": "null",
                        "subcomponent": null
                    }
                }
            }}
        }
    },
    "profile": {
        "type": "Keymap",
        "keys": {
            "enabled_groups": {"types": "list", "subcomponent": "group_list"},
            "min_group_stack_depth": {"types": "int"},
            "headers_enabled": {"types": "bool"}
        }
    },
    "group_list": {
        "type": "Set",
        "types": "str",
        "subcomponent": {
            "type": "String"
        },
        "delegate_arguments": {
            "field": "group"
        }
    }
}