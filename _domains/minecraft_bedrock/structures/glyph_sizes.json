{
    "base": {
        "type": "StructureBase",
        "delegate_arguments": {
            "name": "glyph_sizes"
        },
        "this_types": "dict",
        "pre_normalized_types": "file",
        "structure": "cache"
    },
    "cache": {
        "type": "Cache",
        "this_types": "dict",
        "pre_normalized_types": "file",
        "structure": {
            "type": "File",
            "serializer": "glyph_sizes_serializer",
            "file_types": "file",
            "content_types": "dict",
            "structure": "file"
        }
    },
    "file": {
        "type": "Dict",
        "delegate_arguments": {
            "measure_length": true,
            "sort": "sort_by_key",
            "field": "codepoint"
        },
        "value_types": "dict",
        "allow_key_moves": false,
        "value_structure": "range"
    },
    "range": {
        "type": "Keymap",
        "delegate_arguments": {
            "sort": "sort_by_component_order",
            "field": "end"
        },
        "keys": {
            "character": {"types": "str", "delegate_arguments": {"always_print": true}},
            "left": {"types": "int"},
            "right": {"types": "int"}
        }
    }
}