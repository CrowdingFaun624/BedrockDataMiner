{
    "group_aliases": {
        "s": "serializers",
        "temp": "structures/template"
    },
    "base": {
        "inherit": "temp/base_single_file",
        "$name": "glyph_sizes",
        "$serializer": "#@s/glyph_sizes_serializer",
        "$structure": "#@file"
    },
    "file": {
        "type": "Dict",
        "delegate_arguments": {
            "measure_length": true,
            "sort": "sort_by_key",
            "field": "codepoint"
        },
        "value_types": "dict",
        "allow_key_moves": false,
        "value_structure": "range"
    },
    "range": {
        "type": "Keymap",
        "delegate_arguments": {
            "sort": "sort_by_component_order",
            "field": "end"
        },
        "keys": {
            "character": {"types": "str", "delegate_arguments": {"always_print": true}},
            "left": {"types": "int"},
            "right": {"types": "int"}
        }
    }
}