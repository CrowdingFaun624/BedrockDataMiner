{
    "group_aliases": {
        "s": "serializers",
        "temp": "structures/template"
    },
    "base": {
        "inherit": "temp/base_single_file",
        "$name": "renderer_material_index",
        "$structure": "#@material_index"
    },
    "material_index": {
        "type": "Keymap",
        "keys": {
            "materials": {"types": "list", "structure": "materials"}
        }
    },
    "materials": {
        "type": "Dict",
        "key_weight": 0,
        "key_types": "int",
        "this_types": "list",
        "delegate_arguments": {
            "field": "material",
            "show_item_key": false,
            "measure_length": true
        },
        "value_types": "dict",
        "value_structure": "material"
    },
    "material": {
        "type": "Keymap",
        "delegate_arguments": {
            "sort": "sort_by_component_order"
        },
        "keys": {
            "name": {"types": "str", "structure": {"type": "String"}, "delegate_arguments": {"always_print": true}},
            "path": {"types": "str", "structure": {"type": "String"}, "delegate_arguments": {"always_print": true}}
        }
    }
}