{
    "base": {
        "type": "Base",
        "delegate_arguments": {
            "name": "renderer_material_index"
        },
        "types": "file",
        "subcomponent": "cache"
    },
    "cache": {
        "type": "Cache",
        "types": "file",
        "subcomponent": {
            "type": "File",
            "file_types": "file",
            "content_types": "dict",
            "subcomponent": "material_index"
        }
    },
    "material_index": {
        "type": "Keymap",
        "keys": {
            "materials": {"types": "list", "subcomponent": "materials"}
        }
    },
    "materials": {
        "type": "Set",
        "delegate_arguments": {
            "field": "material",
            "measure_length": true
        },
        "types": ["dict"],
        "subcomponent": "material"
    },
    "material": {
        "type": "Keymap",
        "sort": "by_component_order",
        "keys": {
            "name": {"types": "str", "subcomponent": {"type": "String"}, "delegate_arguments": {"always_print": true}},
            "path": {"types": "str", "subcomponent": {"type": "String"}, "delegate_arguments": {"always_print": true}}
        }
    }
}