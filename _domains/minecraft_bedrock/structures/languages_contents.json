{
    "base": {
        "type": "Base",
        "delegate_arguments": {
            "name": "languages_contents"
        },
        "types": "dict",
        "subcomponent": "languages"
    },
    "languages": {
        "type": "Dict",
        "types": "fake_file",
        "sort": "by_key",
        "delegate_arguments": {
            "field": "language",
            "measure_length": true
        },
        "subcomponent": "cache"
    },
    "cache": {
        "type": "Cache",
        "types": "fake_file",
        "subcomponent": {
            "type": "File",
            "pre_normalized_types": "dict",
            "file_types": "fake_file",
            "garbage_collect": false,
            "content_types": "dict",
            "normalizer": {
                "function_name": "normalizers/collapse_resource_packs/dict_file",
                "arguments": {
                    "add_defined_in": false
                }
            },
            "subcomponent": "packs"
        }
    },
    "packs": {
        "type": "Dict",
        "delegate_arguments": {
            "field": "pack",
            "measure_length": true
        },
        "types": ["dict"],
        "sort": "by_key",
        "subcomponent": "keys"
    },
    "cache_keys": { // for other structures to use.
        "type": "Cache",
        "types": "file",
        "subcomponent": {
            "type": "File",
            "file_types": "file",
            "content_types": "dict",
            "subcomponent": "keys"
        }
    },
    "keys": {
        "type": "Dict",
        "delegate_arguments": {
            "field": "key",
            "measure_length": true
        },
        "types": ["dict"],
        "detect_key_moves": true,
        "sort": "by_key",
        "subcomponent": "properties"
    },
    "properties": {
        "type": "Keymap",
        "sort": "by_component_order",
        "keys": {
            "value": {"type": "str", "delegate_arguments": {"always_print": true}},
            "comment": {"type": "str"}
        }
    }
}