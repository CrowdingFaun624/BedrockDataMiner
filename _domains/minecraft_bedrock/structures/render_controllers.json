{
    "group_aliases": {
        "s": "serializers",
        "temp": "structures/template"
    },
    "base": {
        "inherit": "temp/base_multiple_pack_files",
        "$name": "render_controllers",
        "$pack_type": "resource pack",
        "$structure": "render_controllers_file"
    },
    "render_controllers_file": {
        "type": "Normalizer",
        "outer_types": "dict",
        "inner_types": "dict",
        "functions": {
            "function_name": "normalizers/normalizers render_controllers_fix_old"
        },
        "structure": {
            "type": "Keymap",
            "delegate_arguments": {
                "sort": "sort_by_component_order"
            },
            "keys": {
                "format_version": {"types": "str"},
                "render_controllers": {"types": "dict", "structure": "render_controllers"}
            }
        }
    },
    "render_controllers": {
        "type": "Dict",
        "delegate_arguments": {
            "field": "render controller",
            "sort": "sort_by_key",
            "measure_length": true
        },
        "value_types": "dict",
        "value_structure": "render_controller"
    },
    "color": {
        "type": "Keymap",
        "delegate_arguments": {
            "field": "channel"
        },
        "keys": {
            "a": {"types": ["float", "str"]},
            "r": {"types": ["float", "str"]},
            "g": {"types": ["float", "str"]},
            "b": {"types": ["float", "str"]}
        }
    },
    "render_controller": {
        "type": "Normalizer",
        "outer_types": "dict",
        "inner_types": "dict",
        "functions": {
            "function_name": "delete_key",
            "arguments": {
                "key": "texures"
            }
        },
        "structure": {
            "type": "Keymap",
            "keys": {
                "arrays": {"types": "dict", "structure": "arrays"},
                "color": {"types": "dict", "structure": "color"},
                "filter_lighting": {"types": "bool"},
                "geometry": {"types": "str"},
                "ignore_lighting": {"types": "bool"},
                "is_hurt_color": {"types": "dict", "structure": "color"},
                "light_color_multiplier": {"types": "float"},
                "materials": {"types": "list", "structure": "materials"},
                "on_fire_color": {"types": "dict", "structure": "color"},
                "overlay_color": {"types": "dict", "structure": "color"},
                "part_visibility": {"types": ["dict", "list"], "structure": "part_visibility.group"},
                "rebuild_animation_matrices": {"types": "bool"},
                "textures": {"types": "list", "structure": "textures"},
                "uv_anim": {"types": "dict", "structure": "uv_anim"}
            }
        }
    },
    "arrays": {
        "type": "Keymap",
        "keys": {
            "geometries": {"types": "dict", "structure": "array"},
            "materials": {"types": "dict", "structure": "array"},
            "textures": {"types": "dict", "structure": "array"}
        }
    },
    "array": {
        "type": "Dict",
        "delegate_arguments": {
            "field": "array",
            "sort": "sort_by_key",
            "measure_length": true
        },
        "value_types": "list",
        "value_structure": "array_list"
    },
    "array_list": {
        "type": "Dict",
        "key_weight": 0,
        "key_types": "int",
        "this_types": "list",
        "delegate_arguments": {
            "field": "array item",
            "show_item_key": false,
            "sort": "sort_by_value",
            "measure_length": true
        },
        "value_types": "str",
        "value_structure": null
    },
    "materials": {
        "type": "Dict",
        "key_weight": 0,
        "key_types": "int",
        "this_types": "list",
        "delegate_arguments": {
            "field": "material dict",
            "show_item_key": false,
            "measure_length": true
        },
        "value_types": "dict",
        "value_structure": "material_dict"
    },
    "material_dict": {
        "type": "Dict",
        "delegate_arguments": {
            "field": "material",
            "sort": "sort_by_key",
            "measure_length": true
        },
        "value_types": "str",
        "value_structure": null
    },
    "part_visibility.group": {
        "type": "Union",
        "substructures": {
            "dict": "part_visibility_dict",
            "list": "part_visibility"
        }
    },
    "part_visibility": {
        "type": "Dict",
        "key_weight": 0,
        "key_types": "int",
        "this_types": "list",
        "delegate_arguments": {
            "field": "part visibility dict",
            "show_item_key": false,
            "measure_length": true
        },
        "value_types": "dict",
        "min_value_similarity_threshold": 0.3,
        "value_structure": "part_visibility_dict"
    },
    "part_visibility_dict": {
        "type": "Dict",
        "delegate_arguments": {
            "field": "part visibility",
            "sort": "sort_by_key",
            "measure_length": true
        },
        "value_types": ["bool", "int", "str"],
        "min_key_similarity_threshold": 0.5,
        "min_value_similarity_threshold": 0.1,
        "key_weight": 9,
        "value_weight": 1,
        "key_structure": {
            "type": "String"
        },
        "value_structure": {
            "type": "Union",
            "substructures": {
                "bool": null,
                "int": null,
                "str": {
                    "type": "String",
                    "max_square_length": 1000000
                }
            }
        }
    },
    "textures": {
        "type": "Dict",
        "key_weight": 0,
        "key_types": "int",
        "this_types": "list",
        "delegate_arguments": {
            "field": "texture",
            "show_item_key": false,
            "sort": "sort_by_value",
            "measure_length": true
        },
        "value_types": "str",
        "value_structure": null
    },
    "uv_anim": {
        "type": "Keymap",
        "keys": {
            "offset": {"types": "list", "structure": "uv_anim.offsets"},
            "scale": {"types": "list", "structure": "uv_anim.scale"}
        }
    },
    "uv_anim.offsets": {
        "type": "Dict",
        "key_weight": 0,
        "key_types": "int",
        "this_types": "list",
        "delegate_arguments": {
            "field": "expression",
            "show_item_key": false,
            "measure_length": true
        },
        "value_types": ["float", "str"],
        "value_structure": null
    },
    "uv_anim.scale": {
        "type": "Dict",
        "allow_key_moves": false,
        "key_weight": 0,
        "key_types": "int",
        "this_types": "list",
        "delegate_arguments": {
            "field": "range item",
            "measure_length": false,
            "print_flat": true,
            "print_all": true
        },
        "value_types": "float",
        "value_structure": null
    }
}