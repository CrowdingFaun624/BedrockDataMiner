{
    "group_aliases": {
        "s": "serializers",
        "temp": "structures/template"
    },
    "base": {
        "inherit": "temp/base_single_file",
        "$name": "terrain_meta",
        "$types": "list",
        "$structure": "#@images"
    },
    "images": {
        "type": "Normalizer",
        "outer_types": "list",
        "inner_types": "dict",
        "functions": {
            "function_name": "wrap_in_dict",
            "arguments": {
                "key": "name",
                "delete": true
            }
        },
        "structure": {
            "type": "Dict",
            "delegate_arguments": {
                "field": "image",
                "sort": "sort_by_key",
                "measure_length": true
            },
            "min_key_similarity_threshold": 0.5,
            "key_structure": {"type": "String"},
            "value_types": "dict",
            "value_structure": "image"
        }
    },
    "image": {
        "type": "Keymap",
        "delegate_arguments": {
            "sort": "sort_by_component_order"
        },
        "keys": {
            "additonal_textures": {"types": "list", "structure": "uvs"},
            "uv": {"types": "list", "structure": "uv"},
            "uvs": {"types": "list", "structure": "uvs"}
        }
    },
    "uvs": {
        "type": "Dict",
        "allow_key_moves": false,
        "key_weight": 0,
        "key_types": "int",
        "this_types": "list",
        "delegate_arguments": {
            "field": "uv",
            "measure_length": true
        },
        "value_types": "list",
        "value_structure": "uv"
    },
    "uv": {
        "type": "Normalizer",
        "outer_types": "list",
        "inner_types": "dict",
        "functions": {
            "function_name": "wrap_tuple",
            "arguments": {
                "keys": ["x1", "y1", "x2", "y2", "1", "2"]
            }
        },
        "structure": {
            "type": "Keymap",
            "delegate_arguments": {
                "sort": "sort_by_component_order"
            },
            "keys": {
                "x1": {"types": ["float", "int"]},
                "y1": {"types": ["float", "int"]},
                "x2": {"types": ["float", "int"]},
                "y2": {"types": ["float", "int"]},
                "1":  {"types": "int"},
                "2":  {"types": "int"}
            }
        }
    }
}