{
    "base": {
        "type": "Base",
        "delegate_arguments": {
            "name": "terrain_meta"
        },
        "types": "file",
        "subcomponent": "cache"
    },
    "cache": {
        "type": "Cache",
        "types": "file",
        "subcomponent": {
            "type": "File",
            "file_types": "file",
            "content_types": "dict",
            "subcomponent": "images"
        }
    },
    "images": {
        "type": "Dict",
        "delegate_arguments": {
            "field": "image",
            "measure_length": true
        },
        "pre_normalized_types": "list",
        "normalizer": {
            "function_name": "wrap_in_dict",
            "arguments": {
                "key": "name",
                "delete": true
            }
        },
        "types": ["dict"],
        "sort": "by_key",
        "subcomponent": "image"
    },
    "image": {
        "type": "Keymap",
        "sort": "by_component_order",
        "keys": {
            "name": {"types": "str", "subcomponent": {"type": "String"}, "delegate_arguments": {"always_print": true}},
            "additonal_textures": {"types": "list", "subcomponent": "uvs"},
            "uv": {"types": "dict", "subcomponent": "uv"},
            "uvs": {"types": "list", "subcomponent": "uvs"}
        }
    },
    "uvs": {
        "type": "List",
        "delegate_arguments": {
            "field": "uv",
            "measure_length": true
        },
        "types": ["dict"],
        "subcomponent": "uv"
    },
    "uv": {
        "type": "Keymap",
        "sort": "by_component_order",
        "pre_normalized_types": "list",
        "normalizer": {
            "function_name": "wrap_tuple",
            "arguments": {
                "keys": ["x1", "y1", "x2", "y2", "1", "2"]
            }
        },
        "keys": {
            "x1": {"types": ["float", "int"]},
            "y1": {"types": ["float", "int"]},
            "x2": {"types": ["float", "int"]},
            "y2": {"types": ["float", "int"]},
            "1":  {"types": ["float", "int"]},
            "2":  {"types": ["float", "int"]}
        }
    }
}