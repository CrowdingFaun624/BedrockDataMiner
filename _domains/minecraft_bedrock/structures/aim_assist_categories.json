{
    "group_aliases": {
        "s": "serializers",
        "temp": "structures/template"
    },
    "base": {
        "inherit": "temp/base_pack_files",
        "$name": "aim_assist_categories",
        "$pack_type": "behavior pack",
        "$structure": "#@file"
    },
    "file": {
        "type": "Keymap",
        "delegate_arguments": {
            "sort": "sort_by_component_order"
        },
        "keys": {
            "format_version": {"types": "str", "similarity_weight": 0},
            "minecraft:aim_assist_categories": {"types": "dict", "structure": "aim_assist_categories"}
        }
    },
    "aim_assist_categories": {
        "type": "Keymap",
        "delegate_arguments": {
            "sort": "sort_by_component_order"
        },
        "keys": {
            "identifier": {"types": "str", "structure": "temp/string_remove_minecraft_prefix", "delegate_arguments": {"always_print": true}},
            "categories": {"types": "list", "structure": "categories", "similarity_weight": 3}
        }
    },
    "categories": {
        "type": "Dict",
        "delegate_arguments": {
            "field": "category",
            "show_item_key": false,
            "measure_length": true
        },
        "key_types": "int",
        "key_weight": 0,
        "this_types": "list",
        "value_types": "dict",
        "value_structure": "category"
    },
    "category": {
        "type": "Keymap",
        "delegate_arguments": {
            "sort": "sort_by_component_order"
        },
        "min_value_similarity_threshold": 0.4,
        "keys": {
            "name": {"types": "str", "structure": "temp/string_remove_minecraft_prefix", "similarity_weight": 10, "delegate_arguments": {"always_print": true}},
            "entity_default": {"types": "int"},
            "block_default": {"types": "int"},
            "priorities": {"types": "dict", "structure": "priorities"}
        }
    },
    "priorities": {
        "type": "Keymap",
        "delegate_arguments": {
            "sort": "sort_by_component_order"
        },
        "keys": {
            "blocks": {"types": "dict", "structure": "priorities.blocks"},
            "block_default": {"types": "int"},
            "entity_default": {"types": "int"}
        }
    },
    "priorities.blocks": {
        "type": "Dict",
        "delegate_arguments": {
            "field": "block",
            "measure_length": true,
            "sort": "sort_by_key"
        },
        "key_structure": "temp/string_remove_minecraft_prefix",
        "value_types": "int",
        "value_structure": null
    }
}