{
    "base": {
        "type": "Base",
        "delegate_arguments": {
            "name": "renderer_light_clustering"
        },
        "types": "file",
        "subcomponent": "cache"
    },
    "cache": {
        "type": "Cache",
        "types": "file",
        "subcomponent": {
            "type": "File",
            "file_types": "file",
            "content_types": "dict",
            "subcomponent": "renderer_light_clustering"
        }
    },
    "renderer_light_clustering": {
        "type": "Keymap",
        "sort": "by_key",
        "keys": {
            "cluster_size": {"types": "list", "subcomponent": "vector_int"},
            "clusterSize": {"types": "list", "subcomponent": "vector_int", "tags": ["outdated"]},
            "far_plane": {"types": "float"},
            "farPlane": {"types": "float", "tags": ["outdated"]},
            "lights_per_cluster": {"types": "int"},
            "lightsPerCluster": {"types": "int", "tags": ["outdated"]},
            "NearFar": {"types": "dict", "subcomponent": "near_far", "tags": ["outdated"]}
        }
    },
    "vector_int": {
        "type": "List",
        "delegate_arguments": {
            "field": "vector item",
            "print_all": true,
            "print_flat": true,
            "measure_length": false
        },
        "types": ["int"],
        "subcomponent": null
    },
    "near_far": {
        "type": "Keymap",
        "sort": "by_key",
        "delegate_arguments": {
            "print_all": true
        },
        "keys": {
            "x": {"types": "float"},
            "y": {"types": "float"}
        }
    }
}