{
    "group_aliases": {
        "s": "serializers",
        "temp": "structures/template"
    },
    "base": {
        "inherit": "temp/base_multiple_pack_files",
        "$name": "animations",
        "$pack_type": "resource pack",
        "$key_weight": 5,
        "$value_weight": 8,
        "$min_value_similarity_threshold": 0.3,
        "$structure": "#@animations_file"
    },
    "animations_file": {
        "type": "Normalizer",
        "functions": {
            "function_name": "normalizers/normalizers animations_fix_old"
        },
        "outer_types": "dict",
        "inner_types": "dict",
        "structure": {
            "type": "Keymap",
            "delegate_arguments": {
                "sort": "sort_by_component_order"
            },
            "keys": {
                "format_version": {"types": "str"},
                "animations": {"types": "dict", "structure": "animations", "similarity_weight": 20}
            }
        }
    },
    "animations": {
        "type": "Dict",
        "value_types": "dict",
        "delegate_arguments": {
            "field": "animation",
            "measure_length": true,
            "sort": "sort_by_key"
        },
        "min_key_similarity_threshold": 0.9,
        "min_value_similarity_threshold": 0.75,
        "key_structure": {"type": "String"},
        "value_structure": "animation"
    },
    "timestamps_and_vectors_or_vector_or_lerp": {
        "type": "Union",
        "substructures": {
            "dict": "timestamps_and_vectors_or_lerp",
            "list": "vector_special"
        }
    },
    "timestamps_and_vectors_or_lerp": {
        "type": "Dict",
        "value_types": ["dict", "list"],
        "allow_key_moves": false,
        "delegate_arguments": {
            "field": "timestamp",
            "measure_length": true
        },
        "value_structure": "vector_or_lerp"
    },
    "vector_or_lerp": {
        "type": "Union",
        "substructures": {
            "dict": "lerp",
            "list": "vector_special"
        }
    },
    "lerp": {
        "type": "Keymap",
        "delegate_arguments": {
            "sort": "sort_by_component_order"
        },
        "keys": {
            "lerp_mode": {"types": "str"},
            "pre": {"types": "list", "structure": "vector_special"},
            "post": {"types": "list", "structure": "vector_special"}
        }
    },
    "vector_special": {
        "type": "Dict",
        "allow_key_moves": false,
        "allow_same_key_optimization": true,
        "key_weight": 0,
        "key_types": "int",
        "this_types": "list",
        "value_types": ["temp/decimal", "dict", "str"],
        "delegate_arguments": {
            "field": "vector item",
            "print_all": true,
            "print_flat": true,
            "measure_length": false
        },
        "value_structure": {
            "type": "Union",
            "substructures": {
                "temp/decimal": null,
                "dict": {
                    "type": "Keymap",
                    "delegate_arguments": {
                        "sort": "sort_by_component_order",
                        "print_all": true
                    },
                    "keys": {
                        "x": {"types": "str"},
                        "y": {"types": "str"}
                    }
                },
                "str": {
                    "type": "String",
                    "max_square_length": 1000000
                }
            }
        }
    },
    "animation": {
        "type": "Keymap",
        "delegate_arguments": {
            "sort": "sort_by_component_order"
        },
        "keys": {
            "anim_time_update": {"types": "str"},
            "animation_length": {"types": "temp/decimal"},
            "loop": {"types": ["bool", "str"]},
            "override_previous_animation": {"types": "bool"},
            "start_delay": {"types": "str"},
            "bones": {"types": "dict", "structure": "bones"},
            "particle_effects": {"types": "dict", "structure": "particle_effects"},
            "sound_effects": {"types": "dict", "structure": "sound_effects"}
        }
    },
    "bones": {
        "type": "Dict",
        "delegate_arguments": {
            "field": "bone",
            "sort": "sort_by_key",
            "measure_length": true
        },
        "value_types": "dict",
        "min_value_similarity_threshold": 0.3,
        "min_key_similarity_threshold": 0.3,
        "key_weight": 3,
        "value_weight": 5,
        "key_structure": {
            "type": "String"
        },
        "value_structure": "bone"
    },
    "bone": {
        "type": "Keymap",
        "delegate_arguments": {
            "sort": "sort_by_key"
        },
        "keys": {
            "relative_to": {"types": "dict", "structure": "bone.relative_to"},
            "rotation": {"types": ["list", "dict"], "structure": "timestamps_and_vectors_or_vector_or_lerp"},
            "position": {"types": ["list", "dict"], "structure": "timestamps_and_vectors_or_vector_or_lerp"},
            "scale": {"types": ["temp/decimal", "dict", "list", "str"], "structure": "bone.scale"}
        }
    },
    "bone.relative_to": {
        "type": "Keymap",
        "keys": {
            "rotation": {"types": "str"}
        }
    },
    "bone.scale": {
        "type": "Union",
        "substructures": {
            "temp/decimal": null,
            "dict": "timestamps_and_vectors_or_lerp",
            "list": "vector_special",
            "str": null
        }
    },
    "particle_effects": {
        "type": "Dict",
        "delegate_arguments": {
            "field": "timestamp",
            "measure_length": true,
            "sort": "sort_by_key"
        },
        "value_types": "dict",
        "allow_key_moves": false,
        "value_structure": {
            "type": "Keymap",
            "keys": {
                "effect": {"types": "str", "delegate_arguments": {"always_print": true}},
                "pre_effect_script": {"types": "str"}
            }
        }
    },
    "sound_effects": {
        "type": "Dict",
        "delegate_arguments": {
            "field": "timestamp",
            "measure_length": true,
            "sort": "sort_by_key"
        },
        "value_types": "dict",
        "allow_key_moves": false,
        "value_structure": {
            "type": "Keymap",
            "keys": {
                "effect": {"types": "str", "delegate_arguments": {"always_print": true}}
            }
        }
    }
}