{
    "group_aliases": {
        "s": "serializers",
        "temp": "structures/template"
    },
    "base": {
        "inherit": "temp/base_pack_files",
        "$name": "resources",
        "$pack_type": "resource pack",
        "$structure": "#@file"
    },
    "file": {
        "type": "Keymap",
        "imports": "categories",
        "keys": {
            "pack_id": {"types": "str", "structure": "temp/string", "delegate_arguments": {"always_print": true}},
            "name": {"types": "str"},
            "description": {"types": "str"},
            "key_art_path": {"types": "str"},
            "screenshots": {"types": "list", "structure": "screenshots"},
            "resources": {"types": "dict", "structure": "categories"},
            "skins": {"types": "dict", "structure": "skins"}
        }
    },
    "screenshots": {
        "type": "Dict",
        "key_weight": 0,
        "key_types": "int",
        "this_types": "list",
        "value_types": "dict",
        "delegate_arguments": {
            "field": "screenshot",
            "show_item_key": false,
            "measure_length": true
        },
        "value_structure": "screenshot"
    },
    "screenshot": {
        "type": "Keymap",
        "keys": {
            "path": {"types": "str", "structure": "temp/string", "delegate_arguments": {"always_print": true}}
        }
    },
    "categories": {
        "type": "Keymap",
        "delegate_arguments": {
            "field": "category",
            "measure_length": true
        },
        "keys": {
            "models": {"types": "dict", "structure": "resources"},
            "textures": {"types": "dict", "structure": "resources"},
            "metas": {"types": "dict", "structure": "resources"}
        }
    },
    "resources": {
        "type": "Dict",
        "value_types": "str",
        "delegate_arguments": {
            "field": "resource",
            "measure_length": true
        },
        "value_structure": "temp/string",
        "key_structure": "temp/string"
    },
    "skins": {
        "type": "Dict",
        "value_types": "str",
        "delegate_arguments": {
            "field": "skin",
            "measure_length": true
        },
        "value_structure": "temp/string",
        "key_structure": "temp/string"
    }
}