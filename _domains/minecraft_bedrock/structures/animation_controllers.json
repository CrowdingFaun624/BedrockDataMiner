{
    "group_aliases": {
        "s": "serializers",
        "t": "structure_tags",
        "temp": "structures/template"
    },
    "base": {
        "inherit": "temp/base_multiple_pack_files",
        "$name": "animation_controllers",
        "$pack_type": "resource pack",
        "$files_key_weight": 4,
        "$files_key_structure": "temp/string_remove_file_suffix",
        "$files_min_key_similarity_threshold": 0.5,
        "$structure": "#@animation_controllers_file"
    },
    "animation_controllers_file": {
        "type": "Normalizer",
        "outer_types": "dict",
        "inner_types": "dict",
        "functions": {
            "function_name": "normalizers/normalizers animation_controllers_fix_old"
        },
        "structure": {
            "type": "Keymap",
            "delegate_arguments": {
                "sort": "sort_by_component_order"
            },
            "keys": {
                "format_version": {"types": "str"},
                "animation_controllers": {"types": "dict", "structure": "animation_controllers", "similarity_weight": 14}
            }
        }
    },
    "animation_controllers": {
        "type": "Dict",
        "delegate_arguments": {
            "field": "animation controller",
            "measure_length": true,
            "sort": "sort_by_key"
        },
        "min_value_similarity_threshold": 0.7,
        "value_types": "dict",
        "key_structure": {"type": "String"},
        "value_structure": "animation_controller"
    },
    "animation_controller": {
        "type": "Keymap",
        "delegate_arguments": {
            "sort": "sort_by_component_order"
        },
        "keys": {
            "initial_state": {"types": "str"},
            "states": {"types": "dict", "structure": "states"}
        }
    },
    "states": {
        "type": "Dict",
        "delegate_arguments": {
            "field": "state",
            "measure_length": true,
            "sort": "sort_by_key"
        },
        "min_key_similarity_threshold": 0.4,
        "value_types": "dict",
        "value_structure": "state"
    },
    "state": {
        "type": "Keymap",
        "keys": {
            "animations": {"types": ["dict", "list"], "structure": {
                "type": "Union",
                "substructures": {
                    "dict": "animations:dict",
                    "list": "animations"
                }
            }},
            "blend_transition": {"types": "float"},
            "blend_via_shortest_path": {"types": "bool"},
            "parameters": {"types": "list", "structure": "parameters", "tags": ["t/outdated"]},
            "particle_effects": {"types": "list", "structure": "particle_effects"},
            "sound_effects": {"types": "list", "structure": "sound_effects"},
            "transitions": {"types": "list", "structure": "transitions"}
        }
    },
    "animations": {
        "type": "Dict",
        "delegate_arguments": {
            "field": "animation dict",
            "show_item_key": false,
            "measure_length": true
        },
        "this_types": "list",
        "key_types": "int",
        "key_weight": 0,
        "value_types": ["dict", "str"],
        "value_structure": {
            "type": "Union",
            "substructures": {
                "dict": "animation",
                "str": null
            }
        }
    },
    "animations:dict": {
        "type": "Dict",
        "delegate_arguments": {
            "field": "animation",
            "measure_length": true
        },
        "value_types": ["dict", "list"],
        "value_structure": {
            "type": "Union",
            "substructures": {
                "dict": "temp/empty",
                "list": "animation:list"
            }
        }
    },
    "animation": {
        "type": "Dict",
        "delegate_arguments": {
            "field": "animation",
            "measure_length": true
        },
        "value_types": ["list", "str"],
        "value_structure": {
            "type": "Union",
            "substructures": {
                "list": "animation:list",
                "str": null
            }
        }
    },
    "animation:list": {
        "type": "Dict",
        "delegate_arguments": {
            "field": "item",
            "show_item_key": false,
            "measure_length": true
        },
        "this_types": "list",
        "key_types": "int",
        "key_weight": 0,
        "value_types": "dict",
        "value_structure": "animation:list.dict"
    },
    "animation:list.dict": {
        "type": "Dict",
        "delegate_arguments": {
            "field": "timestamp",
            "measure_length": true
        },
        "value_types": "float",
        "value_structure": null
    },
    "parameters": {
        "type": "Dict",
        "delegate_arguments": {
            "field": "parameter",
            "show_item_key": false,
            "measure_length": true
        },
        "this_types": "list",
        "key_types": "int",
        "key_weight": 0,
        "value_types": "str",
        "value_structure": {"type": "String"}
    },
    "particle_effects": {
        "type": "Dict",
        "delegate_arguments": {
            "field": "particle effect",
            "show_item_key": false,
            "measure_length": true
        },
        "this_types": "list",
        "key_types": "int",
        "key_weight": 0,
        "value_types": "dict",
        "value_structure": "particle_effect"
    },
    "particle_effect": {
        "type": "Keymap",
        "keys": {
            "effect": {"types": "str", "delegate_arguments": {"always_print": true}},
            "locator": {"types": "str"}
        }
    },
    "sound_effects": {
        "type": "Dict",
        "delegate_arguments": {
            "field": "sound effect",
            "show_item_key": false,
            "measure_length": true
        },
        "key_weight": 0,
        "key_types": "int",
        "this_types": "list",
        "value_types": "dict",
        "value_structure": "sound_effect"
    },
    "sound_effect": {
        "type": "Keymap",
        "keys": {
            "effect": {"types": "str", "delegate_arguments": {"always_print": true}}
        }
    },
    "transitions": {
        "type": "Dict",
        "delegate_arguments": {
            "field": "transition list",
            "show_item_key": false,
            "measure_length": true
        },
        "this_types": "list",
        "key_types": "int",
        "key_weight": 0,
        "value_types": "dict",
        "value_structure": "transition"
    },
    "transition": {
        "type": "Dict",
        "delegate_arguments": {
            "field": "transition",
            "measure_length": true
        },
        "value_types": "str",
        "value_structure": null
    }
}