{
    "group_aliases": {
        "s": "serializers",
        "t": "structure_tags",
        "temp": "structures/template"
    },
    "base": {
        "inherit": "temp/base_multiple_pack_files",
        "$name": "materials",
        "$file_field": "material",
        "$pack_type": "resource pack",
        "$structure": "#@material"
    },
    "material": {
        "type": "Normalizer",
        "outer_types": "dict",
        "inner_types": "dict",
        "functions": {
            "function_name": "normalizers/normalizers materials_normalize_material"
        },
        "structure": {
            "type": "Keymap",
            "delegate_arguments": {
                "sort": "sort_by_component_order"
            },
            "keys": {
                "version": {"types": "str"},
                "materials": {"types": "dict", "structure": "material.materials"}
            }
        }
    },
    "material.materials": {
        "type": "Dict",
        "delegate_arguments": {
            "field": "material",
            "sort": "sort_by_key",
            "measure_length": true
        },
        "value_types": "dict",
        "min_key_similarity_threshold": 0.875,
        "key_structure": {
            "type": "String"
        },
        "value_structure": "material.material"
    },
    "material.material": {
        "type": "Keymap",
        "keys": {
            "+defines": {"types": "list", "structure": "defines"},
            "+samplerStates": {"types": "list", "structure": "sampler_states"},
            "+states": {"types": "list", "structure": "states"},
            "+variants": {"types": "list", "structure": "variants"},
            "-defines": {"types": "list", "structure": "defines"},
            "-states": {"types": "list", "structure": "states"},
            "alphaDst": {"types": "str"},
            "alphaSrc": {"types": "str", "tags": ["t/outdated"]},
            "backFace": {"types": "dict", "structure": "face"},
            "blendDst": {"types": "str"},
            "blendSrc": {"types": "str"},
            "defines": {"types": "list", "structure": "defines"},
            "depthBias": {"types": "temp/decimal"},
            "depthBiasOGL": {"types": "temp/decimal"},
            "depthFunc": {"types": "str"},
            "fragmentShader": {"types": "str"},
            "frontFace": {"types": "dict", "structure": "face"},
            "geometryShader": {"types": "str", "tags": ["t/outdated"]},
            "isAnimatedTexture": {"types": "int"},
            "msaaSupport": {"types": "str"},
            "polygonOffsetLevel": {"types": "temp/decimal", "tags": ["t/outdated"]},
            "primitiveMode": {"types": "str"},
            "samplerStates": {"types": "list", "structure": "sampler_states"},
            "slopeScaledDepthBias": {"types": "temp/decimal"},
            "slopeScaledDepthBiasOGL": {"types": "temp/decimal"},
            "states": {"types": "list", "structure": "states"},
            "stencilReadMask": {"types": "int"},
            "stencilRef": {"types": "int"},
            "stencilRefOverride": {"types": "int"},
            "stencilWriteMask": {"types": "int"},
            "textures": {"types": "list", "structure": "textures", "tags": ["t/outdated"]},
            "variants": {"types": "list", "structure": "variants"},
            "vertexFields": {"types": "list", "structure": "vertex_fields"},
            "vertexShader": {"types": "str"},
            "vrGeometryShader": {"types": "str"}
        }
    },
    "defines": {
        "type": "Dict",
        "key_weight": 0,
        "key_types": "int",
        "this_types": "list",
        "delegate_arguments": {
            "field": "definition",
            "show_item_key": false,
            "sort": "sort_by_value",
            "measure_length": true
        },
        "value_types": "str",
        "value_structure": null
    },
    "face": {
        "type": "Keymap",
        "keys": {
            "stencilDepthFailOp": {"types": "str"},
            "stencilFailOp": {"types": "str"},
            "stencilFunc": {"types": "str"},
            "stencilPass": {"types": "str"},
            "stencilPassOp": {"types": "str"}
        }
    },
    "sampler_states": {
        "type": "Dict",
        "key_weight": 0,
        "key_types": "int",
        "this_types": "list",
        "delegate_arguments": {
            "field": "sampler state",
            "show_item_key": false,
            "measure_length": true
        },
        "value_types": "dict",
        "value_structure": "sampler_states.item"
    },
    "sampler_states.item": {
        "type": "Keymap",
        "keys": {
            "samplerIndex": {"types": "int"},
            "textureFilter": {"types": "str"},
            "textureIndex": {"types": "int", "tags": ["t/outdated"]},
            "textureLocation": {"types": "str", "tags": ["t/outdated"]},
            "textureName": {"types": "str", "tags": ["t/outdated"]},
            "textureWrap": {"types": "str"}
        }
    },
    "states": {
        "type": "Dict",
        "key_weight": 0,
        "key_types": "int",
        "this_types": "list",
        "delegate_arguments": {
            "field": "statement",
            "show_item_key": false,
            "sort": "sort_by_value",
            "measure_length": true
        },
        "value_types": "str",
        "value_structure": null
    },
    "textures": {
        "type": "Dict",
        "key_weight": 0,
        "key_types": "int",
        "this_types": "list",
        "delegate_arguments": {
            "field": "texture",
            "show_item_key": false,
            "measure_length": true
        },
        "value_types": "dict",
        "value_structure": "texture"
    },
    "texture": {
        "type": "Keymap",
        "keys": {
            "textureIndex": {"types": "int"},
            "textureLocation": {"types": "str", "structure": {"type": "String"}},
            "textureName": {"types": "str", "structure": {"type": "String"}, "delegate_arguments": {"always_print": true}}
        }
    },
    "variants": {
        "type": "Dict",
        "key_weight": 0,
        "key_types": "int",
        "this_types": "list",
        "delegate_arguments": {
            "field": "variant",
            "show_item_key": false,
            "measure_length": true
        },
        "value_types": "dict",
        "value_structure": "material.materials"
    },
    "vertex_fields": {
        "type": "Dict",
        "key_weight": 0,
        "key_types": "int",
        "this_types": "list",
        "delegate_arguments": {
            "field": "vertex field",
            "show_item_key": false,
            "measure_length": true
        },
        "value_types": "dict",
        "value_structure": "vertex_fields.item"
    },
    "vertex_fields.item": {
        "type": "Keymap",
        "keys": {
            "field": {"types": "str", "structure": {"type": "String"}}
        }
    }
}