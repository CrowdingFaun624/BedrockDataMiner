{
    "base": {
        "type": "Base",
        "delegate_arguments": {
            "name": "trade_tables"
        },
        "types": "dict",
        "subcomponent": "trade_tables"
    },
    "decimal": {
        "type": "TypeAlias",
        "types": ["float", "int"]
    },
    "trade_tables": {
        "type": "Dict",
        "delegate_arguments": {
            "field": "trade table",
            "measure_length": true
        },
        "types": ["dict"],
        "sort": "by_key",
        "subcomponent": "behavior_packs"
    },
    "behavior_packs": {
        "type": "Dict",
        "types": ["file"],
        "delegate_arguments": {
            "field": "behavior pack"
        },
        "post_normalizer": {
            "function_name": "normalizers/collapse_resource_packs collapse_resource_packs_flat"
        },
        "sort": "by_key",
        "subcomponent": "cache"
    },
    "cache": {
        "type": "Cache",
        "types": "file",
        "subcomponent": {
            "type": "File",
            "file_types": "file",
            "content_types": "dict",
            "subcomponent": "trade_table"
        }
    },
    "trade_table": {
        "type": "Keymap",
        "sort": "by_component_order",
        "keys": {
            "format_version": {"types": "str"},
            "defined_in": {"types": "list", "subcomponent": "defined_in"},
            "tiers": {"types": "list", "subcomponent": "tiers"}
        }
    },
    "defined_in": {
        "type": "Set",
        "delegate_arguments": {
            "field": "behavior pack",
            "measure_length": true
        },
        "types": ["str"],
        "sort": true,
        "subcomponent": null
    },
    "tiers": {
        "type": "List",
        "delegate_arguments": {
            "field": "tier"
        },
        "types": ["dict"],
        "subcomponent": "tier"
    },
    "tier": {
        "type": "Keymap",
        "keys": {
            "groups": {"types": "list", "subcomponent": "groups", "weight": 10},
            "num_to_select": {"types": "int"},
            "total_exp_required": {"types": "int"},
            "trades": {"types": "list", "subcomponent": "trades", "weight": 10}
        }
    },
    "groups": {
        "type": "Set",
        "delegate_arguments": {
            "field": "group",
            "measure_length": true
        },
        "types": ["dict"],
        "min_similarity_threshold": 0.3,
        "max_similarity_descendent_depth": 8,
        "subcomponent": "tier"
    },
    "trades": {
        "type": "Set",
        "delegate_arguments": {
            "field": "trade",
            "measure_length": true
        },
        "types": ["dict"],
        "min_similarity_threshold": 0.3,
        "subcomponent": "trade"
    },
    "biome_list": {
        "type": "Set",
        "delegate_arguments": {
            "field": "biome",
            "measure_length": true
        },
        "types": ["str"],
        "sort": true,
        "subcomponent": null
    },
    "trade": {
        "type": "Keymap",
        "sort": "by_component_order",
        "keys": {
            "weight": {"types": "int"},
            "max_uses": {"types": "int"},
            "reward_exp": {"types": "bool"},
            "total_exp_required": {"types": "int"},
            "trader_exp": {"types": "int"},
            "gives": {"types": "list", "subcomponent": "trade:gives"},
            "wants": {"types": "list", "subcomponent": "trade:wants"}
        }
    },
    "trade:gives": {
        "type": "Set",
        "delegate_arguments": {
            "field": "given item",
            "measure_length": true
        },
        "types": ["dict"],
        "min_similarity_threshold": 0.75,
        "max_similarity_descendent_depth": null,
        "subcomponent": "trade:gives.item"
    },
    "trade:wants": {
        "type": "Set",
        "delegate_arguments": {
            "field": "wanted item",
            "measure_length": true
        },
        "types": ["dict"],
        "min_similarity_threshold": 0.75,
        "max_similarity_descendent_depth": null,
        "subcomponent": "trade:wants.item"
    },
    "trade:gives.item": {
        "type": "Keymap",
        "sort": "by_component_order",
        "keys": {
            "item": {"types": "str", "subcomponent": {
                "type": "String",
                "max_similarity_ancestor_depth": 5
            }, "weight": 12, "delegate_arguments": {"always_print": true}},
            "quantity": {"types": ["dict", "int"], "subcomponent": "structures/loot_tables/range_int_or_int"},
            "biomes": {"types": "list", "subcomponent": "biome_list"},
            "choice": {"types": "list", "subcomponent": "trade:gives.item.choice"},
            "filters": {"types": "dict", "subcomponent": "structures/entities/filter"},
            "functions": {"types": "dict", "subcomponent": "structures/loot_tables/functions"}
        }
    },
    "trade:gives.item.choice": {
        "type": "Set",
        "delegate_arguments": {
            "field": "choice",
            "measure_length": true
        },
        "types": ["dict"],
        "subcomponent": "trade:gives.item"
    },
    "trade:wants.item": {
        "type": "Keymap",
        "sort": "by_component_order",
        "keys": {
            "item": {"types": "str", "subcomponent": {
                "type": "String",
                "max_similarity_ancestor_depth": 5
            }, "weight": 12, "delegate_arguments": {"always_print": true}},
            "quantity": {"types": ["dict", "int"], "subcomponent": "structures/loot_tables/range_int_or_int"},
            "choice": {"types": "list", "subcomponent": "trade:wants.item.choice"},
            "biomes": {"types": "list", "subcomponent": "biome_list", "tags": ["outdated"]},
            "filters": {"types": "dict", "subcomponent": "structures/entities/filter"},
            "functions": {"types": "dict", "subcomponent": "structures/loot_tables/functions", "tags": ["outdated"]},
            "price_multiplier": {"types": "decimal"}
        }
    },
    "trade:wants.item.choice": {
        "type": "Set",
        "delegate_arguments": {
            "field": "choice",
            "measure_length": true
        },
        "types": ["dict"],
        "subcomponent": "trade:wants.item"
    }
}