{
    "base": {
        "type": "StructureBase",
        "delegate_arguments": {
            "name": "renderer_shadow_configs"
        },
        "this_types": "dict",
        "structure": "files"
    },
    "files": {
        "type": "Dict",
        "delegate_arguments": {
            "field": "file",
            "sort": "sort_by_key",
            "measure_length": true
        },
        "value_types": "dict",
        "value_structure": "cache"
    },
    "cache": {
        "type": "Cache",
        "this_types": "dict",
        "pre_normalized_types": "file",
        "structure": {
            "type": "File",
            "serializer": "minecraft_common!json",
            "file_types": "file",
            "content_types": "dict",
            "structure": "file"
        }
    },
    "file": {
        "type": "Keymap",
        "delegate_arguments": {
            "sort": "sort_by_component_order"
        },
        "imports": "shadow_config",
        "keys": {
            "format_version": {"types": "str"},
            "cascades": {"types": "list", "structure": "cascades"},
            "low": {"types": "dict", "structure": "shadow_config"},
            "medium": {"types": "dict", "structure": "shadow_config"},
            "high": {"types": "dict", "structure": "shadow_config"},
            "ultra": {"types": "dict", "structure": "shadow_config"}
        }
    },
    "cascades": {
        "type": "Dict",
        "key_weight": 0,
        "key_types": "int",
        "this_types": "list",
        "value_types": "list",
        "delegate_arguments": {
            "field": "cascade",
            "show_item_key": false,
            "measure_length": true
        },
        "value_structure": "cascade"
    },
    "cascade": {
        "type": "Dict",
        "key_weight": 0,
        "key_types": "int",
        "this_types": "list",
        "value_types": "dict",
        "delegate_arguments": {
            "field": "item",
            "show_item_key": false,
            "measure_length": true
        },
        "value_structure": "point_light_shadow"
    },
    "shadow_config": {
        "type": "Keymap",
        "delegate_arguments": {
            "sort": "sort_by_key"
        },
        "keys": {
            "cascade_bias": {"types": "list", "structure": "vector_float"},
            "cascade_light_quantization_steps": {"types": "list", "structure": "vector_int"},
            "cascade_pcf_width": {"types": "list", "structure": "vector_float"},
            "cascade_range": {"types": "list", "structure": "vector_float"},
            "cascade_resolution": {"types": "list", "structure": "vector_int"},
            "cascade_slope_bias": {"types": "list", "structure": "vector_float"},
            "cloud_shadow_contribution": {"types": "float"},
            "cloud_shadow_pcf_width": {"types": "float"},
            "cloud_shadow_quantization_steps": {"types": "int"},
            "cloud_shadows": {"types": "bool"},
            "directional_light_shadow": {"types": "dict", "structure": "shadow_config", "tags": ["outdated"]},
            "file": {"types": "str"},
            "max_shadow_distance": {"types": "float"},
            "moon_shadows": {"types": "bool"},
            "normal_offset_strength": {"types": "float"},
            "num_cascades": {"types": "int", "tags": ["outdated"]},
            "num_cascade_levels": {"types": "int"},
            "num_variable_cascade_levels": {"types": "int"},
            "point_light_shadow": {"types": "dict", "structure": "point_light_shadow", "tags": ["outdated"]},
            "shadow_clamp_angle": {"types": "int"},
            "shadow_range": {"types": "dict", "structure": "shadow_range"},
            "sun_shadows": {"types": "bool"}
        }
    },
    "shadow_range": {
        "type": "Keymap",
        "delegate_arguments": {
            "sort": "sort_by_component_order",
            "print_all": true
        },
        "keys": {
            "near": {"types": "float"},
            "far": {"types": "float"}
        }
    },
    "point_light_shadow": {
        "type": "Keymap",
        "delegate_arguments": {
            "sort": "sort_by_component_order"
        },
        "keys": {
            "bias": {"types": "float"},
            "has_dynamic_geometry": {"types": "bool"},
            "has_static_geometry": {"types": "bool"},
            "pcf_width": {"types": ["int", "float"]},
            "range": {"types": "float"},
            "update_frequency": {"types": "int"},
            "update_offset": {"types": "int"},
            "light_quantization_steps": {"types": "int"},
            "slope_bias": {"types": "float"},
            "far_plane": {"types": "float"},
            "near_plane": {"types": "float"},
            "max_point_light_shadows": {"types": "int"},
            "resolution": {"types": "int"},
            "enabled": {"types": "bool"}
        }
    },
    "vector_float": {
        "type": "Dict",
        "allow_key_moves": false,
        "key_weight": 0,
        "key_types": "int",
        "this_types": "list",
        "delegate_arguments": {
            "field": "vector item",
            "measure_length": false
        },
        "value_types": "float",
        "value_structure": null
    },
    "vector_int": {
        "type": "Dict",
        "allow_key_moves": false,
        "key_weight": 0,
        "key_types": "int",
        "this_types": "list",
        "delegate_arguments": {
            "field": "vector item",
            "measure_length": false
        },
        "value_types": "int",
        "value_structure": null
    }
}