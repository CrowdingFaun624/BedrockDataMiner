{
    "group_aliases": {
        "s": "serializers",
        "temp": "structures/template"
    },
    "base": {
        "inherit": "temp/base_multiple_pack_files",
        "$name": "water",
        "$file_field": "water file",
        "$pack_type": "resource pack",
        "$structure": "#@water_file"
    },
    "water_file": {
        "type": "Keymap",
        "keys": {
            "format_version": {"types": "str"},
            "minecraft:water_settings": {"types": "dict", "structure": "water_settings", "similarity_weight": 15}
        }
    },
    "water_settings": {
        "type": "Keymap",
        "keys": {
            "description": {"types": "dict", "structure": "description", "delegate_arguments": {"always_print": true}},
            "particle_concentrations": {"types": "dict", "structure": "particle_concentrations"},
            "caustics": {"types": "dict", "structure": "caustics"},
            "waves": {"types": "dict", "structure": "waves"}
        }
    },
    "description": {
        "type": "Keymap",
        "keys": {
            "identifier": {"types": "str", "structure": "temp/string", "delegate_arguments": {"always_print": true}}
        }
    },
    "particle_concentrations": {
        "type": "Keymap",
        "delegate_arguments": {
            "field": "particle"
        },
        "keys": {
            "chlorophyll": {"types": "float"},
            "suspended_sediment": {"types": "temp/decimal"},
            "cdom": {"types": "temp/decimal"}
        }
    },
    "caustics": {
        "type": "Keymap",
        "keys": {
            "enabled": {"types": "bool"},
            "frame_length": {"types": "float"},
            "scale": {"types": "float"},
            "power": {"types": "int"}
        }
    },
    "waves": {
        "type": "Keymap",
        "keys": {
            "enabled": {"types": "bool"},
            "frequency": {"types": "int"},
            "octaves": {"types": "int"},
            "depth": {"types": "int"},
            "sampleWidth": {"types": "float"},
            "speed": {"types": "int"},
            "shape": {"types": "float"},
            "pull": {"types": "float"},
            "mix": {"types": "float"},
            "frequency_scaling": {"types": "float"},
            "speed_scaling": {"types": "float"}
        }
    }
}