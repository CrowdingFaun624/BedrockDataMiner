{
    "base": {
        "type": "Base",
        "delegate_arguments": {
            "name": "spawn_groups"
        },
        "types": "dict",
        "subcomponent": "spawn_groups"
    },
    "empty_dict": {
        "type": "Keymap",
        "keys": {}
    },
    "spawn_groups": {
        "type": "Dict",
        "delegate_arguments": {
            "field": "spawn group",
            "measure_length": true
        },
        "types": ["file"],
        "sort": "by_key",
        "subcomponent": "cache"
    },
    "cache": {
        "type": "Cache",
        "types": "file",
        "subcomponent": {
            "type": "File",
            "file_types": "file",
            "content_types": "dict",
            "subcomponent": "spawn_group"
        }
    },
    "spawn_group": {
        "type": "Keymap",
        "sort": "by_component_order",
        "keys": {
            "format_version": {"types": "str"},
            "minecraft:spawn_groups": {"types": "dict", "subcomponent": {
                "type": "Keymap",
                "sort": "by_component_order",
                "keys": {
                    "description": {"types": "dict", "subcomponent": "description"},
                    "conditions": {"types": "list", "subcomponent": "conditions"}
                }
            }}
        }
    },
    "description": {
        "type": "Keymap",
        "keys": {
            "identifier": {"types": "str", "subcomponent": {"type": "String"}, "delegate_arguments": {"always_print": true}}
        }
    },
    "conditions": {
        "type": "Set",
        "delegate_arguments": {
            "field": "condition",
            "measure_length": true
        },
        "types": ["dict"],
        "subcomponent": "condition"
    },
    "condition": {
        "type": "Keymap",
        "sort": "by_key",
        "keys": {
            "minecraft:difficulty_filter": {"types": "dict", "subcomponent": "component.difficulty_filter"},
            "minecraft:distance_filter": {"types": "dict", "subcomponent": "component.distance_filter"},
            "minecraft:herd": {"types": "dict", "subcomponent": "component.herd"},
            "minecraft:mob_event_filter": {"types": "dict", "subcomponent": "component.mob_event_filter"},
            "minecraft:permute_type": {"types": "list", "subcomponent": "component.permute_type"},
            "minecraft:spawns_on_surface": {"types": "dict", "subcomponent": "empty_dict"}
        }
    },
    "component.difficulty_filter": {
        "type": "Keymap",
        "delegate_arguments": {
            "print_all": true
        },
        "keys": {
            "max": {"types": "str"},
            "min": {"types": "str"}
        }
    },
    "component.distance_filter": {
        "type": "Keymap",
        "delegate_arguments": {
            "print_all": true
        },
        "keys": {
            "max": {"types": "int"},
            "min": {"types": "int"}
        }
    },
    "component.herd": {
        "type": "Keymap",
        "keys": {
            "event": {"types": "str", "delegate_arguments": {"always_print": true}},
            "max_size": {"types": "int"},
            "min_size": {"types": "int"}
        }
    },
    "component.mob_event_filter": {
        "type": "Keymap",
        "keys": {
            "event": {"types": "str"}
        }
    },
    "component.permute_type": {
        "type": "Set",
        "delegate_arguments": {
            "field": "permute type",
            "measure_length": true
        },
        "types": ["dict"],
        "subcomponent": "component.permute_type:item"
    },
    "component.permute_type:item": {
        "type": "Keymap",
        "keys": {
            "entity_type": {"types": "str", "subcomponent": {"type": "String"}, "delegate_arguments": {"always_print": true}},
            "weight": {"types": "int"}
        }
    }
}