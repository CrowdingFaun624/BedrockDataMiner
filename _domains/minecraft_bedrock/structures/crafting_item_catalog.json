{
    "group_aliases": {
        "s": "serializers",
        "temp": "structures/template"
    },
    "base": {
        "inherit": "temp/base_pack_files",
        "$name": "crafting_item_catalog",
        "$pack_type": "behavior pack",
        "$structure": "#@file"
    },
    "file": {
        "type": "Keymap",
        "delegate_arguments": {
            "sort": "sort_by_component_order"
        },
        "keys": {
            "format_version": {"types": "str", "similarity_weight": 0},
            "minecraft:crafting_items_catalog": {"types": "dict", "structure": "crafting_items_catalog"}
        }
    },
    "crafting_items_catalog": {
        "type": "Keymap",
        "delegate_arguments": {
            "sort": "sort_by_component_order"
        },
        "keys": {
            "categories": {"types": "list", "structure": "categories"}
        }
    },
    "categories": {
        "type": "Dict",
        "delegate_arguments": {
            "field": "category",
            "show_item_key": false,
            "measure_length": true
        },
        "key_weight": 0,
        "key_types": "int",
        "this_types": "list",
        "value_types": "dict",
        "value_structure": "category"
    },
    "category": {
        "type": "Keymap",
        "delegate_arguments": {
            "sort": "sort_by_component_order"
        },
        "keys": {
            "category_name": {"types": "str", "structure": {"type": "String"}, "similarity_weight": 5, "delegate_arguments": {"always_print": true}},
            "groups": {"types": "list", "structure": "groups"}
        }
    },
    "groups": {
        "type": "Dict",
        "delegate_arguments": {
            "field": "group",
            "show_item_key": false,
            "measure_length": true
        },
        "key_weight": 0,
        "key_types": "int",
        "this_types": "list",
        "value_types": "dict",
        "value_structure": "group"
    },
    "group": {
        "type": "Keymap",
        "delegate_arguments": {
            "sort": "sort_by_component_order"
        },
        "keys": {
            "group_identifier": {"types": "dict", "structure": "group_identifier", "delegate_arguments": {"always_print": true}},
            "items": {"types": "list", "structure": "items_list"}
        }
    },
    "group_identifier": {
        "type": "Keymap",
        "delegate_arguments": {
            "sort": "sort_by_component_order"
        },
        "keys": {
            "icon": {"types": "str", "structure": {"type": "String"}, "delegate_arguments": {"always_print": true}},
            "name": {"types": "str", "structure": {"type": "String"}, "delegate_arguments": {"always_print": true}, "similarity_weight": 2}
        }
    },
    "items_list": {
        "type": "Dict",
        "delegate_arguments": {
            "field": "item",
            "show_item_key": false,
            "measure_length": true
        },
        "key_weight": 0,
        "key_types": "int",
        "this_types": "list",
        "value_types": "str",
        "value_structure": {
            "type": "String"
        }
    }
}