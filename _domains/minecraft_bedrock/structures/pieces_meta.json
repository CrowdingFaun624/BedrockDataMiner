{
    "group_aliases": {
        "s": "serializers",
        "temp": "structures/template"
    },
    "base": {
        "inherit": "temp/base_multiple_pack_files_reversed",
        "$name": "pieces_meta",
        "$function": [],
        "$pack_type": "piece",
        "$file_field": "meta file",
        "$structure": "#@meta"
    },
    "meta": {
        "type": "Keymap",
        "keys": {
            "piece_id": {"types": "str"},
            "piece_name": {"types": "str", "structure": {"type": "String"}, "delegate_arguments": {"always_print": true}},
            "piece_type": {"types": "str"},
            "zone": {"types": "list", "structure": "zones"},
            "tint_base_color": {"types": "dict", "structure": "tint_base_color"},
            "tint_color": {"types": "dict", "structure": "tint_base_color"},
            "body_variant": {"types": "str"},
            "face_clothing_map": {"types": "str"},
            "clothing_map": {"types": "str"},
            "animation_sources": {"types": "list", "structure": "animation_sources"},
            "geometry_sources": {"types": "list", "structure": "geometry_sources"},
            "texture_sources": {"types": "list", "structure": "texture_sources"}
        }
    },
    "zones": {
        "type": "Dict",
        "key_weight": 0,
        "key_types": "int",
        "this_types": "list",
        "value_types": "str",
        "delegate_arguments": {
            "field": "zone",
            "show_item_key": false,
            "measure_length": true
        },
        "value_structure": {
            "type": "String"
        }
    },
    "texture_sources": {
        "type": "Dict",
        "key_weight": 0,
        "key_types": "int",
        "this_types": "list",
        "value_types": "dict",
        "delegate_arguments": {
            "field": "texture source",
            "show_item_key": false,
            "measure_length": true
        },
        "value_structure": "texture_source"
    },
    "texture_source": {
        "type": "Keymap",
        "keys": {
            "texture": {"types": "str", "structure": {"type": "String"}, "similarity_weight": 4, "delegate_arguments": {"always_print": true}},
            "tint_map": {"types": "str", "structure": {"type": "String"}},
            "use_face_uv": {"types": "bool"},
            "frames": {"types": "int"},
            "animated": {"types": "bool"}
        }
    },
    "tint_base_color": {
        "type": "Keymap",
        "delegate_arguments": {
            "sort": "sort_by_component_order"
        },
        "keys": {
            "r_color": {"types": "str"},
            "g_color": {"types": "str"},
            "b_color": {"types": "str"},
            "a_color": {"types": "str"}
        }
    },
    "animation_sources": {
        "type": "Dict",
        "key_weight": 0,
        "key_types": "int",
        "this_types": "list",
        "value_types": "dict",
        "delegate_arguments": {
            "field": "animation source",
            "show_item_key": false,
            "measure_length": true
        },
        "value_structure": "animation_source"
    },
    "animation_source": {
        "type": "Keymap",
        "keys": {
            "name": {"types": "str", "structure": {"type": "String"}, "similarity_weight": 4, "delegate_arguments": {"always_print": true}},
            "animationFile": {"types": "str", "structure": {"type": "String"}}
        }
    },
    "geometry_sources": {
        "type": "Dict",
        "key_weight": 0,
        "key_types": "int",
        "this_types": "list",
        "value_types": "dict",
        "delegate_arguments": {
            "field": "geometry source",
            "show_item_key": false,
            "measure_length": true
        },
        "value_structure": "geometry_source"
    },
    "geometry_source": {
        "type": "Keymap",
        "keys": {
            "arm_size": {"types": "str"},
            "geometry": {"types": "str", "structure": {"type": "String"}, "similarity_weight": 3, "delegate_arguments": {"always_print": true}},
            "zone": {"types": "str"},
            "is_face": {"types": "str"}
        }
    }
}