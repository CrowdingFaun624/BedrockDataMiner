{
    "base": {
        "type": "Base",
        "delegate_arguments": {
            "name": "pieces_meta"
        },
        "types": "dict",
        "subcomponent": "pieces"
    },
    "pieces": {
        "type": "Dict",
        "types": "dict",
        "subcomponent": "meta_files",
        "sort": "by_key",
        "delegate_arguments": {
            "field": "piece"
        }
    },
    "meta_files": {
        "type": "Dict",
        "types": "file",
        "subcomponent": "cache",
        "sort": "by_key",
        "delegate_arguments": {
            "field": "meta file"
        }
    },
    "cache": {
        "type": "Cache",
        "types": "file",
        "subcomponent": {
            "type": "File",
            "file_types": "file",
            "content_types": "dict",
            "subcomponent": "meta"
        }
    },
    "meta": {
        "type": "Keymap",
        "keys": {
            "piece_id": {"types": "str"},
            "piece_name": {"types": "str", "subcomponent": {"type": "String"}, "delegate_arguments": {"always_print": true}},
            "piece_type": {"types": "str"},
            "zone": {"types": "list", "subcomponent": "zones"},
            "tint_base_color": {"types": "dict", "subcomponent": "tint_base_color"},
            "tint_color": {"types": "dict", "subcomponent": "tint_base_color"},
            "body_variant": {"types": "str"},
            "face_clothing_map": {"types": "str"},
            "clothing_map": {"types": "str"},
            "animation_sources": {"types": "list", "subcomponent": "animation_sources"},
            "geometry_sources": {"types": "list", "subcomponent": "geometry_sources"},
            "texture_sources": {"types": "list", "subcomponent": "texture_sources"}
        }
    },
    "zones": {
        "type": "Set",
        "types": "str",
        "delegate_arguments": {
            "field": "zone",
            "measure_length": true
        },
        "subcomponent": {
            "type": "String"
        }
    },
    "texture_sources": {
        "type": "Set",
        "types": "dict",
        "delegate_arguments": {
            "field": "texture source",
            "measure_length": true
        },
        "subcomponent": "texture_source"
    },
    "texture_source": {
        "type": "Keymap",
        "keys": {
            "texture": {"types": "str", "subcomponent": {"type": "String"}, "weight": 4, "delegate_arguments": {"always_print": true}},
            "tint_map": {"types": "str", "subcomponent": {"type": "String"}},
            "use_face_uv": {"types": "bool"},
            "frames": {"types": "int"},
            "animated": {"types": "bool"}
        }
    },
    "tint_base_color": {
        "type": "Keymap",
        "sort": "by_component_order",
        "keys": {
            "r_color": {"types": "str"},
            "g_color": {"types": "str"},
            "b_color": {"types": "str"},
            "a_color": {"types": "str"}
        }
    },
    "animation_sources": {
        "type": "Set",
        "types": "dict",
        "delegate_arguments": {
            "field": "animation source",
            "measure_length": true
        },
        "subcomponent": "animation_source"
    },
    "animation_source": {
        "type": "Keymap",
        "keys": {
            "name": {"types": "str", "subcomponent": {"type": "String"}, "weight": 4, "delegate_arguments": {"always_print": true}},
            "animationFile": {"types": "str", "subcomponent": {"type": "String"}}
        }
    },
    "geometry_sources": {
        "type": "Set",
        "types": "dict",
        "delegate_arguments": {
            "field": "geometry source",
            "measure_length": true
        },
        "subcomponent": "geometry_source"
    },
    "geometry_source": {
        "type": "Keymap",
        "keys": {
            "arm_size": {"types": "str"},
            "geometry": {"types": "str", "subcomponent": {"type": "String"}, "weight": 3, "delegate_arguments": {"always_print": true}},
            "zone": {"types": "str"},
            "is_face": {"types": "str"}
        }
    }
}