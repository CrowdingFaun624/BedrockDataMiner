{
    "base": {
        "type": "Base",
        "delegate_arguments": {
            "name": "hb_touch_gestures_config"
        },
        "types": "file",
        "subcomponent": "cache"
    },
    "cache": {
        "type": "Cache",
        "types": "file",
        "subcomponent": {
            "type": "File",
            "file_types": "file",
            "content_types": "dict",
            "subcomponent": "file"
        }
    },
    "file": {
        "type": "Dict",
        "delegate_arguments": {
            "field": "gesture",
            "measure_length": true
        },
        "types": "dict",
        "sort": "by_key",
        "subcomponent": "gesture"
    },
    "gesture": {
        "type": "Keymap",
        "sort": "by_component_order",
        "keys": {
            "max_touches": {"types": "int"},
            "min_touches": {"types": "int"},
            "max_interval_between_moves_ms": {"types": "int"},
            "time_tolerance_ms": {"types": "int"},
            "distance_tolerance_cm": {"types": "float"},
            "min_distance_cm": {"types": "float", "tags": ["outdated"]}
        }
    }
}