{
    "base": {
        "type": "Base",
        "delegate_arguments": {
            "name": "watchdogs"
        },
        "types": "file",
        "subcomponent": "cache"
    },
    "cache": {
        "type": "Cache",
        "types": "file",
        "subcomponent": {
            "type": "File",
            "file_types": "file",
            "content_types": "dict",
            "subcomponent": "file"
        }
    },
    "file": {
        "type": "Keymap",
        "keys": {
            "timers": {"types": "list", "subcomponent": "timers"}
        }
    },
    "timers": {
        "type": "Set",
        "types": "dict",
        "delegate_arguments": {
            "field": "timer",
            "measure_length": true
        },
        "subcomponent": "timer"
    },
    "timer": {
        "type": "Keymap",
        "keys": {
            "timer_label": {"types": "str", "subcomponent": {"type": "String"}, "weight": 4, "delegate_arguments": {"always_print": true}},
            "timer_name": {"types": "str", "subcomponent": {"type": "String"}, "weight": 4, "delegate_arguments": {"always_print": true}},
            "timer_group": {"types": "str"},
            "watchdogs": {"types": "list", "subcomponent": "watchdogs"}
        }
    },
    "watchdogs": {
        "type": "Set",
        "types": "dict",
        "delegate_arguments": {
            "field": "watchdog",
            "measure_length": true
        },
        "subcomponent": "watchdog"
    },
    "watchdog": {
        "type": "Keymap",
        "keys": {
            "execution_budget": {"types": "str", "subcomponent": {"type": "String"}},
            "overage_actions": {"types": "list", "subcomponent": "actions"}
        }
    },
    "actions": {
        "type": "Set",
        "types": "str",
        "delegate_arguments": {
            "field": "action",
            "measure_length": true
        },
        "subcomponent": {
            "type": "String"
        }
    }
}