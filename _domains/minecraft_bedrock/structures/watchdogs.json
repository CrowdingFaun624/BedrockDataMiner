{
    "group_aliases": {
        "s": "serializers",
        "temp": "structures/template"
    },
    "base": {
        "inherit": "temp/base_single_file",
        "$name": "watchdogs",
        "$structure": "#@file"
    },
    "file": {
        "type": "Keymap",
        "keys": {
            "timers": {"types": "list", "structure": "timers"}
        }
    },
    "timers": {
        "type": "Dict",
        "key_weight": 0,
        "key_types": "int",
        "this_types": "list",
        "value_types": "dict",
        "delegate_arguments": {
            "field": "timer",
            "show_item_key": false,
            "measure_length": true
        },
        "value_structure": "timer"
    },
    "timer": {
        "type": "Keymap",
        "keys": {
            "timer_label": {"types": "str", "structure": {"type": "String"}, "similarity_weight": 4, "delegate_arguments": {"always_print": true}},
            "timer_name": {"types": "str", "structure": {"type": "String"}, "similarity_weight": 4, "delegate_arguments": {"always_print": true}},
            "timer_group": {"types": "str"},
            "watchdogs": {"types": "list", "structure": "watchdogs"}
        }
    },
    "watchdogs": {
        "type": "Dict",
        "key_weight": 0,
        "key_types": "int",
        "this_types": "list",
        "value_types": "dict",
        "delegate_arguments": {
            "field": "watchdog",
            "show_item_key": false,
            "measure_length": true
        },
        "value_structure": "watchdog"
    },
    "watchdog": {
        "type": "Keymap",
        "keys": {
            "execution_budget": {"types": "str", "structure": {"type": "String"}},
            "overage_actions": {"types": "list", "structure": "actions"}
        }
    },
    "actions": {
        "type": "Dict",
        "key_weight": 0,
        "key_types": "int",
        "this_types": "list",
        "value_types": "str",
        "delegate_arguments": {
            "field": "action",
            "show_item_key": false,
            "measure_length": true
        },
        "value_structure": {
            "type": "String"
        }
    }
}