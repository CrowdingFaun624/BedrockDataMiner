{
    "base": {
        "type": "Base",
        "delegate_arguments": {
            "name": "renderer_point_light_configs"
        },
        "types": "dict",
        "subcomponent": "files"
    },
    "files": {
        "type": "Dict",
        "delegate_arguments": {
            "field": "file",
            "measure_length": true
        },
        "types": ["file"],
        "detect_key_moves": true,
        "sort": "by_key",
        "subcomponent": "cache"
    },
    "cache": {
        "type": "Cache",
        "types": "file",
        "subcomponent": {
            "type": "File",
            "file_types": "file",
            "content_types": "dict",
            "subcomponent": "file"
        }
    },
    "file": {
        "type": "Keymap",
        "delegate_arguments": {
            "field": "setting"
        },
        "sort": "by_component_order",
        "imports": "point_light_configuration",
        "keys": {
            "low": {"types": "dict", "subcomponent": "point_light_configuration"},
            "medium": {"types": "dict", "subcomponent": "point_light_configuration"},
            "high": {"types": "dict", "subcomponent": "point_light_configuration"},
            "ultra": {"types": "dict", "subcomponent": "point_light_configuration"}
        }
    },
    "point_light_configuration": {
        "type": "Keymap",
        "sort": "by_key",
        "keys": {
            "file": {"types": "str", "subcomponent": {"type": "String"}, "delegate_arguments": {"always_print": true}},
            "grid_hierarchy_radii": {"types": "list", "subcomponent": "grid_hierarchy_radii"},
            "before_diffuse_fade_out": {"types": "float"},
            "after_diffuse_fade_out": {"types": "float"},
            "start_diffuse_fade_out": {"types": "float"},
            "end_diffuse_fade_out": {"types": "float"},
            "start_specular_fade_out": {"types": "float"},
            "end_specular_fade_out": {"types": "float"},
            "enable_fade_out_diffuse": {"types": "bool"},
            "enable_fade_out_specular": {"types": "bool"}
        }
    },
    "grid_hierarchy_radii": {
        "type": "List",
        "delegate_arguments": {
            "field": "radius",
            "measure_length": true
        },
        "types": ["float"],
        "subcomponent": null
    }
}