{
    "group_aliases": {
        "s": "serializers",
        "temp": "structures/template"
    },
    "base": {
        "inherit": "temp/base_multiple_files",
        "$name": "disconnection_error_messaging",
        "$structure": "#@file"
    },
    "file": {
        "type": "Keymap",
        "keys": {
            "FallbackStrings": {"types": "dict", "structure": "error"},
            "Themes": {"types": "list", "structure": "themes"}
        }
    },
    "themes": {
        "type": "Normalizer",
        "outer_types": "list",
        "inner_types": "dict",
        "functions": {
            "function_name": "wrap_in_dict",
            "arguments": {
                "key": "ThemeName",
                "delete": true
            }
        },
        "structure": {
            "type": "Dict",
            "delegate_arguments": {
                "field": "theme",
                "measure_length": true
            },
            "value_types": "dict",
            "value_structure": "theme"
        }
    },
    "theme": {
        "type": "Keymap",
        "keys": {
            "DefaultStrings": {"types": "dict", "structure": "error"},
            "Errors": {"types": "list", "structure": "errors"}
        }
    },
    "errors": {
        "type": "Normalizer",
        "outer_types": "list",
        "inner_types": "dict",
        "functions": {
            "function_name": "turn_to_dict",
            "arguments": {
                "key_key": "FailReason",
                "value_key": "OverrideStrings",
                "default": {}
            }
        },
        "structure": {
            "type": "Dict",
            "delegate_arguments": {
                "field": "error"
            },
            "value_types": "dict",
            "value_structure": "error"
        }
    },
    "error": {
        "type": "Keymap",
        "keys": {
            "Codeword": {"types": "str", "structure": {"type": "String"}, "delegate_arguments": {"always_print": true}},
            "Title": {"types": "str", "delegate_arguments": {"always_print": true}},
            "Body": {"types": "str"},
            "WebUrl": {"types": "str"}
        }
    }
}