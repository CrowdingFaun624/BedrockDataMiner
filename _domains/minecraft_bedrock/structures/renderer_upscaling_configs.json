{
    "base": {
        "type": "Base",
        "types": "dict",
        "delegate_arguments": {
            "name": "renderer_upscaling_configs"
        },
        "subcomponent": "files"
    },
    "files": {
        "type": "Dict",
        "sort": "by_key",
        "key_component": {"type": "String"},
        "delegate_arguments": {
            "field": "file",
            "measure_length": true
        },
        "types": "file",
        "subcomponent": "cache"
    },
    "cache": {
        "type": "Cache",
        "types": "file",
        "subcomponent": {
            "type": "File",
            "file_types": "file",
            "content_types": "dict",
            "subcomponent": "file"
        }
    },
    "file": {
        "type": "Keymap",
        "keys": {
            "upscaling_configuration": {"types": "dict", "subcomponent": "file_link"},
            "target_resolution": {"types": "dict", "subcomponent": "target_resolution"}
        }
    },
    "file_link": {
        "type": "Keymap",
        "keys": {
            "file": {"types": "str", "subcomponent": {"type": "String"}, "delegate_arguments": {"always_print": true}}
        }
    },
    "target_resolution": {
        "type": "Keymap",
        "keys": {
            "x": {"types": "int"},
            "y": {"types": "int"}
        }
    }
}