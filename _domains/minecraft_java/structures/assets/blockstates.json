{
    "base": {
        "type": "StructureBase",
        "this_types": "dict",
        "delegate_arguments": {
            "name": "blockstates"
        },
        "structure": "resource_packs"
    },
    "resource_packs": {
        "type": "Dict",
        "value_types": "dict",
        "delegate_arguments": {
            "field": "resource pack",
            "measure_length": true
        },
        "value_structure": "files"
    },
    "files": {
        "type": "Dict",
        "value_types": "dict",
        "delegate_arguments": {
            "field": "file",
            "measure_length": true
        },
        "value_structure": "cache"
    },
    "cache": {
        "type": "Cache",
        "this_types": "dict",
        "pre_normalized_types": "file",
        "structure": {
            "type": "File",
            "serializer": "minecraft_common!json",
            "file_types": "file",
            "content_types": "dict",
            "structure": "root"
        }
    },
    "blockstate_condition": {
        "type": "Dict",
        "value_types": ["str", "list"],
        "delegate_arguments": {
            "field": "state",
            "measure_length": true
        },
        "key_structure": "string",
        "value_structure": {
            "type": "Union",
            "substructures": {
                "str": "string",
                "list": "blockstate_conditions"
            }
        }
    },
    "blockstate_conditions": {
        "type": "Dict",
        "key_weight": 0,
        "key_types": "int",
        "this_types": "list",
        "value_types": "dict",
        "delegate_arguments": {
            "field": "condition",
            "show_item_key": false,
            "measure_length": true
        },
        "value_structure": "blockstate_condition"
    },
    "string": {
        "type": "String"
    },
    "root": {
        "type": "Keymap",
        "delegate_arguments": {
            "sort": "sort_by_component_order"
        },
        "keys": {
            "multipart": {"types": "list", "structure": "multipart"},
            "variants": {"types": "dict", "structure": "variants"}
        }
    },
    "multipart": {
        "type": "Dict",
        "key_weight": 0,
        "key_types": "int",
        "this_types": "list",
        "value_types": "dict",
        "delegate_arguments": {
            "field": "rule",
            "show_item_key": false,
            "measure_length": true
        },
        "value_structure": "rule"
    },
    "rule": {
        "type": "Keymap",
        "delegate_arguments": {
            "sort": "sort_by_component_order"
        },
        "keys": {
            "apply": {"types": ["dict", "list"], "structure": {
                "type": "Union",
                "substructures": {
                    "dict": "variant",
                    "list": "variants_list"
                }
            }},
            "when": {"types": "dict", "structure": "blockstate_condition"}
        }
    },
    "variants": {
        "type": "Dict",
        "value_types": ["dict", "list"],
        "delegate_arguments": {
            "field": "variant",
            "measure_length": true
        },
        "key_structure": "string",
        "value_structure": "variants_group"
    },
    "variants_list": {
        "type": "Dict",
        "key_weight": 0,
        "key_types": "int",
        "this_types": "list",
        "value_types": ["dict", "list"],
        "delegate_arguments": {
            "field": "variant",
            "show_item_key": false,
            "measure_length": true
        },
        "value_structure": "variants_group"
    },
    "variants_group": {
        "type": "Union",
        "substructures": {
            "dict": "variant",
            "list": "subvariants"
        }
    },
    "subvariants": {
        "type": "Dict",
        "key_weight": 0,
        "key_types": "int",
        "this_types": "list",
        "value_types": "dict",
        "delegate_arguments": {
            "field": "subvariant",
            "show_item_key": false,
            "measure_length": true
        },
        "value_structure": "variant"
    },
    "variant": {
        "type": "Keymap",
        "delegate_arguments": {
            "sort": "sort_by_component_order"
        },
        "keys": {
            "model": {"types": "str", "structure": "string", "delegate_arguments": {"always_print": true}},
            "x": {"types": "int"},
            "y": {"types": "int"},
            "uvlock": {"types": "bool"},
            "weight": {"types": "int"}
        }
    }
}