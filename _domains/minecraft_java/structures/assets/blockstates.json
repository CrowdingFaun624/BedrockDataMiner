{
    "group_aliases": {
        "block_models": "structures/assets/block_models",
        "misc": "structures/misc/misc",
        "t": "structure_tags",
        "temp": "structures/misc/template",
        "s": "serializers",
        "vector": "structures/misc/vector"
    },
    "base": {
        "type": "StructureBase",
        "this_types": "dict",
        "delegate_arguments": {
            "name": "blockstates"
        },
        "structure": "resource_packs"
    },
    "resource_packs": {
        "type": "Dict",
        "value_types": "dict",
        "delegate_arguments": {
            "field": "resource pack",
            "measure_length": true
        },
        "value_structure": "files"
    },
    "files": {
        "type": "Dict",
        "value_types": "file",
        "delegate_arguments": {
            "field": "file",
            "measure_length": true
        },
        "value_structure": "cache"
    },
    "cache": {
        "type": "Cache",
        "this_types": "file",
        "structure": {
            "type": "Condition",
            "substructures": [
                {"types": "file", "filter": {"type": "EqFilter", "key": "fix_json", "value": false, "default": true}, "structure": "#!file{serializer=@minecraft_common!s/json}"},
                {"types": "file", "filter": {"type": "EqFilter", "key": "fix_json", "value": true}, "structure": "#!file{serializer=@s/blockstates_fixer}"}
            ]
        }
    },
    "file": {
        "type": "File",
        "serializer": "#serializer",
        "outer_types": "file",
        "inner_types": "dict",
        "structure": "root"
    },
    "blockstate_condition": {
        "type": "Dict",
        "value_types": ["str", "list", "bool"],
        "delegate_arguments": {
            "field": "state",
            "measure_length": true
        },
        "key_structure": "misc/string",
        "value_structure": {
            "type": "Union",
            "substructures": {
                "bool": null, // outdated
                "str": "misc/string",
                "list": "blockstate_conditions"
            }
        }
    },
    "blockstate_conditions": {
        "type": "Dict",
        "key_weight": 0,
        "key_types": "int",
        "this_types": "list",
        "value_types": "dict",
        "delegate_arguments": {
            "field": "condition",
            "show_item_key": false,
            "measure_length": true
        },
        "value_structure": "blockstate_condition"
    },
    "root": {
        "inherit": "temp/SortedKeymap",
        "keys": {
            "__comment": {"types": "str", "tags": "t/outdated"},
            ",facing=_comment": {"types": "str", "tags": "t/outdated"},
            "multipart": {"types": "list", "structure": "multipart"},
            "variants": {"types": "dict", "structure": "variants"},
            "name": {"types": "str", "delegate_arguments": {"always_print": true}, "tags": "t/outdated"},
            "inheritFrom": {"types": "str", "tags": "t/outdated"},
            "inventoryRender3D": {"types": "bool", "tags": "t/outdated"},
            "randomOffsetX": {"types": "bool", "tags": "t/outdated"},
            "randomOffsetY": {"types": "bool", "tags": "t/outdated"},
            "randomOffsetZ": {"types": "bool", "tags": "t/outdated"},
            "rotation": {"types": "list", "structure": "vector/vector.decimal", "tags": "t/outdated"},
            "rotationOrigin": {"types": "list", "structure": "vector/vector.decimal", "tags": "t/outdated"},
            "translation": {"types": "list", "structure": "vector/vector.decimal", "tags": "t/outdated"},
            "useAmbientOcclusion": {"types": "bool", "tags": "t/outdated"},
            "elements": {"types": "list", "structure": "block_models/elements", "tags": "t/outdated"},
            "faces": {"types": "list", "structure": "faces", "tags": "t/outdated"}
        }
    },
    "multipart": {
        "type": "Dict",
        "key_weight": 0,
        "key_types": "int",
        "this_types": "list",
        "value_types": "dict",
        "delegate_arguments": {
            "field": "rule",
            "show_item_key": false,
            "measure_length": true
        },
        "value_structure": "rule"
    },
    "rule": {
        "inherit": "temp/SortedKeymap",
        "keys": {
            "apply": {"types": ["dict", "list"], "structure": {
                "type": "Union",
                "substructures": {
                    "dict": "variant",
                    "list": "variants_list"
                }
            }},
            "when": {"types": "dict", "structure": "blockstate_condition"}
        }
    },
    "variants": {
        "type": "Dict",
        "value_types": ["dict", "list"],
        "delegate_arguments": {
            "field": "variant",
            "measure_length": true
        },
        "key_structure": "misc/string",
        "value_structure": "variants_group"
    },
    "variants_list": {
        "type": "Dict",
        "key_weight": 0,
        "key_types": "int",
        "this_types": "list",
        "value_types": ["dict", "list"],
        "delegate_arguments": {
            "field": "variant",
            "show_item_key": false,
            "measure_length": true
        },
        "value_structure": "variants_group"
    },
    "variants_group": {
        "type": "Union",
        "substructures": {
            "dict": "variant",
            "list": "subvariants"
        }
    },
    "subvariants": {
        "type": "Dict",
        "key_weight": 0,
        "key_types": "int",
        "this_types": "list",
        "value_types": "dict",
        "delegate_arguments": {
            "field": "subvariant",
            "show_item_key": false,
            "measure_length": true
        },
        "value_structure": "variant"
    },
    "variant": {
        "inherit": "temp/SortedKeymap",
        "keys": {
            "model": {"types": "str", "structure": "misc/string", "delegate_arguments": {"always_print": true}},
            "x": {"types": "int"},
            "y": {"types": "int"},
            "uvlock": {"types": "bool"},
            "weight": {"types": "int"}
        }
    },
    "faces": {
        "inherit": "temp/Set",
        "$field": "face",
        "value_types": "dict",
        "value_structure": "face"
    },
    "face": {
        "inherit": "temp/SortedKeymap",
        "keys": {
            "overlay": {"types": "bool", "tags": "t/outdated"},
            "shade": {"types": "float", "tags": "t/outdated"},
            "tint": {"types": "bool", "tags": "t/outdated"},
            "textureFacing": {"types": "str", "tags": "t/outdated"},
            "cullFacing": {"types": "str"},
            "vertices": {"types": "list", "structure": "vertices"}
        }
    },
    "vertices": {
        "inherit": "temp/Sequence",
        "$field": "vertex",
        "value_types": "dict",
        "value_structure": "vertex"
    },
    "vertex": {
        "inherit": "temp/SortedKeymap",
        "keys": {
            "position": {"types": "list", "structure": "vector/vector.float"},
            "color": {"types": "list", "structure": "vector/vector.float"},
            "texcoord": {"types": "list", "structure": "vector/vector.decimal"}
        }
    }
}