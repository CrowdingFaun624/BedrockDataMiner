{
    "base": {
        "type": "StructureBase",
        "this_types": "dict",
        "delegate_arguments": {
            "name": "post_effect"
        },
        "structure": "resource_packs"
    },
    "decimal": {
        "type": "TypeAlias",
        "types": ["float", "int"]
    },
    "resource_packs": {
        "type": "Dict",
        "value_types": "dict",
        "delegate_arguments": {
            "field": "resource pack",
            "measure_length": true
        },
        "value_structure": "files"
    },
    "files": {
        "type": "Dict",
        "value_types": "dict",
        "delegate_arguments": {
            "field": "file",
            "measure_length": true
        },
        "value_structure": "cache"
    },
    "cache": {
        "type": "Cache",
        "this_types": "dict",
        "pre_normalized_types": "file",
        "structure": {
            "type": "File",
            "serializer": "minecraft_common!json",
            "file_types": "file",
            "content_types": "dict",
            "structure": "root"
        }
    },
    "string": {
        "type": "String"
    },
    "vector_decimal": {
        "type": "Dict",
        "allow_key_moves": false,
        "key_weight": 0,
        "key_types": "int",
        "this_types": "list",
        "value_types": "decimal",
        "delegate_arguments": {
            "print_all": true,
            "print_flat": true,
            "field": "component"
        },
        "value_structure": null
    },
    "root": {
        "type": "Keymap",
        "delegate_arguments": {
            "sort": "sort_by_component_order"
        },
        "keys": {
            "targets": {"types": "dict", "structure": "targets"},
            "passes": {"types": "list", "structure": "passes"}
        }
    },
    "targets": {
        "type": "Dict",
        "value_types": "dict",
        "delegate_arguments": {
            "field": "target",
            "measure_length": true
        },
        "key_structure": "string",
        "value_structure": "target"
    },
    "target": {
        "type": "Keymap",
        "delegate_arguments": {
            "sort": "sort_by_component_order"
        },
        "keys": {
            "width": {"types": "int"},
            "height": {"types": "int"}
        }
    },
    "passes": {
        "type": "Dict",
        "key_weight": 0,
        "key_types": "int",
        "this_types": "list",
        "value_types": "dict",
        "delegate_arguments": {
            "field": "pass",
            "show_item_key": false,
            "measure_length": true
        },
        "value_structure": "pass"
    },
    "pass": {
        "type": "Keymap",
        "delegate_arguments": {
            "sort": "sort_by_component_order"
        },
        "keys": {
            "program": {"types": "str", "structure": "string", "delegate_arguments": {"always_print": true}},
            "inputs": {"types": "list", "structure": "inputs"},
            "output": {"types": "str", "structure": "string"},
            "uniforms": {"types": "list", "structure": "uniforms"}
        }
    },
    "inputs": {
        "type": "Dict",
        "key_weight": 0,
        "key_types": "int",
        "this_types": "list",
        "value_types": "dict",
        "delegate_arguments": {
            "field": "input",
            "show_item_key": false,
            "measure_length": true
        },
        "value_structure": "input"
    },
    "input": {
        "type": "Keymap",
        "delegate_arguments": {
            "sort": "sort_by_component_order"
        },
        "keys": {
            "sampler_name": {"types": "str", "structure": "string", "delegate_arguments": {"always_print": true}},
            "bilinear": {"types": "bool"},
            "target": {"types": "str"},
            "use_depth_buffer": {"types": "bool"},
            "location": {"types": "str", "structure": "string"},
            "width": {"types": "int"},
            "height": {"types": "int"}
        }
    },
    "uniforms": {
        "type": "Dict",
        "key_weight": 0,
        "key_types": "int",
        "this_types": "list",
        "value_types": "dict",
        "delegate_arguments": {
            "field": "uniform",
            "show_item_key": false,
            "measure_length": true
        },
        "value_structure": "uniform"
    },
    "uniform": {
        "type": "Switch",
        "switch_function": {
            "function_name": "normalizers/normalizers post_effect_switch"
        },
        "substructures": {
            "number": {"types": "dict", "structure": "uniform.number"},
            "matrix": {"types": "dict", "structure": "uniform.matrix"}
        }
    },
    "uniform.number": {
        "type": "Dict",
        "value_types": "decimal",
        "delegate_arguments": {
            "field": "value",
            "measure_length": true
        },
        "key_structure": "string",
        "value_structure": null
    },
    "uniform.matrix": {
        "type": "Keymap",
        "delegate_arguments": {
            "sort": "sort_by_component_order"
        },
        "keys": {
            "name": {"types": "str", "structure": "string", "delegate_arguments": {"always_print": true}},
            "values": {"types": "list", "structure": "vector_decimal"},
            "RadiusMultiplier": {"types": "decimal"}
        }
    }
}