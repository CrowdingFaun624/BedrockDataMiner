{
    "group_aliases": {
        "misc": "structures/misc/misc",
        "temp": "structures/misc/template",
        "s": "serializers",
        "t": "structure_tags"
    },
    "base": {
        "inherit": "temp/base_multiple_pack_files",
        "$name": "post_effect",
        "$pack_type": "resource pack",
        "$structure": "#@root"
    },
    "vector_decimal": {
        "type": "Dict",
        "allow_key_moves": false,
        "key_weight": 0,
        "key_types": "int",
        "this_types": "list",
        "value_types": "misc/decimal",
        "delegate_arguments": {
            "print_all": true,
            "print_flat": true,
            "field": "component"
        },
        "value_structure": null
    },
    "root": {
        "inherit": "temp/SortedKeymap",
        "keys": {
            "targets": {"types": "dict", "structure": "targets"},
            "passes": {"types": "list", "structure": "passes"}
        }
    },
    "targets": {
        "type": "Dict",
        "value_types": "dict",
        "delegate_arguments": {
            "field": "target",
            "measure_length": true
        },
        "key_structure": "misc/string",
        "value_structure": "target"
    },
    "target": {
        "inherit": "temp/SortedKeymap",
        "keys": {
            "width": {"types": "int"},
            "height": {"types": "int"}
        }
    },
    "passes": {
        "type": "Dict",
        "key_weight": 0,
        "key_types": "int",
        "this_types": "list",
        "value_types": "dict",
        "delegate_arguments": {
            "field": "pass",
            "show_item_key": false,
            "measure_length": true
        },
        "value_structure": "pass"
    },
    "pass": {
        "inherit": "temp/SortedKeymap",
        "keys": {
            "program": {"types": "str", "structure": "misc/string", "delegate_arguments": {"always_print": true}},
            "fragment_shader": {"types": "str", "structure": "misc/string"},
            "inputs": {"types": "list", "structure": "inputs"},
            "output": {"types": "str", "structure": "misc/string"},
            "uniforms": {"types": ["list", "dict"], "structure": "uniforms_union"},
            "vertex_shader": {"types": "str", "structure": "misc/string"}
        }
    },
    "inputs": {
        "type": "Dict",
        "key_weight": 0,
        "key_types": "int",
        "this_types": "list",
        "value_types": "dict",
        "delegate_arguments": {
            "field": "input",
            "show_item_key": false,
            "measure_length": true
        },
        "value_structure": "input"
    },
    "input": {
        "inherit": "temp/SortedKeymap",
        "keys": {
            "sampler_name": {"types": "str", "structure": "misc/string", "delegate_arguments": {"always_print": true}},
            "bilinear": {"types": "bool"},
            "target": {"types": "str"},
            "use_depth_buffer": {"types": "bool"},
            "location": {"types": "str", "structure": "misc/string"},
            "width": {"types": "int"},
            "height": {"types": "int"}
        }
    },
    "uniforms_union": {
        "type": "Union",
        "substructures": {
            "list": "uniforms",
            "dict": {
                "type": "Keymap",
                "keys": {
                    "BitsConfig": {"types": "list", "structure": "uniforms"},
                    "BlitConfig": {"types": "list", "structure": "uniforms"},
                    "BlurConfig": {"types": "list", "structure": "uniforms"},
                    "ColorConfig": {"types": "list", "structure": "uniforms"},
                    "InvertConfig": {"types": "list", "structure": "uniforms"},
                    "RotScaleConfig": {"types": "list", "structure": "uniforms"},
                    "SpiderConfig": {"types": "list", "structure": "uniforms"}
                }
            }
        }
    },
    "uniforms": {
        "type": "Dict",
        "key_weight": 0,
        "key_types": "int",
        "this_types": "list",
        "value_types": "dict",
        "delegate_arguments": {
            "field": "uniform",
            "show_item_key": false,
            "measure_length": true
        },
        "value_structure": "uniform"
    },
    "uniform": {
        "type": "Switch",
        "switch_function": {
            "function_name": "normalizers/normalizers post_effect_choose_uniform"
        },
        "substructures": {
            "float": {"types": "dict", "structure": "uniform.float"},
            "vec2": {"types": "dict", "structure": "uniform.matrix"},
            "vec3": {"types": "dict", "structure": "uniform.matrix"},
            "vec4": {"types": "dict", "structure": "uniform.matrix"}
        }
    },
    "uniform.float": {
        "inherit": "temp/SortedKeymap",
        "keys": {
            "type": {"types": "str", "delegate_arguments": {"always_print": true}},
            "name": {"types": "str", "structure": "misc/string", "delegate_arguments": {"always_print": true}},
            "value": {"types": "misc/decimal"},
            "values": {"types": "list", "structure": "vector_decimal", "tags": "t/outdated"}
        }
    },
    "uniform.matrix": {
        "inherit": "temp/SortedKeymap",
        "keys": {
            "type": {"types": "str", "delegate_arguments": {"always_print": true}},
            "name": {"types": "str", "structure": "misc/string", "delegate_arguments": {"always_print": true}},
            "value": {"types": "list", "structure": "vector_decimal"},
            "values": {"types": "list", "structure": "vector_decimal", "tags": "t/outdated"},
            "RadiusMultiplier": {"types": "float", "tags": "t/outdated"}
        }
    }
}