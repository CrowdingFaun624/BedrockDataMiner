{
    "group_aliases": {
        "misc": "structures/misc/misc",
        "temp": "structures/misc/template",
        "s": "serializers"
    },
    "base": {
        "inherit": "temp/base_multiple_pack_files",
        "$name": "configuration",
        "$pack_type": "resource pack",
        "$structure": "#@root"
    },
    "vector_decimal": {
        "type": "Dict",
        "allow_key_moves": false,
        "key_weight": 0,
        "key_types": "int",
        "this_types": "list",
        "value_types": "misc/decimal",
        "delegate_arguments": {
            "print_all": true,
            "print_flat": true,
            "field": "component"
        },
        "value_structure": null
    },
    "root": {
        "inherit": "temp/SortedKeymap",
        "keys": {
            "vertex": {"types": "str", "structure": "misc/string"},
            "fragment": {"types": "str", "structure": "misc/string"},
            "blend": {"types": "dict", "structure": "blend"},
            "defines": {"types": "dict", "structure": "defines"},
            "samplers": {"types": "list", "structure": "samplers"},
            "uniforms": {"types": "list", "structure": "uniforms"}
        }
    },
    "blend": {
        "inherit": "temp/SortedKeymap",
        "keys": {
            "func": {"types": "str", "delegate_arguments": {"always_print": true}},
            "srcrgb": {"types": "str"},
            "dstrgb": {"types": "str"}
        }
    },
    "defines": {
        "inherit": "temp/SortedKeymap",
        "keys": {
            "values": {"types": "dict", "structure": "values"},
            "flags": {"types": "list", "structure": "flags"}
        }
    },
    "values": {
        "type": "Dict",
        "value_types": "str",
        "delegate_arguments": {
            "field": "value",
            "measure_length": true
        },
        "key_structure": "misc/string",
        "value_structure": "misc/string"
    },
    "flags": {
        "type": "Dict",
        "key_weight": 0,
        "key_types": "int",
        "this_types": "list",
        "value_types": "str",
        "delegate_arguments": {
            "field": "flag",
            "show_item_key": false,
            "measure_length": true
        },
        "value_structure": "misc/string"
    },
    "samplers": {
        "type": "Dict",
        "key_weight": 0,
        "key_types": "int",
        "this_types": "list",
        "value_types": "dict",
        "delegate_arguments": {
            "field": "sampler",
            "show_item_key": false,
            "measure_length": true
        },
        "value_structure": "sampler"
    },
    "sampler": {
        "inherit": "temp/SortedKeymap",
        "keys": {
            "name": {"types": "str", "structure": "misc/string", "delegate_arguments": {"always_print": true}}
        }
    },
    "uniforms": {
        "type": "Dict",
        "key_weight": 0,
        "key_types": "int",
        "this_types": "list",
        "value_types": "dict",
        "delegate_arguments": {
            "field": "uniform",
            "show_item_key": false,
            "measure_length": true
        },
        "value_structure": "uniform"
    },
    "uniform": {
        "inherit": "temp/SortedKeymap",
        "keys": {
            "name": {"types": "str", "structure": "misc/string", "delegate_arguments": {"always_print": true}},
            "type": {"types": "str", "delegate_arguments": {"always_print": true}},
            "count": {"types": "int"},
            "values": {"types": "list", "structure": "vector_decimal"}
        }
    }
}