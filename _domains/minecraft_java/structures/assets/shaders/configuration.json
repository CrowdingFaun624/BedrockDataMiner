{
    "base": {
        "type": "Base",
        "types": "dict",
        "delegate_arguments": {
            "name": "configuration"
        },
        "subcomponent": "resource_packs"
    },
    "decimal": {
        "type": "TypeAlias",
        "types": ["float", "int"]
    },
    "resource_packs": {
        "type": "Dict",
        "types": "dict",
        "delegate_arguments": {
            "field": "resource pack",
            "measure_length": true
        },
        "subcomponent": "files"
    },
    "files": {
        "type": "Dict",
        "types": "file",
        "delegate_arguments": {
            "field": "file",
            "measure_length": true
        },
        "subcomponent": "cache"
    },
    "cache": {
        "type": "Cache",
        "types": "file",
        "subcomponent": {
            "type": "File",
            "file_types": "file",
            "content_types": "dict",
            "subcomponent": "root"
        }
    },
    "string": {
        "type": "String"
    },
    "vector_decimal": {
        "type": "List",
        "types": "decimal",
        "delegate_arguments": {
            "print_all": true,
            "print_flat": true,
            "field": "component"
        },
        "subcomponent": null
    },
    "root": {
        "type": "Keymap",
        "sort": "by_component_order",
        "keys": {
            "vertex": {"types": "str", "subcomponent": "string"},
            "fragment": {"types": "str", "subcomponent": "string"},
            "blend": {"types": "dict", "subcomponent": "blend"},
            "defines": {"types": "dict", "subcomponent": "defines"},
            "samplers": {"types": "list", "subcomponent": "samplers"},
            "uniforms": {"types": "list", "subcomponent": "uniforms"}
        }
    },
    "blend": {
        "type": "Keymap",
        "sort": "by_component_order",
        "keys": {
            "func": {"types": "str", "delegate_arguments": {"always_print": true}},
            "srcrgb": {"types": "str"},
            "dstrgb": {"types": "str"}
        }
    },
    "defines": {
        "type": "Keymap",
        "sort": "by_component_order",
        "keys": {
            "values": {"types": "dict", "subcomponent": "values"},
            "flags": {"types": "list", "subcomponent": "flags"}
        }
    },
    "values": {
        "type": "Dict",
        "types": "str",
        "delegate_arguments": {
            "field": "value",
            "measure_length": true
        },
        "key_component": "string",
        "subcomponent": "string"
    },
    "flags": {
        "type": "Set",
        "types": "str",
        "delegate_arguments": {
            "field": "flag",
            "measure_length": true
        },
        "subcomponent": "string"
    },
    "samplers": {
        "type": "Set",
        "types": "dict",
        "delegate_arguments": {
            "field": "sampler",
            "measure_length": true
        },
        "subcomponent": "sampler"
    },
    "sampler": {
        "type": "Keymap",
        "sort": "by_component_order",
        "keys": {
            "name": {"types": "str", "subcomponent": "string", "delegate_arguments": {"always_print": true}}
        }
    },
    "uniforms": {
        "type": "Set",
        "types": "dict",
        "delegate_arguments": {
            "field": "uniform",
            "measure_length": true
        },
        "subcomponent": "uniform"
    },
    "uniform": {
        "type": "Keymap",
        "sort": "by_component_order",
        "keys": {
            "name": {"types": "str", "subcomponent": "string", "delegate_arguments": {"always_print": true}},
            "type": {"types": "str", "delegate_arguments": {"always_print": true}},
            "count": {"types": "int"},
            "values": {"types": "list", "subcomponent": "vector_decimal"}
        }
    }
}