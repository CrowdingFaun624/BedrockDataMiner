{
    "group_aliases": {
        "misc": "structures/misc/misc",
        "temp": "structures/misc/template",
        "s": "serializers"
    },
    "base": {
        "inherit": "temp/base_multiple_pack_files",
        "$name": "configuration",
        "$pack_type": "resource pack",
        "$structure": "#@root"
    },
    "vector_decimal": {
        "type": "Dict",
        "allow_key_moves": false,
        "key_weight": 0,
        "key_types": "int",
        "this_types": "list",
        "value_types": "misc/decimal",
        "delegate_arguments": {
            "print_all": true,
            "print_flat": true,
            "field": "component"
        },
        "value_structure": null
    },
    "root": {
        "inherit": "temp/SortedKeymap",
        "keys": {
            "attributes": {"types": "list", "structure": "#!misc/named_list{name='attribute'}"},
            "targets": {"types": ["dict", "list"], "structure": "targets"},
            "passes": {"types": "list", "structure": "passes"},
            "vertex": {"types": "str", "structure": "misc/string"},
            "fragment": {"types": "str", "structure": "misc/string"},
            "blend": {"types": "dict", "structure": "blend"},
            "defines": {"types": "dict", "structure": "defines"},
            "samplers": {"types": "list", "structure": "samplers"},
            "uniforms": {"types": "list", "structure": "uniforms"}
        }
    },
    "targets": {
        "type": "Union",
        "substructures": {
            "list": "#!misc/named_list{name='target'}",
            "dict": {
                "inherit": "temp/SortedKeymap",
                "keys": {
                    "swap": {"types": "dict", "structure": "misc/empty_dict"},
                    "large_blur": {"types": "dict", "structure": "misc/empty_dict"},
                    "small_blur": {"types": "dict", "structure": "misc/empty_dict"},
                    "temp": {"types": "dict", "structure": "misc/empty_dict"},
                    "final": {"types": "dict", "structure": "misc/empty_dict"}
                }
            }
        }
    },
    "passes": {
        "inherit": "temp/Sequence",
        "$field": "pass",
        "value_types": "dict",
        "value_structure": "pass"
    },
    "pass": {
        "inherit": "temp/SortedKeymap",
        "keys": {
            "name": {"types": "str", "structure": "misc/string", "delegate_arguments": {"always_print": true}},
            "auxtargets": {"types": "list", "structure": "aux_targets"},
            "inputs": {"types": "list", "structure": "inputs"},
            "intarget": {"types": "str"},
            "use_linear_filter": {"types": "bool"},
            "output": {"types": "str"},
            "outtarget": {"types": "str"},
            "uniforms": {"types": "list", "structure": "uniforms"}
        }
    },
    "aux_targets": {
        "inherit": "temp/Sequence",
        "$field": "aux target",
        "value_types": "dict",
        "value_structure": "aux_target"
    },
    "aux_target": {
        "inherit": "temp/SortedKeymap",
        "keys": {
            "name": {"types": "str", "structure": "misc/string", "delegate_arguments": {"always_print": true}},
            "id": {"types": "str", "structure": "misc/string", "delegate_arguments": {"always_print": true}},
            "width": {"types": "int"},
            "height": {"types": "int"},
            "bilinear": {"types": "bool"}
        }
    },
    "inputs": {
        "inherit": "temp/Sequence",
        "$field": "input",
        "value_types": "dict",
        "value_structure": "input"
    },
    "input": {
        "inherit": "temp/SortedKeymap",
        "keys": {
            "sampler_name": {"types": "str", "structure": "misc/string", "delegate_arguments": {"always_print": true}},
            "target": {"types": "str"},
            "bilinear": {"types": "bool"},
            "use_depth_buffer": {"types": "bool"}
        }
    },
    "blend": {
        "inherit": "temp/SortedKeymap",
        "keys": {
            "func": {"types": "str", "delegate_arguments": {"always_print": true}},
            "srcrgb": {"types": "str"},
            "dstrgb": {"types": "str"}
        }
    },
    "defines": {
        "inherit": "temp/SortedKeymap",
        "keys": {
            "values": {"types": "dict", "structure": "values"},
            "flags": {"types": "list", "structure": "flags"}
        }
    },
    "values": {
        "inherit": "temp/Dict",
        "$field": "value",
        "value_types": "str",
        "key_structure": "misc/string",
        "value_structure": "misc/string"
    },
    "flags": {
        "inherit": "temp/Set",
        "$field": "flag",
        "value_types": "str",
        "value_structure": "misc/string"
    },
    "samplers": {
        "inherit": "temp/Sequence",
        "$field": "sampler",
        "value_types": "dict",
        "value_structure": "sampler"
    },
    "sampler": {
        "inherit": "temp/SortedKeymap",
        "keys": {
            "name": {"types": "str", "structure": "misc/string", "delegate_arguments": {"always_print": true}}
        }
    },
    "uniforms": {
        "inherit": "temp/Sequence",
        "$field": "uniform",
        "value_types": "dict",
        "value_structure": "uniform"
    },
    "uniform": {
        "inherit": "temp/SortedKeymap",
        "keys": {
            "name": {"types": "str", "structure": "misc/string", "delegate_arguments": {"always_print": true}},
            "type": {"types": "str", "delegate_arguments": {"always_print": true}},
            "count": {"types": "int"},
            "RadiusMultiplier": {"types": "float"},
            "values": {"types": "list", "structure": "vector_decimal"}
        }
    }
}