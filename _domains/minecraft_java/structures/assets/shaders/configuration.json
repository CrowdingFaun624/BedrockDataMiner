{
    "base": {
        "type": "StructureBase",
        "this_types": "dict",
        "delegate_arguments": {
            "name": "configuration"
        },
        "structure": "resource_packs"
    },
    "decimal": {
        "type": "TypeAlias",
        "types": ["float", "int"]
    },
    "resource_packs": {
        "type": "Dict",
        "value_types": "dict",
        "delegate_arguments": {
            "field": "resource pack",
            "measure_length": true
        },
        "value_structure": "files"
    },
    "files": {
        "type": "Dict",
        "value_types": "dict",
        "delegate_arguments": {
            "field": "file",
            "measure_length": true
        },
        "value_structure": "cache"
    },
    "cache": {
        "type": "Cache",
        "this_types": "dict",
        "pre_normalized_types": "file",
        "structure": {
            "type": "File",
            "serializer": "minecraft_common!json",
            "file_types": "file",
            "content_types": "dict",
            "structure": "root"
        }
    },
    "string": {
        "type": "String"
    },
    "vector_decimal": {
        "type": "Dict",
        "allow_key_moves": false,
        "key_weight": 0,
        "key_types": "int",
        "this_types": "list",
        "value_types": "decimal",
        "delegate_arguments": {
            "print_all": true,
            "print_flat": true,
            "field": "component"
        },
        "value_structure": null
    },
    "root": {
        "type": "Keymap",
        "delegate_arguments": {
            "sort": "sort_by_component_order"
        },
        "keys": {
            "vertex": {"types": "str", "structure": "string"},
            "fragment": {"types": "str", "structure": "string"},
            "blend": {"types": "dict", "structure": "blend"},
            "defines": {"types": "dict", "structure": "defines"},
            "samplers": {"types": "list", "structure": "samplers"},
            "uniforms": {"types": "list", "structure": "uniforms"}
        }
    },
    "blend": {
        "type": "Keymap",
        "delegate_arguments": {
            "sort": "sort_by_component_order"
        },
        "keys": {
            "func": {"types": "str", "delegate_arguments": {"always_print": true}},
            "srcrgb": {"types": "str"},
            "dstrgb": {"types": "str"}
        }
    },
    "defines": {
        "type": "Keymap",
        "delegate_arguments": {
            "sort": "sort_by_component_order"
        },
        "keys": {
            "values": {"types": "dict", "structure": "values"},
            "flags": {"types": "list", "structure": "flags"}
        }
    },
    "values": {
        "type": "Dict",
        "value_types": "str",
        "delegate_arguments": {
            "field": "value",
            "measure_length": true
        },
        "key_structure": "string",
        "value_structure": "string"
    },
    "flags": {
        "type": "Dict",
        "key_weight": 0,
        "key_types": "int",
        "this_types": "list",
        "value_types": "str",
        "delegate_arguments": {
            "field": "flag",
            "show_item_key": false,
            "measure_length": true
        },
        "value_structure": "string"
    },
    "samplers": {
        "type": "Dict",
        "key_weight": 0,
        "key_types": "int",
        "this_types": "list",
        "value_types": "dict",
        "delegate_arguments": {
            "field": "sampler",
            "show_item_key": false,
            "measure_length": true
        },
        "value_structure": "sampler"
    },
    "sampler": {
        "type": "Keymap",
        "delegate_arguments": {
            "sort": "sort_by_component_order"
        },
        "keys": {
            "name": {"types": "str", "structure": "string", "delegate_arguments": {"always_print": true}}
        }
    },
    "uniforms": {
        "type": "Dict",
        "key_weight": 0,
        "key_types": "int",
        "this_types": "list",
        "value_types": "dict",
        "delegate_arguments": {
            "field": "uniform",
            "show_item_key": false,
            "measure_length": true
        },
        "value_structure": "uniform"
    },
    "uniform": {
        "type": "Keymap",
        "delegate_arguments": {
            "sort": "sort_by_component_order"
        },
        "keys": {
            "name": {"types": "str", "structure": "string", "delegate_arguments": {"always_print": true}},
            "type": {"types": "str", "delegate_arguments": {"always_print": true}},
            "count": {"types": "int"},
            "values": {"types": "list", "structure": "vector_decimal"}
        }
    }
}