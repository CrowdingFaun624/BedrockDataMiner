{
    "group_aliases": {
        "misc": "structures/misc/misc",
        "temp": "structures/misc/template",
        "s": "serializers"
    },
    "base": {
        "inherit": "temp/base_multiple_pack_files",
        "$name": "font_providers",
        "$pack_type": "resource pack",
        "$structure": "#@root"
    },
    "root": {
        "inherit": "temp/SortedKeymap",
        "keys": {
            "providers": {"types": "list", "structure": "providers"}
        }
    },
    "providers": {
        "type": "Dict",
        "key_weight": 0,
        "key_types": "int",
        "this_types": "list",
        "value_types": "dict",
        "delegate_arguments": {
            "field": "provider",
            "show_item_key": false,
            "measure_length": true
        },
        "value_structure": "provider_switch"
    },
    "provider_switch": {
        "type": "Switch",
        "switch_function": {
            "function_name": "get_key",
            "arguments": {
                "key": "type"
            }
        },
        "substructures": {
            "bitmap": {"types": "dict", "structure": "provider.bitmap"},
            "legacy_unicode": {"types": "dict", "structure": "provider.legacy_unicode"},
            "reference": {"types": "dict", "structure": "provider.reference"},
            "space": {"types": "dict", "structure": "provider.space"},
            "ttf": {"types": "dict", "structure": "provider.ttf"},
            "unihex": {"types": "dict", "structure": "provider.unihex"}
        }
    },
    "provider_common": {
        "type": "Keymap",
        "keys": {
            "filter": {"types": "dict", "structure": "filter"}
        }
    },
    "filter": {
        "type": "Keymap",
        "delegate_arguments": {
            "sort": "sort_by_component_order",
            "field": "option"
        },
        "keys": {
            "uniform": {"types": "bool"},
            "jp": {"types": "bool"}
        }
    },
    "provider.bitmap": {
        "inherit": "temp/SortedKeymap",
        "imports": ["provider_common"],
        "keys": {
            "type": {"types": "str", "delegate_arguments": {"always_print": true}},
            "ascent": {"types": "int"},
            "chars": {"types": "list", "structure": "provider.bitmap:chars"},
            "file": {"types": "str", "structure": "misc/string", "delegate_arguments": {"always_print": true}},
            "height": {"types": "int"}
        }
    },
    "provider.bitmap:chars": {
        "type": "Dict",
        "allow_key_moves": false,
        "key_weight": 0,
        "key_types": "int",
        "this_types": "list",
        "value_types": "str",
        "delegate_arguments": {
            "field": "line",
            "measure_length": true
        },
        "value_structure": "misc/string"
    },
    "provider.legacy_unicode": {
        "inherit": "temp/SortedKeymap",
        "imports": ["provider_common"],
        "keys": {
            "type": {"types": "str", "delegate_arguments": {"always_print": true}},
            "template": {"types": "str", "structure": "misc/string", "delegate_arguments": {"always_print": true}},
            "sizes": {"types": "str", "structure": "misc/string"}
        }
    },
    "provider.reference": {
        "inherit": "temp/SortedKeymap",
        "imports": ["provider_common"],
        "keys": {
            "type": {"types": "str", "delegate_arguments": {"always_print": true}},
            "id": {"types": "str", "structure": "misc/string", "delegate_arguments": {"always_print": true}}
        }
    },
    "provider.space": {
        "inherit": "temp/SortedKeymap",
        "imports": ["provider_common"],
        "keys": {
            "type": {"types": "str", "delegate_arguments": {"always_print": true}},
            "advances": {"types": "dict", "structure": "provider.space:advances"}
        }
    },
    "provider.space:advances": {
        "type": "Dict",
        "value_types": "int",
        "delegate_arguments": {
            "sort": "sort_by_key",
            "field": "character",
            "measure_length": true
        },
        "value_structure": null
    },
    "provider.ttf": {
        "inherit": "temp/SortedKeymap",
        "imports": ["provider_common"],
        "keys": {
            "type": {"types": "str", "delegate_arguments": {"always_print": true}},
            "file": {"types": "str", "structure": "misc/string", "delegate_arguments": {"always_print": true}},
            "shift": {"types": "list", "structure": "provider.ttf:shift"},
            "scale": {"types": "int"},
            "oversample": {"types": "int"},
            "skip": {"types": ["list", "str"], "structure": "provider.ttf:skip"}
        }
    },
    "provider.ttf:shift": {
        "type": "Dict",
        "allow_key_moves": false,
        "key_weight": 0,
        "key_types": "int",
        "this_types": "list",
        "value_types": "int",
        "delegate_arguments": {
            "field": "shift",
            "print_flat": true
        },
        "value_structure": null
    },
    "provider.ttf:skip": {
        "type": "Normalizer",
        "outer_types": ["list", "str"],
        "inner_types": "list",
        "functions": {
            "function_name": "normalizers/normalizers font_providers_normalize_skip"
        },
        "structure": {
            "type": "Dict",
            "key_weight": 0,
            "key_types": "int",
            "this_types": "list",
            "value_types": "str",
            "delegate_arguments": {
                "field": "character",
                "show_item_key": false,
                "measure_length": true
            },
            "value_structure": null
        }
    },
    "provider.unihex": {
        "inherit": "temp/SortedKeymap",
        "imports": ["provider_common"],
        "keys": {
            "type": {"types": "str", "delegate_arguments": {"always_print": true}},
            "hex_file": {"types": "str", "structure": "misc/string", "delegate_arguments": {"always_print": true}},
            "size_overrides": {"types": "list", "structure": "provider.unihex:size_overrides"}
        }
    },
    "provider.unihex:size_overrides": {
        "type": "Dict",
        "key_weight": 0,
        "key_types": "int",
        "this_types": "list",
        "value_types": "dict",
        "delegate_arguments": {
            "field": "range",
            "show_item_key": false,
            "measure_length": true
        },
        "value_structure": "provider.unihex:size_overrides.item"
    },
    "provider.unihex:size_overrides.item": {
        "type": "Keymap",
        "delegate_arguments": {
            "sort": "sort_by_component_order",
            "print_all": true
        },
        "keys": {
            "from": {"types": "str"},
            "to": {"types": "str"},
            "left": {"types": "int"},
            "right": {"types": "int"}
        }
    }
}