{
    "group_aliases": {
        "misc": "structures/misc/misc",
        "temp": "structures/misc/template",
        "s": "serializers"
    },
    "base": {
        "inherit": "temp/base_multiple_pack_files",
        "$name": "equipment",
        "$pack_type": "resource pack",
        "$structure": "#@root"
    },
    "root": {
        "inherit": "temp/SortedKeymap",
        "keys": {
            "layers": {"types": "dict", "structure": "layers"}
        }
    },
    "layers": {
        "type": "Keymap",
        "delegate_arguments": {
            "sort": "sort_by_key",
            "field": "body type",
            "measure_length": true
        },
        "keys": {
            "camel_saddle": {"types": "list", "structure": "textures"},
            "donkey_saddle": {"types": "list", "structure": "textures"},
            "happy_ghast_body": {"types": "list", "structure": "textures"},
            "horse_body": {"types": "list", "structure": "textures"},
            "horse_saddle": {"types": "list", "structure": "textures"},
            "humanoid": {"types": "list", "structure": "textures"},
            "humanoid_leggings": {"types": "list", "structure": "textures"},
            "llama_body": {"types": "list", "structure": "textures"},
            "mule_saddle": {"types": "list", "structure": "textures"},
            "pig_saddle": {"types": "list", "structure": "textures"},
            "skeleton_horse_saddle": {"types": "list", "structure": "textures"},
            "strider_saddle": {"types": "list", "structure": "textures"},
            "wings": {"types": "list", "structure": "textures"},
            "wolf_body": {"types": "list", "structure": "textures"},
            "zombie_horse_saddle": {"types": "list", "structure": "textures"}
        }
    },
    "textures": {
        "type": "Dict",
        "key_weight": 0,
        "key_types": "int",
        "this_types": "list",
        "value_types": "dict",
        "delegate_arguments": {
            "field": "texture",
            "show_item_key": false,
            "measure_length": true
        },
        "value_structure": "texture"
    },
    "texture": {
        "inherit": "temp/SortedKeymap",
        "keys": {
            "texture": {"types": "str", "structure": "misc/string", "delegate_arguments": {"always_print": true}},
            "dyeable": {"types": "dict", "structure": "dyeable"},
            "use_player_texture": {"types": "bool"}
        }
    },
    "dyeable": {
        "inherit": "temp/SortedKeymap",
        "keys": {
            "color_when_undyed": {"types": "int"}
        }
    }
}