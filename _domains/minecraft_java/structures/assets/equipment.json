{
    "base": {
        "type": "Base",
        "types": "dict",
        "delegate_arguments": {
            "name": "equipment"
        },
        "subcomponent": "resource_packs"
    },
    "resource_packs": {
        "type": "Dict",
        "types": "dict",
        "delegate_arguments": {
            "field": "resource pack",
            "measure_length": true
        },
        "subcomponent": "files"
    },
    "files": {
        "type": "Dict",
        "types": "file",
        "delegate_arguments": {
            "field": "file",
            "measure_length": true
        },
        "subcomponent": "cache"
    },
    "cache": {
        "type": "Cache",
        "types": "file",
        "subcomponent": {
            "type": "File",
            "file_types": "file",
            "content_types": "dict",
            "subcomponent": "root"
        }
    },
    "string": {
        "type": "String"
    },
    "root": {
        "type": "Keymap",
        "sort": "by_component_order",
        "keys": {
            "layers": {"types": "dict", "subcomponent": "layers"}
        }
    },
    "layers": {
        "type": "Keymap",
        "sort": "by_key",
        "delegate_arguments": {
            "field": "body type",
            "measure_length": true
        },
        "keys": {
            "horse_body": {"types": "list", "subcomponent": "textures"},
            "humanoid": {"types": "list", "subcomponent": "textures"},
            "humanoid_leggings": {"types": "list", "subcomponent": "textures"},
            "llama_body": {"types": "list", "subcomponent": "textures"},
            "wings": {"types": "list", "subcomponent": "textures"},
            "wolf_body": {"types": "list", "subcomponent": "textures"}
        }
    },
    "textures": {
        "type": "Set",
        "types": "dict",
        "delegate_arguments": {
            "field": "texture",
            "measure_length": true
        },
        "subcomponent": "texture"
    },
    "texture": {
        "type": "Keymap",
        "sort": "by_component_order",
        "keys": {
            "texture": {"types": "str", "subcomponent": "string", "delegate_arguments": {"always_print": true}},
            "dyeable": {"types": "dict", "subcomponent": "dyeable"},
            "use_player_texture": {"types": "bool"}
        }
    },
    "dyeable": {
        "type": "Keymap",
        "sort": "by_component_order",
        "keys": {
            "color_when_undyed": {"types": "int"}
        }
    }
}