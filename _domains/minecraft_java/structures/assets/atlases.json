{
    "group_aliases": {
        "misc": "structures/misc/misc",
        "temp": "structures/misc/template",
        "s": "serializers"
    },
    "base": {
        "inherit": "temp/base_multiple_pack_files",
        "$name": "atlases",
        "$pack_type": "resource pack",
        "$structure": "#@root"
    },
    "root": {
        "inherit": "temp/SortedKeymap",
        "keys": {
            "sources": {"types": "list", "structure": "sources"}
        }
    },
    "sources": {
        "type": "Dict",
        "key_weight": 0,
        "key_types": "int",
        "this_types": "list",
        "value_types": "dict",
        "delegate_arguments": {
            "field": "source",
            "show_item_key": false,
            "measure_length": true
        },
        "value_structure": "source_switch"
    },
    "source_switch": {
        "type": "Switch",
        "switch_function": {
            "function_name": "get_key",
            "arguments": {
                "key": "type"
            }
        },
        "substructures": {
            "directory": {"types": "dict", "structure": "source.directory"},
            "minecraft:directory": {"types": "dict", "structure": "source.directory"},
            "paletted_permutations": {"types": "dict", "structure": "source.paletted_permutations"},
            "minecraft:paletted_permutations": {"types": "dict", "structure": "source.paletted_permutations"},
            "single": {"types": "dict", "structure": "source.single"},
            "minecraft:single": {"types": "dict", "structure": "source.single"}
        }
    },
    "texture_list": {
        "type": "Dict",
        "key_weight": 0,
        "key_types": "int",
        "this_types": "list",
        "value_types": "str",
        "delegate_arguments": {
            "field": "texture",
            "show_item_key": false,
            "measure_length": true
        },
        "value_structure": "misc/string"
    },
    "source.directory": {
        "inherit": "temp/SortedKeymap",
        "keys": {
            "type": {"types": "str", "delegate_arguments": {"always_print": true}},
            "source": {"types": "str", "structure": "misc/string", "delegate_arguments": {"always_print": true}},
            "prefix": {"types": "str", "structure": "misc/string"}
        }
    },
    "source.paletted_permutations": {
        "inherit": "temp/SortedKeymap",
        "keys": {
            "type": {"types": "str", "delegate_arguments": {"always_print": true}},
            "textures": {"types": "list", "structure": "texture_list"},
            "palette_key": {"types": "str", "structure": "misc/string"},
            "permutations": {"types": "dict", "structure": "source.paletted_permutations:permutations"}
        }
    },
    "source.paletted_permutations:permutations": {
        "type": "Dict",
        "value_types": "str",
        "delegate_arguments": {
            "field": "permutation",
            "measure_length": true
        },
        "key_structure": "misc/string",
        "value_structure": "misc/string"
    },
    "source.single": {
        "inherit": "temp/SortedKeymap",
        "keys": {
            "type": {"types": "str", "delegate_arguments": {"always_print": true}},
            "resource": {"types": "str", "structure": "misc/string", "delegate_arguments": {"always_print": true}}
        }
    }
}