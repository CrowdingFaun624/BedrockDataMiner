{
    "group_aliases": {
        "misc": "structures/misc/misc",
        "temp": "structures/misc/template",
        "s": "serializers",
        "t": "structure_tags"
    },
    "base": {
        "inherit": "temp/base_multiple_pack_files",
        "$name": "mob_variants",
        "$pack_type": "data pack",
        "$types": "dict",
        "$structure": "#!root{imports=imports}"
    },
    "root": {
        "inherit": "temp/SortedKeymap",
        "imports": "#imports"
    },
    "variant:spawn_conditions": {
        "type": "Keymap",
        "keys": {
            "spawn_conditions": {"types": "list", "structure": "spawn_conditions"},
            "biomes": {"types": ["str", "list"], "structure": "#!misc/named_list_or_item{name='biome'}", "tags": "t/outdated"}
        }
    },
    "variant:asset_id": {
        "type": "Keymap",
        "keys": {
            "asset_id": {"types": "str", "structure": "misc/string", "delegate_arguments": {"always_print": true}},
            "texture": {"types": "str", "structure": "misc/string", "tags": "t/outdated"}
        }
    },
    "variant:assets": {
        "type": "Keymap",
        "keys": {
            "assets": {"types": "dict", "structure": "wolf.assets"},
            "texture": {"types": "str", "tags": "t/outdated"},
            "angry_texture": {"types": "str", "tags": "t/outdated"},
            "tame_texture": {"types": "str", "tags": "t/outdated"},
            "wild_texture": {"types": "str", "tags": "t/outdated"}
        }
    },
    "variant:model": {
        "type": "Keymap",
        "keys": {
            "model": {"types": "str"}
        }
    },
    "wolf.assets": {
        "inherit": "temp/SortedKeymap",
        "keys": {
            "angry": {"types": "str"},
            "wild": {"types": "str"},
            "tame": {"types": "str"}
        }
    },
    "spawn_conditions": {
        "inherit": "temp/Set",
        "$field": "spawn condition",
        "value_types": "dict",
        "value_structure": "spawn_condition"
    },
    "spawn_condition": {
        "inherit": "temp/SortedKeymap",
        "keys": {
            "priority": {"types": "int"},
            "condition": {"types": "dict", "structure": "condition"}
        }
    },
    "condition": {
        "type": "Switch",
        "switch_function": {
            "function_name": "get_key",
            "arguments": {
                "key": "type"
            }
        },
        "substructures": {
            "minecraft:biome": {"types": "dict", "structure": "condition.biome"},
            "minecraft:structure": {"types": "dict", "structure": "condition.structure"},
            "minecraft:moon_brightness": {"types": "dict", "structure": "condition.moon_brightness"}
        }
    },
    "condition:all": {
        "type": "Keymap",
        "keys": {
            "type": {"types": "str", "delegate_arguments": {"always_print": true}}
        }
    },
    "condition.biome": {
        "inherit": "temp/SortedKeymap",
        "imports": "condition:all",
        "keys": {
            "biomes": {"types": ["str", "list"], "structure": "#!misc/named_list_or_item{name='biome'}"}
        }
    },
    "condition.structure": {
        "inherit": "temp/SortedKeymap",
        "imports": "condition:all",
        "keys": {
            "structures": {"types": ["str", "list"], "structure": "#!misc/named_list_or_item{name='structure'}"}
        }
    },
    "condition.moon_brightness": {
        "inherit": "temp/SortedKeymap",
        "imports": "condition:all",
        "keys": {
            "range": {"types": ["misc/decimal", "dict"], "structure": "misc/range_decimal_or_decimal"}
        }
    }
}