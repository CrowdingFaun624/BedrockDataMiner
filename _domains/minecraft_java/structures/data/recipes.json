        "tags": "t/outdated",
        "keys": {
            "item": {"types": "str", "delegate_arguments": {"always_print": true}},
            "tag": {"types": "str", "delegate_arguments": {"always_print": true}},
            "data": {"types": "int", "tags": "t/outdated", "delegate_arguments": {"always_print": true}}
        }
