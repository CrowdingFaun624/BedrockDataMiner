            "weight": {"types": "int"},
            "quality": {"types": "int"}
        }
    },
    "entry:composite": {
        "type": "Keymap",
        "keys": {
            "conditions": {"types": "list", "structure": "pred/predicate_list"},
            "children": {"types": "list", "structure": "entry_list"}
        }
    },
    "entry.alternatives": {
