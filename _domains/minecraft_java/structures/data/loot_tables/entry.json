            "minecraft:tag": {"types": "dict", "structure": "entry.tag"}
        }
    },
    "entry:all": {
        "type": "Keymap",
        "keys": {
            "type": {"types": "str", "delegate_arguments": {"always_print": true}}
        }
    },
    "entry:singleton": {
        "type": "Keymap",
        "keys": {
