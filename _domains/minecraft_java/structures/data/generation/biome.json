{
    "group_aliases": {
        "a": "structures/misc/types",
        "carver": "structures/data/generation/carver",
        "pf": "structures/data/generation/placed_feature",
        "misc": "structures/misc/misc",
        "particle": "structures/data/particles/particles",
        "temp": "structures/misc/template"
    },
    "base": {
        "inherit": "temp/base_multiple_pack_files",
        "$name": "biome",
        "$pack_type": "data pack",
        "$structure": "#@biome"
    },
    "biome": {
        "inherit": "temp/SortedKeymap",
        "keys": {
            "has_precipitation": {"types": "bool"},
            "temperature": {"types": "float"},
            "temperature_modifier": {"types": "str"},
            "downfall": {"types": "float"},
            "effects": {"types": "dict", "structure": "effects"},
            "carvers": {"types": ["str", "dict", "list"], "structure": "carver/carver_list_or_carver"},
            "features": {"types": "list", "structure": "features"},
            "creature_spawn_probability": {"types": "float"},
            "spawners": {"types": "dict", "structure": "spawners"},
            "spawn_costs": {"types": "dict", "structure": "spawn_costs"}
        }
    },
    "effects": {
        "inherit": "temp/SortedKeymap",
        "keys": {
            "fog_color": {"types": "int"},
            "sky_color": {"types": "int"},
            "water_color": {"types": "int"},
            "water_fog_color": {"types": "int"},
            "foliage_color": {"types": "int"},
            "dry_foliage_color": {"types": "int"},
            "grass_color": {"types": "int"},
            "grass_color_modifier": {"types": "str"},
            "particle": {"types": "dict", "structure": "effects.particle"},
            "ambient_sound": {"types": ["a/string", "a/compound"], "structure": "misc/sound_event"},
            "mood_sound": {"types": "dict", "structure": "effects.mood_sound"},
            "additions_sound": {"types": "dict", "structure": "effects.additions_sound"},
            "music": {"types": "list", "structure": "effects.musics"},
            "music_volume": {"types": "float"}
        }
    },
    "effects.additions_sound": {
        "inherit": "temp/SortedKeymap",
        "keys": {
            "sound": {"types": ["a/string", "a/compound"], "structure": "misc/sound_event"},
            "tick_chance": {"types": "float"} // double
        }
    },
    "effects.mood_sound": {
        "inherit": "temp/SortedKeymap",
        "keys": {
            "sound": {"types": ["a/string", "a/compound"], "structure": "misc/sound_event"},
            "tick_delay": {"types": "int"},
            "block_search_extent": {"types": "int"},
            "offset": {"types": "float"} // double
        }
    },
    "effects.musics": {
        "inherit": "temp/Set",
        "$field": "music",
        "value_types": "dict",
        "value_structure": "effects.music"
    },
    "effects.music": {
        "inherit": "temp/SortedKeymap",
        "keys": {
            "data": {"types": "dict", "structure": "effects.music.data"},
            "weight": {"types": "int"}
        }
    },
    "effects.music.data": {
        "inherit": "temp/SortedKeymap",
        "keys": {
            "sound": {"types": ["a/string", "a/compound"], "structure": "misc/sound_event"},
            "min_delay": {"types": "int"},
            "max_delay": {"types": "int"},
            "replace_current_music": {"types": "bool"}
        }
    },
    "effects.particle": {
        "inherit": "temp/SortedKeymap",
        "keys": {
            "probability": {"types": "float"},
            "options": {"types": "a/compound", "structure": "particle/particle"}
        }
    },
    "features": {
        "inherit": "temp/Set",
        "$field": "placed feature",
        "value_types": ["str", "dict", "list"],
        "value_structure": "pf/placed_feature_list_or_feature"
    },
    "spawners": {
        "inherit": "temp/SortedKeymap",
        "$keymap_field": "mob category",
        "keys": {
            "monster": {"types": "list", "structure": "spawners.category"},
            "creature": {"types": "list", "structure": "spawners.category"},
            "ambient": {"types": "list", "structure": "spawners.category"},
            "water_creature": {"types": "list", "structure": "spawners.category"},
            "underground_water_creature": {"types": "list", "structure": "spawners.category"},
            "water_ambient": {"types": "list", "structure": "spawners.category"},
            "misc": {"types": "list", "structure": "spawners.category"},
            "axolotls": {"types": "list", "structure": "spawners.category"}
        }
    },
    "spawners.category": {
        "inherit": "temp/Set",
        "$field": "mob",
        "value_types": "dict",
        "value_structure": "spawners.mob"
    },
    "spawners.mob": {
        "inherit": "temp/SortedKeymap",
        "keys": {
            "type": {"types": "str", "structure": "misc/string", "delegate_arguments": {"always_print": true}},
            "weight": {"types": "int"},
            "minCount": {"types": "int"},
            "maxCount": {"types": "int"}
        }
    },
    "spawn_costs": {
        "inherit": "temp/Dict",
        "$field": "entity type",
        "value_types": "dict",
        "value_structure": "spawn_costs.item"
    },
    "spawn_costs.item": {
        "inherit": "temp/SortedKeymap",
        "keys": {
            "energy_budget": {"types": "float"}, // double
            "charge": {"types": "float"} // double
        }
    }
}