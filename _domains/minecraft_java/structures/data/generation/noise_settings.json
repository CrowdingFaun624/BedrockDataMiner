{
    "group_aliases": {
        "a": "structures/misc/types",
        "bs": "structures/misc/block_state",
        "df": "structures/data/generation/density_function",
        "misc": "structures/misc/misc",
        "sr": "structures/data/generation/surface_rule",
        "temp": "structures/misc/template"
    },
    "base": {
        "inherit": "temp/base_multiple_pack_files",
        "$name": "noise_settings",
        "$pack_type": "data pack",
        "$structure": "#@noise_settings"
    },
    "noise_settings_or_id": {
        "type": "Union",
        "substructures": {
            "str": null,
            "dict": "noise_settings"
        }
    },
    "noise_settings": {
        "inherit": "temp/SortedKeymap",
        "keys": {
            "sea_level": {"types": "int"},
            "disable_mob_generation": {"types": "bool"},
            "ore_veins_enabled": {"types": "bool"},
            "aquifers_enabled": {"types": "bool"},
            "legacy_random_source": {"types": "bool"},
            "default_block": {"types": "a/compound", "structure": "bs/block_state"},
            "default_fluid": {"types": "a/compound", "structure": "bs/block_state"},
            "spawn_target": {"types": "list", "structure": "spawn_target"},
            "noise": {"types": "dict", "structure": "noise"},
            "noise_router": {"types": "dict", "structure": "noise_router"},
            "surface_rule": {"types": "dict", "structure": "sr/surface_rule"}
        }
    },
    "spawn_target": {
        "inherit": "temp/Set",
        "$field": "range",
        "value_types": "dict",
        "value_structure": "noise_parameter"
    },
    "noise_parameter": {
        "inherit": "temp/SortedKeymap",
        "keys": {
            "temperature": {"types": ["dict", "list", "float"], "structure": "spawn_target:range"},
            "humidity": {"types": ["dict", "list", "float"], "structure": "spawn_target:range"},
            "continentalness": {"types": ["dict", "list", "float"], "structure": "spawn_target:range"},
            "erosion": {"types": ["dict", "list", "float"], "structure": "spawn_target:range"},
            "weirdness": {"types": ["dict", "list", "float"], "structure": "spawn_target:range"},
            "depth": {"types": ["dict", "list", "float"], "structure": "spawn_target:range"},
            "offset": {"types": "float"}
        }
    },
    "spawn_target:range": {
        "type": "Union",
        "substructures": {
            "float": null,
            "dict": {
                "inherit": "temp/SortedKeymap",
                "keys": {
                    "min": {"types": "float"},
                    "max": {"types": "float"}
                }
            },
            "list": {
                "inherit": "temp/List",
                "$print_all": true,
                "$print_flat": true,
                "$field": "range item",
                "value_types": "float",
                "value_structure": null
            }
        }
    },
    "noise": {
        "inherit": "temp/SortedKeymap",
        "keys": {
            "min_y": {"types": "int"},
            "height": {"types": "int"},
            "size_horizontal": {"types": "int"},
            "size_vertical": {"types": "int"}
        }
    },
    "noise_router": {
        "inherit": "temp/SortedKeymap",
        "keys": {
            "initial_density_without_jaggedness": {"types": ["str", "df/density_function_type"], "structure": "df/density_function_or_id"},
            "final_density": {"types": ["str", "df/density_function_type"], "structure": "df/density_function_or_id"},
            "barrier": {"types": ["str", "df/density_function_type"], "structure": "df/density_function_or_id"},
            "fluid_level_floodedness": {"types": ["str", "df/density_function_type"], "structure": "df/density_function_or_id"},
            "fluid_level_spread": {"types": ["str", "df/density_function_type"], "structure": "df/density_function_or_id"},
            "lava": {"types": ["str", "df/density_function_type"], "structure": "df/density_function_or_id"},
            "vein_toggle": {"types": ["str", "df/density_function_type"], "structure": "df/density_function_or_id"},
            "vein_ridged": {"types": ["str", "df/density_function_type"], "structure": "df/density_function_or_id"},
            "vein_gap": {"types": ["str", "df/density_function_type"], "structure": "df/density_function_or_id"},
            "temperature": {"types": ["str", "df/density_function_type"], "structure": "df/density_function_or_id"},
            "vegetation": {"types": ["str", "df/density_function_type"], "structure": "df/density_function_or_id"},
            "continents": {"types": ["str", "df/density_function_type"], "structure": "df/density_function_or_id"},
            "erosion": {"types": ["str", "df/density_function_type"], "structure": "df/density_function_or_id"},
            "depth": {"types": ["str", "df/density_function_type"], "structure": "df/density_function_or_id"},
            "ridges": {"types": ["str", "df/density_function_type"], "structure": "df/density_function_or_id"}
        }
    }
}