{
    "group_aliases": {
        "a": "structures/misc/types",
        "bs": "structures/misc/block_state",
        "fp": "structures/misc/provider/float_provider",
        "hp": "structures/misc/provider/height_provider",
        "misc": "structures/misc/misc",
        "temp": "structures/misc/template"
    },
    "base": {
        "inherit": "temp/base_multiple_pack_files",
        "$name": "carver",
        "$pack_type": "data pack",
        "$structure": "#@carver"
    },
    "carver_list_or_carver": {
        "type": "Union",
        "substructures": {
            "str": null,
            "dict": "carver",
            "list": "carver_or_id_list"
        }
    },
    "carver_or_id": {
        "type": "Union",
        "substructures": {
            "str": null,
            "dict": "carver"
        }
    },
    "carver_or_id_list": {
        "inherit": "temp/Set",
        "$field": "carver",
        "value_types": ["str", "dict"],
        "value_structure": "carver_or_id"
    },
    "carver": {
        "type": "Switch",
        "switch_function": {
            "function_name": "get_key",
            "arguments": {
                "key": "type"
            }
        },
        "substructures": {
            "minecraft:cave": {"types": "dict", "structure": "#!carver:abstract{imports=[@config.cave]}"},
            "minecraft:nether_cave": {"types": "dict", "structure": "#!carver:abstract{imports=[@config.cave]}"},
            "minecraft:canyon": {"types": "dict", "structure": "#!carver:abstract{imports=[@config.canyon]}"}
        }
    },
    "carver:abstract": {
        "inherit": "temp/SortedKeymap",
        "keys": {
            "type": {"types": "str", "delegate_arguments": {"always_print": true}},
            "config": {"types": "dict", "structure": "#!config{imports=imports}"}
        }
    },
    "config": {
        "inherit": "temp/SortedKeymap",
        "imports": "#imports",
        "keys": {
            "probability": {"types": "float"},
            "y": {"types": "dict", "structure": "hp/height_provider"},
            "lava_level": {"types": "dict", "structure": "hp/vertical_anchor"},
            "replaceable": {"types": ["str", "list"], "structure": "#!misc/named_list_or_item{name='block'}"},
            "debug_settings": {"types": "dict", "structure": "debug_settings"}
        }
    },
    "debug_settings": {
        "inherit": "temp/SortedKeymap",
        "keys": {
            "debug_mode": {"types": "bool"},
            "air_state": {"types": "a/compound", "structure": "bs/block_state"},
            "water_state": {"types": "a/compound", "structure": "bs/block_state"},
            "lava_state": {"types": "a/compound", "structure": "bs/block_state"},
            "barrier_state": {"types": "a/compound", "structure": "bs/block_state"}
        }
    },
    "config.cave": {
        "type": "Keymap",
        "keys": {
            "yScale": {"types": ["float", "dict"], "structure": "fp/float_provider"},
            "horizontal_radius_multiplier": {"types": ["float", "dict"], "structure": "fp/float_provider"},
            "vertical_radius_multiplier": {"types": ["float", "dict"], "structure": "fp/float_provider"},
            "floor_level": {"types": ["float", "dict"], "structure": "fp/float_provider"}
        }
    },
    "config.canyon": {
        "type": "Keymap",
        "keys": {
            "yScale": {"types": ["float", "dict"], "structure": "fp/float_provider"},
            "vertical_rotation": {"types": ["float", "dict"], "structure": "fp/float_provider"},
            "shape": {"types": "dict", "structure": "config.canyon:shape"}
        }
    },
    "config.canyon:shape": {
        "inherit": "temp/SortedKeymap",
        "keys": {
            "distance_factor": {"types": ["float", "dict"], "structure": "fp/float_provider"},
            "thickness": {"types": ["float", "dict"], "structure": "fp/float_provider"},
            "horizontal_radius_factor": {"types": ["float", "dict"], "structure": "fp/float_provider"},
            "vertical_radius_default_factor": {"types": "float"},
            "vertical_radius_center_factor": {"types": "float"},
            "width_smoothness": {"types": "int"}
        }
    }
}