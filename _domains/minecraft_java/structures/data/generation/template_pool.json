{
    "group_aliases": {
        "misc": "structures/misc/misc",
        "pl": "structures/data/generation/processor_list",
        "t": "structure_tags",
        "temp": "structures/misc/template"
    },
    "base": {
        "inherit": "temp/base_multiple_pack_files",
        "$name": "template_pool",
        "$pack_type": "data pack",
        "$structure": "#@template_pool"
    },
    "template_pool_or_id": {
        "type": "Union",
        "substructures": {
            "str": null,
            "dict": "template_pool"
        }
    },
    "template_pool": {
        "inherit": "temp/SortedKeymap",
        "keys": {
            "name": {"types": "str", "delegate_arguments": {"always_print": true}, "tags": "t/outdated"},
            "fallback": {"types": "str"},
            "elements": {"types": "list", "structure": "weighted_element_list"}
        }
    },
    "element_list": {
        "inherit": "temp/Set",
        "$field": "element",
        "value_types": "dict",
        "value_structure": "element"
    },
    "weighted_element_list": {
        "inherit": "temp/Set",
        "$field": "element",
        "value_types": "dict",
        "value_structure": "weighted_element"
    },
    "weighted_element": {
        "inherit": "temp/SortedKeymap",
        "keys": {
            "weight": {"types": "int"},
            "element": {"types": "dict", "structure": "element", "delegate_arguments": {"always_print": true}}
        }
    },
    "element": {
        "type": "Switch",
        "switch_function": {
            "function_name": "get_key",
            "arguments": {
                "key": "element_type"
            }
        },
        "substructures": {
            "minecraft:empty_pool_element": {"types": "dict", "structure": "element.empty"},
            "minecraft:feature_pool_element": {"types": "dict", "structure": "element.feature"},
            "minecraft:legacy_single_pool_element": {"types": "dict", "structure": "element.legacy_single"},
            "minecraft:list_pool_element": {"types": "dict", "structure": "element.list"},
            "minecraft:single_pool_element": {"types": "dict", "structure": "element.single"}
        }
    },
    "element:all": {
        "type": "Keymap",
        "keys": {
            "element_type": {"types": "str", "delegate_arguments": {"always_print": true}},
            "projection": {"types": "str", "delegate_arguments": {"always_print": true}}
        }
    },
    "element.empty": {
        "inherit": "temp/SortedKeymap",
        "imports": "element:all"
    },
    "element.feature": {
        "inherit": "temp/SortedKeymap",
        "imports": "element:all",
        "keys": {
            "feature": {"types": "str", "delegate_arguments": {"always_print": true}}
        }
    },
    "element.legacy_single": {
        "inherit": "temp/SortedKeymap",
        "imports": "element:all",
        "keys": {
            "location": {"types": "str", "delegate_arguments": {"always_print": true}},
            "processors": {"types": ["str", "dict", "list"], "structure": "pl/processor_list_or_id"}
        }
    },
    "element.list": {
        "inherit": "temp/SortedKeymap",
        "imports": "element:all",
        "keys": {
            "elements": {"types": "list", "structure": "element_list"}
        }
    },
    "element.single": {
        "inherit": "temp/SortedKeymap",
        "imports": "element:all",
        "keys": {
            "location": {"types": "str", "delegate_arguments": {"always_print": true}},
            "processors": {"types": ["str", "dict", "list"], "structure": "pl/processor_list_or_id"},
            "override_liquid_settings": {"types": "str"}
        }
    }
}