{
    "group_aliases": {
        "misc": "structures/misc/misc",
        "temp": "structures/misc/template"
    },
    "base": {
        "inherit": "temp/base_multiple_pack_files",
        "$name": "flat_level_generator_preset",
        "$pack_type": "data pack",
        "$structure": "#@flat_level_generator_preset"
    },
    "flat_level_generator_preset": {
        "inherit": "temp/SortedKeymap",
        "keys": {
            "display": {"types": "str"},
            "settings": {"types": "dict", "structure": "settings"}
        }
    },
    "settings": {
        "inherit": "temp/SortedKeymap",
        "keys": {
            "layers": {"types": "list", "structure": "layers"},
            "biome": {"types": "str"},
            "lakes": {"types": "bool"},
            "features": {"types": "bool"},
            "structure_overrides": {"types": ["str", "list"], "structure": "#!misc/named_list_or_item{name='structure'}"}
        }
    },
    "layers": {
        "inherit": "temp/Sequence",
        "$field": "layer",
        "value_types": "dict",
        "value_structure": "layer"
    },
    "layer": {
        "inherit": "temp/SortedKeymap",
        "keys": {
            "height": {"types": "int"},
            "block": {"types": "str", "delegate_arguments": {"always_print": true}}
        }
    }
}