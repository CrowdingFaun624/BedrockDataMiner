        "ip": "structures/misc/provider/int_provider",
        "misc": "structures/misc/misc",
        "temp": "structures/misc/template"
    },
    "trunk_placer": {
        "type": "Switch",
        "switch_function": {
            "function_name": "get_key",
            "arguments": {
                "key": "type"
            }
        },
        "substructures": {
            "minecraft:straight_trunk_placer": {"types": "dict", "structure": "trunk_placer.empty"},
            "minecraft:forking_trunk_placer": {"types": "dict", "structure": "trunk_placer.empty"},
            "minecraft:giant_trunk_placer": {"types": "dict", "structure": "trunk_placer.empty"},
            "minecraft:mega_jungle_trunk_placer": {"types": "dict", "structure": "trunk_placer.empty"},
            "minecraft:dark_oak_trunk_placer": {"types": "dict", "structure": "trunk_placer.empty"},
            "minecraft:fancy_trunk_placer": {"types": "dict", "structure": "trunk_placer.empty"},
            "minecraft:bending_trunk_placer": {"types": "dict", "structure": "trunk_placer.bending_trunk_placer"},
