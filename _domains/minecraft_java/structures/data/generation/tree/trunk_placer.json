            "minecraft:cherry_trunk_placer": {"types": "dict", "structure": "trunk_placer.cherry_trunk_placer"}
        }
    },
    "trunk_placer:all": {
        "type": "Keymap",
        "keys": {
            "type": {"types": "str", "delegate_arguments": {"always_print": true}},
            "base_height": {"types": "int"},
            "height_rand_a": {"types": "int"},
            "height_rand_b": {"types": "int"}
        }
    },
    "trunk_placer.empty": {
