        "imports": "trunk_placer:all"
    },
    "trunk_placer.bending_trunk_placer": {
        "inherit": "temp/SortedKeymap",
        "imports": "trunk_placer:all",
        "keys": {
            "bend_length": {"types": ["int", "dict"], "structure": "ip/int_provider"},
            "min_height_for_leaves": {"types": "int"}
        }
    },
    "trunk_placer.upwards_branching_trunk_placer": {
        "inherit": "temp/SortedKeymap",
        "imports": "trunk_placer:all",
        "keys": {
            "extra_branch_steps": {"types": ["int", "dict"], "structure": "ip/int_provider"},
