{
    "group_aliases": {
        "te": "structures/data/gametest/test_environment",
        "temp": "structures/misc/template"
    },
    "base": {
        "inherit": "temp/base_multiple_pack_files",
        "$name": "test_instance",
        "$pack_type": "data pack",
        "$structure": "test_instance"
    },
    "test_instance": {
        "type": "Switch",
        "switch_function": {
            "function_name": "get_key",
            "arguments": {
                "key": "type"
            }
        },
        "substructures": {
            "minecraft:block_based": {"types": "dict", "structure": "test_instance.block_based"},
            "minecraft:function": {"types": "dict", "structure": "test_instance.function"}
        }
    },
    "test_instance:all": {
        "type": "Keymap",
        "keys": {
            "type": {"types": "str", "delegate_arguments": {"always_print": true}},
            "environment": {"types": ["str", "dict"], "structure": "te/test_environment_or_id"},
            "structure": {"types": "str"},
            "max_ticks": {"types": "int"},
            "setup_ticks": {"types": "int"},
            "required": {"types": "bool"},
            "rotation": {"types": "str"},
            "manual_only": {"types": "bool"},
            "sky_access": {"types": "bool"},
            "max_attempts": {"types": "int"},
            "required_successes": {"types": "int"}
        }
    },
    "test_instance.block_based": {
        "inherit": "temp/SortedKeymap",
        "imports": "test_instance:all"
    },
    "test_instance.function": {
        "inherit": "temp/SortedKeymap",
        "imports": "test_instance:all",
        "keys": {
            "function": {"types": "str", "delegate_arguments": {"always_print": true}}
        }
    }
}