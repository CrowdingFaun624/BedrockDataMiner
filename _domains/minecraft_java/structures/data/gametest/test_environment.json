{
    "group_aliases": {
        "misc": "structures/misc/misc",
        "t": "structure_tags",
        "temp": "structures/misc/template"
    },
    "base": {
        "inherit": "temp/base_multiple_pack_files",
        "$name": "test_environment",
        "$pack_type": "data pack",
        "$structure": "test_environment"
    },
    "test_environment_list": {
        "inherit": "temp/Set",
        "$field": "test_environment",
        "value_types": ["str", "dict"],
        "value_structure": "test_environment_or_id"
    },
    "test_environment_or_id": {
        "type": "Union",
        "substructures": {
            "str": null,
            "dict": "test_environment"
        }
    },
    "test_environment": {
        "type": "Switch",
        "switch_function": {
            "function_name": "get_key",
            "arguments": {
                "key": "type"
            }
        },
        "substructures": {
            "minecraft:all_of": {"types": "dict", "structure": "test_environment.all_of"},
            "minecraft:function": {"types": "dict", "structure": "test_environment.function"},
            "minecraft:game_rules": {"types": "dict", "structure": "test_environment.game_rules"},
            "minecraft:weather": {"types": "dict", "structure": "test_environment.weather"},
            "minecraft:raining": {"types": "dict", "structure": "test_environment.weather", "tags": "t/outdated"},
            "minecraft:time_of_day": {"types": "dict", "structure": "test_environment.time_of_day"}
        }
    },
    "test_environment:all": {
        "type": "Keymap",
        "keys": {
            "type": {"types": "str", "delegate_arguments": {"always_print": true}}
        }
    },
    "test_environment.all_of": {
        "inherit": "temp/SortedKeymap",
        "imports": "test_environment:all",
        "keys": {
            "definitions": {"types": "list", "structure": "test_environment_list"}
        }
    },
    "test_environment.function": {
        "inherit": "temp/SortedKeymap",
        "imports": "test_environment:all",
        "keys": {
            "setup": {"types": "str", "delegate_arguments": {"always_print": true}},
            "teardown": {"types": "str", "delegate_arguments": {"always_print": true}}
        }
    },
    "test_environment.game_rules": {
        "inherit": "temp/SortedKeymap",
        "imports": "test_environment:all",
        "keys": {
            "bool_rules": {"types": "list", "structure": "#!test_environment.game_rules:rules{value_types='bool'}"},
            "int_rules": {"types": "list", "structure": "#!test_environment.game_rules:rules{value_types='int'}"}
        }
    },
    "test_environment.game_rules:rules": {
        "inherit": "temp/Set",
        "$field": "game rule",
        "value_types": "dict",
        "value_structure": "#!test_environment.game_rules:rule{value_types=value_types}"
    },
    "test_environment.game_rules:rule": {
        "inherit": "temp/SortedKeymap",
        "keys": {
            "rule": {"types": "str", "structure": "misc/string", "delegate_arguments": {"always_print": true}},
            "value": {"types": "#value_types"}
        }
    },
    "test_environment.weather": {
        "inherit": "temp/SortedKeymap",
        "imports": "test_environment:all",
        "keys": {
            "weather": {"types": "str"}
        }
    },
    "test_environment.time_of_day": {
        "inherit": "temp/SortedKeymap",
        "imports": "test_environment:all",
        "keys": {
            "time": {"types": "int"}
        }
    }
}