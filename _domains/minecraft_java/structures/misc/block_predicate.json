{
    "group_aliases": {
        "a": "structures/misc/types",
        "misc": "structures/misc/misc",
        "temp": "structures/misc/template",
        "bs": "structures/misc/block_state",
        "vector": "structures/misc/vector"
    },
    "block_predicate_list": {
        "inherit": "temp/Set",
        "$field": "predicate",
        "value_types": "dict",
        "value_structure": "block_predicate"
    },
    "block_predicate": {
        "type": "Switch",
        "switch_function": {
            "function_name": "get_key",
            "arguments": {
                "key": "type"
            }
        },
        "substructures": {
            "minecraft:true": {"types": "dict", "structure": "block_predicate.predicate_list"},
            "minecraft:all_of": {"types": "dict", "structure": "block_predicate.predicate_list"},
            "minecraft:any_of": {"types": "dict", "structure": "block_predicate.predicate_list"},
            "minecraft:has_sturdy_face": {"types": "dict", "structure": "block_predicate.has_sturdy_face"},
            "minecraft:inside_world_bounds": {"types": "dict", "structure": "block_predicate.offset"},
            "minecraft:matching_block_tag": {"types": "dict", "structure": "block_predicate.matching_block_tag"},
            "minecraft:matching_blocks": {"types": "dict", "structure": "block_predicate.matching_blocks"},
            "minecraft:matching_fluids": {"types": "dict", "structure": "block_predicate.matching_fluids"},
            "minecraft:not": {"types": "dict", "structure": "block_predicate.not"},
            "minecraft:replaceable": {"types": "dict", "structure": "block_predicate.offset"},
            "minecraft:solid": {"types": "dict", "structure": "block_predicate.offset"},
            "minecraft:unobstructed": {"types": "dict", "structure": "block_predicate.unobstructed"},
            "minecraft:would_survive": {"types": "dict", "structure": "block_predicate.would_survive"}
        }
    },
    "block_predicate:all": {
        "type": "Keymap",
        "keys": {
            "type": {"types": "str", "delegate_arguments": {"always_print": true}}
        }
    },
    "block_predicate:offset": {
        "type": "Keymap",
        "keys": {
            "offset": {"types": "list", "structure": "vector/vector.int"}
        }
    },
    "block_predicate.has_sturdy_face": {
        "inherit": "temp/SortedKeymap",
        "imports": ["block_predicate:all", "block_predicate:offset"],
        "keys": {
            "direction": {"types": "str"}
        }
    },
    "block_predicate.matching_block_tag": {
        "inherit": "temp/SortedKeymap",
        "imports": ["block_predicate:all", "block_predicate:offset"],
        "keys": {
            "tag": {"types": "str"}
        }
    },
    "block_predicate.matching_blocks": {
        "inherit": "temp/SortedKeymap",
        "imports": ["block_predicate:all", "block_predicate:offset"],
        "keys": {
            "blocks": {"types": ["str", "list"], "structure": "#!misc/named_list_or_item{name='block'}"}
        }
    },
    "block_predicate.matching_fluids": {
        "inherit": "temp/SortedKeymap",
        "imports": ["block_predicate:all", "block_predicate:offset"],
        "keys": {
            "fluids": {"types": ["str", "list"], "structure": "#!misc/named_list_or_item{name='fluid'}"}
        }
    },
    "block_predicate.not": {
        "inherit": "temp/SortedKeymap",
        "imports": ["block_predicate:all"],
        "keys": {
            "predicate": {"types": "dict", "structure": "block_predicate"}
        }
    },
    "block_predicate.offset": {
        "inherit": "temp/SortedKeymap",
        "imports": ["block_predicate:all", "block_predicate:offset"]
    },
    "block_predicate.predicate_list": {
        "inherit": "temp/SortedKeymap",
        "imports": ["block_predicate:all"],
        "keys": {
            "predicates": {"types": "list", "structure": "block_predicate_list"}
        }
    },
    "block_predicate.unobstructed": {
        "inherit": "temp/SortedKeymap",
        "imports": ["block_predicate:all", "block_predicate:offset"]
    },
    "block_predicate.would_survive": {
        "inherit": "temp/SortedKeymap",
        "imports": ["block_predicate:all", "block_predicate:offset"],
        "keys": {
            "state": {"types": "a/compound", "structure": "bs/block_state"}
        }
    }
}