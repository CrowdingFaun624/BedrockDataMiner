{
    "base_multiple_pack_files": {
        // has variables "pack_type", "types", "serializer", and "structure"
        "type": "StructureBase",
        "abstract": true,
        "this_types": "dict",
        "$pack_type": "pack",
        "$types": "dict",
        "$serializer": "minecraft_common!serializers/json",
        "delegate_arguments": {
            "name": "#name"
        },
        "structure": {
            "type": "Dict",
            "value_types": "dict",
            "delegate_arguments": {
                "field": "#pack_type",
                "measure_length": true
            },
            "value_structure": {
                "type": "Dict",
                "value_types": "file",
                "delegate_arguments": {
                    "field": "file",
                    "measure_length": true
                },
                "value_structure": {
                    "type": "Cache",
                    "this_types": "file",
                    "structure": {
                        "type": "File",
                        "outer_types": "file",
                        "inner_types": "#types",
                        "serializer": "#serializer",
                        "structure": "#structure"
                    }
                }
            }
        }
    },
    "base_pack_files": {
        "type": "StructureBase",
        "this_types": "dict",
        "$pack_type": "pack",
        "$types": "dict",
        "$serializer": "minecraft_common!serializers/json",
        "delegate_arguments": {
            "name": "#name"
        },
        "structure": {
            "type": "Dict",
            "value_types": "file",
            "delegate_arguments": {
                "field": "#pack_type",
                "measure_length": true
            },
            "value_structure": {
                "type": "Cache",
                "this_types": "file",
                "structure": {
                    "type": "File",
                    "serializer": "#serializer",
                    "outer_types": "file",
                    "inner_types": "#types",
                    "structure": "#structure"
                }
            }
        }
    },
    "base_single_file": {
        "type": "StructureBase",
        "this_types": "file",
        "$types": "dict",
        "$serializer": "minecraft_common!serializers/json",
        "delegate_arguments": {
            "name": "#name"
        },
        "structure": {
            "type": "Cache",
            "this_types": "file",
            "structure": {
                "type": "File",
                "serializer": "#serializer",
                "outer_types": "file",
                "inner_types": "#types",
                "structure": "#structure"
            }
        }
    },
    "Dict": {
        "type": "Dict",
        "abstract": true,
        "$field": "field",
        "delegate_arguments": {
            "field": "#field",
            "measure_length": true,
            "show_item_key": true
        }
    },
    "Dict.nbt": {
        "inherit": "Dict",
        "abstract": true,
        "key_types": "a/string",
        "this_types": "a/compound"
    },
    "List": {
        "type": "Dict",
        "abstract": true,
        "$field": "item",
        "$print_all": false,
        "$print_flat": false,
        "this_types": "list",
        "key_types": "int",
        "delegate_arguments": {
            "field": "#field",
            "measure_length": true,
            "show_item_key": true,
            "print_all": "#print_all",
            "print_flat": "#print_flat"
        }
    },
    "List.nbt": {
        "inherit": "List",
        "abstract": true,
        "this_types": "a/list"
    },
    "Sequence": {
        "type": "Sequence",
        "abstract": true,
        "$field": "item",
        "delegate_arguments": {
            "field": "#field",
            "measure_length": true,
            "show_item_key": true
        }
    },
    "Sequence.nbt": {
        "inherit": "Sequence",
        "abstract": true,
        "this_types": "a/list"
    },
    "Set": {
        "type": "Dict",
        "abstract": true,
        "$field": "item",
        "this_types": "list",
        "key_types": "int",
        "key_weight": 0,
        "$show_item_key": false,
        "delegate_arguments": {
            "field": "#field",
            "measure_length": true,
            "show_item_key": "#show_item_key"
        }
    },
    "Set.nbt": {
        "inherit": "Set",
        "abstract": true,
        "this_types": "a/list"
    },
    "SortedKeymap": {
        "type": "Keymap",
        "abstract": true,
        "$keymap_field": "field",
        "$measure_length": false,
        "delegate_arguments": {
            "field": "#keymap_field",
            "sort": "sort_by_component_order",
            "measure_length": "#measure_length"
        }
    },
    "SortedKeymap.nbt": {
        "inherit": "SortedKeymap",
        "abstract": true,
        "key_types": "a/string",
        "this_types": "a/compound"
    }
}