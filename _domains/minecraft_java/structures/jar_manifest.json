{
    "group_aliases": {
        "s": "serializers",
        "misc": "structures/misc/misc",
        "temp": "structures/misc/template"
    },
    "base": {
        "inherit": "temp/base_single_file",
        "$name": "jar_manifest",
        "$serializer": "#@s/manifest_serializer",
        "$structure": "#@manifest"
    },
    "manifest": {
        "inherit": "temp/SortedKeymap",
        "keys": {
            // MOJANGCS.SF
            "Signature-Version": {"types": "str"},
            "Created-By": {"types": "str"},
            "Ant-Version": {"types": "str"},
            "Class-Path": {"types": "str"},
            "SHA1-Digest-Manifest": {"types": "str"},
            "SHA1-Digest-Manifest-Main-Attributes": {"types": "str"},
            "SHA-256-Digest-Manifest": {"types": "str"},
            "SHA-256-Digest-Manifest-Main-Attributes": {"types": "str"},
            "SHA-384-Digest-Manifest": {"types": "str"},
            "SHA-384-Digest-Manifest-Main-Attributes": {"types": "str"},
            // MANIFEST.MF
            "Manifest-Version": {"types": "str"},
            "Main-Class": {"types": "str"},
            // generic
            "files": {"types": "dict", "structure": "files"}
        }
    },
    "files": {
        "type": "Dict",
        "value_types": "dict",
        "delegate_arguments": {
            "measure_length": true,
            "sort": "sort_by_key",
            "field": "file"
        },
        "key_structure": "misc/string",
        "value_structure": "attributes"
    },
    "attributes": {
        "inherit": "temp/SortedKeymap",
        "keys": {
            "SHA1-Digest": {"types": "str"},
            "SHA-256-Digest": {"types": "str"},
            "SHA-384-Digest": {"types": "str"}
        }
    }
}