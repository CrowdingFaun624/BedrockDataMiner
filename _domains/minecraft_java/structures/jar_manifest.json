{
    "base": {
        "type": "Base",
        "types": "file",
        "delegate_arguments": {
            "name": "jar_manifest"
        },
        "subcomponent": "cache"
    },
    "cache": {
        "type": "Cache",
        "types": "file",
        "subcomponent": {
            "type": "File",
            "file_types": "file",
            "content_types": "dict",
            "subcomponent": "manifest"
        }
    },
    "manifest": {
        "type": "Keymap",
        "sort": "by_component_order",
        "keys": {
            // MOJANGCS.SF
            "Signature-Version": {"types": "str"},
            "Created-By": {"types": "str"},
            "SHA-384-Digest-Manifest": {"types": "str"},
            "SHA-384-Digest-Manifest-Main-Attributes": {"types": "str"},
            // MANIFEST.MF
            "Manifest-Version": {"types": "str"},
            "Main-Class": {"types": "str"},
            // generic
            "files": {"types": "dict", "subcomponent": "files"}
        }
    },
    "files": {
        "type": "Dict",
        "sort": "by_key",
        "types": "str",
        "delegate_arguments": {
            "measure_length": true,
            "field": "file"
        },
        "key_component": {"type": "String"},
        "subcomponent": null
    }
}