{
    "default_type": "AccessorType",
    "download_client": {
        "accessor_class": "ZipAccessor",
        "linked_accessors": {
            "file": "#!download{location='client.zip'}"
        }
    },
    "download_server": {
        "accessor_class": "ZipAccessor",
        "linked_accessors": {
            "file": "#!download{location='server.zip'}"
        }
    },
    "data_generator": {
        "accessor_class": "ZipAccessor",
        "linked_accessors": {
            "file": {
                "accessor_class": "minecraft_common!ArchivalAccessor",
                "arguments": {
                    "archive_location": "G:/Archive/mcje",
                    "file_name": "data_generator.zip"
                },
                "linked_accessors": {
                    "source": {
                        "accessor_class": "DataGeneratorAccessor",
                        "arguments": {
                            "java_location": "C:/Program Files/Java/jdk-21/bin/java.exe"
                        }
                    }
                }
            }
        }
    },
    "download_json": {
        "inherit": "download",
        "$location": "version.json"
    },
    "experimental_zip": {
        "accessor_class": "SingleDirectoryFileAccessor",
        "linked_accessors": {
            "directory": {
                "accessor_class": "ZipAccessor",
                "linked_accessors": {
                    "file": "#!download{location='version.zip'}"
                }
            }
        }
    },
    "download_experimental_data_pack": {
        "accessor_class": "ZipAccessor",
        "linked_accessors": {
            "file": "#!download{location='experimental_data_pack.zip'}"
        }
    },
    "download": {
        "accessor_class": "minecraft_common!ArchivalAccessor",
        "arguments": {
            "archive_location": "G:/Archive/mcje/",
            "file_name": "#location"
        },
        "linked_accessors": {
            "source": {
                "accessor_class": "DownloadAccessor",
                "arguments": {
                    "log": "minecraft_java!logs/download_log",
                    "location": "#location"
                }
            }
        }
    }
}